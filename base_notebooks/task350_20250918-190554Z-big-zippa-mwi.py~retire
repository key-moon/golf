def p(j,A=range):
	E=[A[:]for A in j]
	for I in A(1,10):
		C=[(B,C)for B in A(len(j))for C in A(len(j[0]))if j[B][C]==I]
		for H in A(len(C)):
			for J in A(H+1,len(C)):
				B,F=C[H];D,G=C[J]
				if B==D:
					for K in A(min(F,G),max(F,G)+1):E[B][K]=8
				elif F==G:
					for L in A(min(B,D),max(B,D)+1):E[L][F]=8
		for(B,D)in C:E[B][D]=1
	return E