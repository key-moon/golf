S=range
d=len
def p(a):
 C,l=d(a),d(a[0])
 def E(r,a):
  A=[0]*l
  for(E,C)in enumerate(r):
   T=E+a
   if C and 0<=T<l:A[T]=C
  return A
 for A in S(1,C+1):
  h=a[:A]
  for G in S(-l,l+1):
   H=True
   for T in S(C):
    if E(h[T%A],T//A*G)!=a[T]:H=False;break
   if H:return[E(h[l%A],l//A*G)for l in S(10)]
 return[a[A%C][:]for A in S(10)]