from collections import*
def p(f,r=range):
 i=[[8if n==8else 0for n in e]for e in f]
 for d in r(len(f)-1):
  for x in r(len(f[0])-1):
   e=[f[d][x:x+2],f[d+1][x:x+2]];n=[t for e in e for t in e];n=Counter(n).most_common(1)
   if n[0][1]==3and n[0][0]!=0:
    for u in r(d,d+2):
     for e in r(x,x+2):
      if i[u][e]==0:i[u][e]=1
 return i
