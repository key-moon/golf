def p(g,L=len,R=range):
	C,D=L(g),L(g[0]);A=[A for A in R(C)if L(set(g[A]))>1][0];B=[B for B in R(D)if g[A][B]>0][0];I=[[-2,-2,g[A+1][B+1]],[2,-2,g[A][B+1]],[-2,2,g[A+1][B]],[2,2,g[A][B]]]
	for E in R(A,A+2):
		for F in R(B,B+2):
			for(G,H,J)in I:
				if 0<=G+E<C and 0<=H+F<D:g[G+E][H+F]=J
	return g