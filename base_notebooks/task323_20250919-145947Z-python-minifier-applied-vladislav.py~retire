def p(j):
	D,E=len(j),len(j[0]);C=[A[:]for A in j];F,G=next((A,B)for A in range(D)for B in range(E)if j[A][B])
	for(H,I)in((-1,1),(1,-1)):
		A,B=F,G
		while 1:
			for J in[0]*2:
				A+=H
				if 0<=A<D:C[A][B]=5
				else:break
			else:
				for J in[0]*2:
					B+=I
					if 0<=B<E:C[A][B]=5
					else:break
				else:continue
			break
	return C