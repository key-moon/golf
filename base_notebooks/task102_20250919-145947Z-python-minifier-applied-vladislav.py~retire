def solve_44d8ac46(I):
	C=tuple(tuple(A)for A in I);H,J=len(C),len(C[0]);from collections import Counter as T,deque;N=T(B for A in C for B in A);U=max(N,key=N.get);K=[[False]*J for A in range(H)];O=[]
	for A in range(H):
		for B in range(J):
			if K[A][B]or C[A][B]==U:continue
			V=C[A][B];L=deque([(A,B)]);K[A][B]=True;P={(A,B)}
			while L:
				W,X=L.popleft()
				for(Y,Z)in((1,0),(-1,0),(0,1),(0,-1)):
					E,F=W+Y,X+Z
					if 0<=E<H and 0<=F<J and not K[E][F]and C[E][F]==V:K[E][F]=True;L.append((E,F));P.add((E,F))
			O.append(P)
	M=set()
	for G in O:
		if not G:continue
		a=min(A for(A,B)in G);b=min(A for(B,A)in G);c=max(A for(A,B)in G);d=max(A for(B,A)in G);D={(A,B)for A in range(a,c+1)for B in range(b,d+1)if(A,B)not in G}
		if not D:continue
		e=min(A for(A,B)in D);f=min(A for(B,A)in D);g=max(A for(A,B)in D);h=max(A for(B,A)in D);Q=g-e+1;R=h-f+1
		if Q==R and len(D)==Q*R:M|=D
	if not M:return C
	S=[list(A)for A in C]
	for(A,B)in M:
		if 0<=A<H and 0<=B<J:S[A][B]=2
	return tuple(tuple(A)for A in S)
def p(g):A=solve_44d8ac46(g);return[list(A)for A in A]