def p(g,L=len,R=range):
	F,G=L(g),L(g[0]);B,D=0,[]
	for A in R(L(g)):
		C=[g[A][0]]
		for E in R(L(g[A])-1):
			if g[A][E+1]!=g[A][E]:C+=[g[A][E+1]]
		if L(C)>B:B=L(C);D=C[:]
	g=[D[:]for A in R(L(D))]
	for A in R(L(g)//2):
		for B in R(A,L(g[0])-A-1):g[A][B]=g[A][A];g[-(A+1)][B]=g[-(A+1)][A]
	return g