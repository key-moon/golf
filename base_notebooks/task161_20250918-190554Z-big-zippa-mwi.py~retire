def p(g,E=enumerate,R=range,L=len):
	F,G=L(g),L(g[0]);D={A:{0:[],1:[]}for A in set(sum(g,[]))}
	for(B,H)in E(g):
		for(C,A)in E(H):D[A][0]+=[B];D[A][1]+=[C]
	del D[0];A=sorted([[len(D[A][1])-max(D[A][0])/100,A]for A in D])[0][1];g=[[B if B==A else 0 for B in B]for B in g]
	for B in R(F):
		if g[B][0]==A or g[B][-1]==A:
			for C in R(G):g[B][C]=A
	for C in R(G):
		if g[0][C]==A or g[-1][C]==A:
			for B in R(F):g[B][C]=A
	return g