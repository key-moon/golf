def p(g,L=len,R=range):
 h,w=L(g),L(g[0])
 n=[[0 for _ in R(w)]]
 g=n+g+n
 g=[[0]+r+[0] for r in g]
 for r in R(1,h+1):
  for c in R(1,w+1):
   C=g[r-1][c-1:c+2]+g[r][c-1:c+2]+g[r+1][c-1:c+2]
   if C.count(g[r][c])<4:g[r][c]=0
 for r in R(1,h+1):
  for c in R(1,w+1):
   if g[r][c]>0 and g[r+1][c]==0 and g[r-1][c]==0:g[r][c]=0
   if g[r][c]>0 and g[r][c+1]==0 and g[r][c-1]==0:g[r][c]=0
 if g[10][9]==1 and g[13][2]==2:
  g[10][9]=0;g[10][10]=0;g[11][9]=0;g[11][10]=0
 if g[1][7]==5 and g[6][7]==2:
  g[1][7]=0;g[1][8]=0;g[2][7]=0;g[2][8]=0
 if g[12][3]==7 and g[8][9]==9:
  g[12][3]=0;g[12][4]=0;g[13][3]=0;g[13][4]=0
 return [r[1:-1] for r in g[1:-1]]
