R=range
L=len
def p(g):
	for H in R(4):
		g=list(map(list,zip(*g[::-1])));G,F,C=L(g),L(g[0]),0
		for A in R(G):
			if len(set(g[A]))>2 and 8 not in g[A]:
				D=E=0
				if C>0:
					for B in R(F):
						if g[A][B]>0 and E==0 and not D:D=1;E=g[A][B]
						if g[A][B]>0 and g[A][B]!=E:D=0
						if D==1 and g[A][B]==0:g[A][B]=8
				C+=1
			elif C>0:
				for B in R(F):
					if g[A-1][B]==8:g[A-1][B]=0
				C=0
	return g