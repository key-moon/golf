def p(t,n=len,R=range,h=max,N=min):
 n,E,r,D=n(t),n(t[0]),[],[];O=[n for n in set(sum(t,[]))if n not in[0,5]][0]
 for n in R(n):
  for o in R(E):
   if t[n][o]==5:r+=[n];D+=[o]
 for n in R(n):
  for o in R(E):
   if n in[N(r)+1,h(r)-1]and N(D)+1<=o<=h(D)-1:t[n][o]=O
   if o in[N(D)+1,h(D)-1]and N(r)+1<=n<=h(r)-1:t[n][o]=O
 return t