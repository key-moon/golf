def p(e):
 D=range;d=len(e);n=[d[:]for d in e]
 for l in range(d):
  if e[1][l]==0and e[d-2][l]==0and sum(e[d][l]for d in D(1,d-1))==0:
   for C in D(1,d-1):n[C][l]=3
 for C in range(d):
  if e[C][1]==0and e[C][d-2]==0and sum(e[C][d]for d in D(1,d-1))==0:
   for l in D(1,d-1):
    if n[C][l]==0:n[C][l]=3
 return n