def p(g):
	O=None;N=False;P,Q=len(g),len(g[0]);from collections import Counter as T;R=T(B for A in g for B in A);A=min(R,key=R.get);F=[(B,C)for B in range(P)for C in range(Q)if g[B][C]==A]
	if not F:return[A[:]for A in g]
	U=min(A for(A,B)in F);V=min(A for(B,A)in F);W=max(A for(A,B)in F);X=max(A for(B,A)in F);D=W-U+1;E=X-V+1;K=max(1,D-2);L=max(1,E-2)
	def S(i,j):
		for B in range(j,j+E):
			if g[i][B]!=A or g[i+D-1][B]!=A:return N
		for C in range(i,i+D):
			if g[C][j]!=A or g[C][j+E-1]!=A:return N
		return True
	G=O
	for B in range(P-(K+2)+1):
		for C in range(Q-(L+2)+1):
			M=True
			for I in range(B+1,B+K+1):
				Y=g[I]
				for J in range(C+1,C+L+1):
					if Y[J]!=0:M=N;break
				if not M:break
			if M:
				if G is O or S(*G)and not S(B,C):G=B,C
	if G is O:return[A[:]for A in g]
	B,C=G;D=K+2;E=L+2;H=[A[:]for A in g]
	for J in range(C,C+E):H[B][J]=A;H[B+D-1][J]=A
	for I in range(B,B+D):H[I][C]=A;H[I][C+E-1]=A
	return H