def p(g):
	I=[A[:]for A in g];F,G=len(g),len(g[0]);O={(A,B)for A in range(F)for B in range(G)if g[A][B]==3};J=set()
	for A in[3,2,1]:
		for D in range(F):
			for E in range(G):
				for P in[0,1]:
					K,L,M,N=(E+A,E,E-A,E+2*A)if P else(E,E+A,E+2*A,E-A);H=set()
					for B in range(A):
						for C in range(A):
							if 0<=D+B<F and 0<=K+C<G:H.add((D+B,K+C))
							if 0<=D+B+A<F and 0<=L+C<G:H.add((D+B+A,L+C))
					if len(H)==2*A*A and H<=O and not H&J:
						J|=H
						for B in range(A):
							for C in range(A):
								if 0<=D+B-A<F and 0<=M+C<G:I[D+B-A][M+C]=8
								if 0<=D+B+2*A<F and 0<=N+C<G:I[D+B+2*A][N+C]=8
	return I