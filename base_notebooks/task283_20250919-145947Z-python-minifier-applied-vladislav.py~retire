def f(j,p,A,c,E,k):
	for B in range(A,E+1):
		for C in range(p,c+1):j[B][C]=k
def z(j,p,A,c,E):f(j,p,A,c,E,4);f(j,p+1,A+1,c-1,E-1,2);j[A][p]=j[A][c]=j[E][p]=j[E][c]=1
def p(j):
	F,C=len(j),len(j[0])
	for G in range(F*C):
		D,E=G%C,G//C
		if j[E][D]==5:
			A,B=D,E
			while A<C-1 and j[B][A+1]==5:A+=1
			while B<F-1 and j[B+1][A]==5:B+=1
			z(j,D,E,A,B)
	return j