def p(g):
	g=[[0 if A==max(g[0])else A for A in A]for A in g];g=[A for A in g if max(A)>0];g=[list(A)for A in zip(*g)];g=[A[::-1]for A in g];g=[A for A in g if max(A)>0];g=[A[::-1]for A in g];g=[list(A)for A in zip(*g)];A=[A[:]for A in g];g=[[A[0][0],0,A[0][3]],[0,0,0],[A[3][0],0,A[3][3]]]
	if A[0].count(0)==1:g[0][1]=max(A[0])
	if A[3].count(0)==1:g[2][1]=max(A[3])
	if g[2][0]==g[2][2]:g[2][1]=g[2][0]
	if A[0][0]==A[0][2]and A[0][0]>0:g[0][1]=g[0][0]
	if A[0][0]==A[2][0]and A[0][0]>0:g[1][0]=g[0][0]
	if A[0][3]==A[2][3]and A[0][3]>0:g[1][2]=g[0][2]
	if A[3][0]==A[1][0]and A[3][0]>0:g[1][0]=g[2][0]
	if A[3][3]==A[1][3]and A[3][3]>0:g[1][2]=g[2][2]
	if A[0][1]==A[0][2]and A[0][0]==0 and A[0][3]==0:g[0][1]=A[0][1]
	if A[1][0]==A[2][0]and A[0][0]==0 and A[3][0]==0:g[1][0]=A[1][0]
	if A[3][1]==A[3][2]and A[3][0]==0 and A[3][3]==0:g[2][1]=A[3][1]
	if A[1][3]==A[2][3]and A[0][3]==0 and A[3][3]==0:g[1][2]=A[2][3]
	return g