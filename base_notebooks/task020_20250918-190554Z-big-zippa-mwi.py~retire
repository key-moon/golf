def p(g,H=enumerate):
	C=D=9**9;E=F=-1
	for(A,O)in H(g):
		for(B,G)in H(O):
			if G:C=min(C,A);E=max(E,A);D=min(D,B);F=max(F,B)
	P=C+E;Q=D+F
	for A in range(C,E+1):
		for B in range(D,F+1):
			I=P-A;J=Q-B;K=C+B-D;G=D+A-C;L=C+F-B;M=D+E-A;N=(A,B),(A,J),(I,B),(I,J),(K,G),(L,G),(K,M),(L,M);R=max(g[A][B]for(A,B)in N)
			for(S,T)in N:g[S][T]=R
	return g