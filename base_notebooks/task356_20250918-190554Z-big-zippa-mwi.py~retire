def p(j,A=range):
	E=[A[:]for A in j]
	for F in A(1,10):
		B=[(B,C)for B in A(len(j))for C in A(len(j[0]))if j[B][C]==F]
		for I in A(len(B)):
			for J in A(I+1,len(B)):
				C,D=B[I];G,H=B[J]
				if C==G:
					for K in A(min(D,H),max(D,H)+1):E[C][K]=F
				elif D==H:
					for L in A(min(C,G),max(C,G)+1):E[L][D]=F
	return E