def p(g,L=len,R=range):
	F,G,C,D=L(g),L(g[0]),[],[];E=1
	for A in R(F//2+1):
		for B in R(G):
			if g[A][B]==E:g[A][B]=g[-(A+1)][B];C+=[A];D+=[B]
			if g[-(A+1)][B]==E:g[-(A+1)][B]=g[A][B];C+=[F-(A+1)];D+=[B]
	for A in R(F):
		for B in R(G//2+1):
			if g[A][B]==E:g[A][B]=g[A][-(B+1)];C+=[A];D+=[B]
			if g[A][-(B+1)]==E:g[A][-(B+1)]=g[A][B];C+=[A];D+=[G-(B+1)]
	g=g[min(C):max(C)+1];g=[A[min(D):max(D)+1]for A in g];return g