L=len
R=range
def p(g):
	F=[0]
	for I in range(8):
		g=list(map(list,zip(*g[::-1])));G,H=L(g),L(g[0])
		for A in R(G):
			if len(set(g[A]))<2 and g[A][0]>0:
				C=g[A][0];F+=[C]
				for D in R(0,A-1):
					E=0
					for B in R(H):
						if g[D][B]==C:g[D][B]=0;E-=1;g[A+E][B]=C
				for D in R(A+1,G):
					E=0
					for B in R(H):
						if g[D][B]==C:g[D][B]=0;E+=1;g[A+E][B]=C
	g=[[A if A in F else 0 for A in A]for A in g];return g