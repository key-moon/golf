def p(g):
	M=[A[:]for A in g];a,b=len(M),len(M[0]);A=[]
	for W in M:
		N=[]
		for X in W:N+=[X]*2
		A.extend([N[:],N[:]])
	J,K=len(A),len(A[0]);L=[[False]*K for A in range(J)];D=[]
	for B in range(J):
		for C in range(K):
			if A[B][C]!=2 or L[B][C]:continue
			O=[(B,C)];L[B][C]=True;P=[]
			while O:
				E,Q=O.pop();P.append((E,Q))
				for R in(-1,0,1):
					for S in(-1,0,1):
						if R==S==0:continue
						F,G=E+R,Q+S
						if 0<=F<J and 0<=G<K and not L[F][G]and A[F][G]==2:L[F][G]=True;O.append((F,G))
			D.append(P)
	Y={(B,C)for B in range(J)for C in range(K)if A[B][C]==2}
	def T(si,sj,ei,ej):
		for B in range(si,ei+1):
			for C in range(sj,ej+1):A[B][C]=4
	for E in D:H=[A for(A,B)in E];I=[A for(B,A)in E];T(min(H),min(I),max(H),max(I))
	def Z(A,B):
		C=10**9
		for(E,F)in A:
			for(G,H)in B:
				D=abs(E-G)+abs(F-H)
				if D<C:C=D
				if C==0:return 0
		return C
	U=len(D)
	for B in range(U):
		for C in range(U):
			if Z(D[B],D[C])<5:V=D[B]+D[C];H=[A for(A,B)in V];I=[A for(B,A)in V];T(min(H),min(I),max(H),max(I))
	for(B,C)in Y:A[B][C]=2
	return[A[::2]for A in A[::2]]