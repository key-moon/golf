def p(j):
	J,E=len(j),len(j[0]);F=[[0]*E for A in j];K=[]
	for G in range(J):
		for H in range(E):
			if j[G][H]==8 and not F[G][H]:
				L=[(G,H)];F[G][H]=1;N=[]
				while L:
					A,B=L.pop();N.append((A,B))
					for O in(-1,0,1):
						for P in(-1,0,1):
							if O==P==0:continue
							C,D=A+O,B+P
							if 0<=C<J and 0<=D<E and not F[C][D]and j[C][D]==8:F[C][D]=1;L.append((C,D))
				K.append(N)
	if not K:return[A[:]for A in j]
	def R(c):A=min(A for(A,B)in c);B=min(A for(B,A)in c);return tuple(sorted((C-A,D-B)for(C,D)in c))
	M={}
	for Q in K:M.setdefault(R(Q),[]).append(Q)
	S=min(len(A)for A in M.values());T=[B for(C,A)in M.items()if len(A)==S for B in A]
	def U(c):return min(A for(A,B)in c),min(A for(B,A)in c)
	V=min(T,key=U);I=[A[:]for A in j]
	for A in range(J):
		for B in range(E):
			if I[A][B]==8:I[A][B]=1
	for(A,B)in V:I[A][B]=2
	return I