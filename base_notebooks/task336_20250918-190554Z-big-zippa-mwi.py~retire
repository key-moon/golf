def p(j,A=len,c=enumerate,E=min,k=max,W=range):
	L,M=A(j),A(j[0]);K=[(A,C)for(A,B)in c(j)for(C,D)in c(B)if D==5];H=E(A for(A,B)in K);I=k(A for(A,B)in K);F=E(A for(B,A)in K);G=k(A for(B,A)in K)
	for B in range(H+1,I):j[B][F+1:G]=[8]*(G-F-1)
	D=None;J=0,0
	for C in W(F,G+1):
		if j[H][C]==0:D=H,C;J=-1,0;break
	if not D:
		for C in range(F,G+1):
			if j[I][C]==0:D=I,C;J=1,0;break
	if not D:
		for B in range(H,I+1):
			if j[B][F]==0:D=B,F;J=0,-1;break
	if not D:
		for B in range(H,I+1):
			if j[B][G]==0:D=B,G;J=0,1;break
	B,C=D;N,O=J
	while 0<=B<L and 0<=C<M and j[B][C]==0:j[B][C]=8;B+=N;C+=O
	return j