j=len
A=range
def p(c):
	E,F=[],[];G,H=j(c),j(c[0])
	for B in A(G-4):
		for C in A(H-4):
			D=[[c[A+B][D+C]for A in A(5)]for D in A(5)];D=[B for A in D for B in A];D=sum([A for A in D if A==1])
			if D==16:E.append(B+2);F.append(C+2)
	for B in A(G):
		for C in A(H):
			if B in E or C in F:
				if c[B][C]!=1:c[B][C]=6
	return c