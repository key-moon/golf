ZERO=0
EIGHT=8
def solve_b782dc8a(I):
	L=True;H,J=len(I),len(I[0]);E=[list(A)for A in I];from collections import deque;M={B for A in I for B in A};Q=EIGHT if EIGHT in M else _most_color(I);N=sorted(A for A in M if A not in(Q,ZERO))
	if len(N)!=2:return I
	K,O=N;F=deque();G=[[False]*J for A in range(H)]
	for C in range(H):
		for D in range(J):
			if E[C][D]in(K,O):F.append((C,D,E[C][D]));G[C][D]=L
	if not F:return I
	while F:
		C,D,R=F.popleft()
		for(S,T)in((1,0),(-1,0),(0,1),(0,-1)):
			A,B=C+S,D+T
			if 0<=A<H and 0<=B<J and not G[A][B]:
				if E[A][B]==ZERO:P=O if R==K else K;E[A][B]=P;G[A][B]=L;F.append((A,B,P))
				else:G[A][B]=L
	return tuple(tuple(A)for A in E)
def p(g):A=tuple(tuple(A)for A in g);B=solve_b782dc8a(A);return[list(A)for A in B]