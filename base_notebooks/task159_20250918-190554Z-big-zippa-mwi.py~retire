R=range
L=len
def p(g):
	H,I=L(g),L(g[0]);A,C=[],[99,99,0,0];G=[A for A in sum(g,[])if A not in[0,2]][0]
	for B in R(H):
		E=g[B].count(2)
		if E>1 and len(A)<1:A=[B,g[B].index(2),E]
		E=g[B].count(G)
		if E>0:
			C[0]=min([B,C[0]])
			for D in R(I):
				if g[B][D]==G:C[1]=min([D,C[1]])
	F=A[2]//3
	for B in R(3):
		for D in R(3):
			for J in R(F):
				for K in R(F):g[A[0]+B*F+J+1][A[1]+D*F+K+1]=g[C[0]+B][C[1]+D]
	g=g[A[0]:A[0]+A[2]];g=[B[A[1]:A[1]+A[2]]for B in g];return g