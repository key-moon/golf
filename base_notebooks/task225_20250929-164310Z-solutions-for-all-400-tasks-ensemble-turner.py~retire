def p(g,L=len,T=range):
 r,i=L(g),L(g[0]);n=[n for n in T(r)if L(set(g[n]))>1][0];B=[B for B in T(i)if g[n][B]>0][0];I=[[-2,-2,g[n+1][B+1]],[2,-2,g[n][B+1]],[-2,2,g[n+1][B]],[2,2,g[n][B]]]
 for H in T(n,n+2):
  for F in T(B,B+2):
   for(G,O,J)in I:
    if 0<=G+H<r and 0<=O+F<i:g[G+H][O+F]=J
 return g