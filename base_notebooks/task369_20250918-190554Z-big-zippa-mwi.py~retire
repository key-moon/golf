def p(j):
	D=range;A=set();E=[A[:]for A in j]
	def F(k,W):
		if(k,W)in A or not(0<=k<10 and 0<=W<10)or j[k][W]:return[]
		A.add((k,W));return[(k,W)]+sum([F(k+A,W+B)for(A,B)in[(-1,0),(1,0),(0,-1),(0,1)]],[])
	for B in D(10):
		for C in D(10):
			if j[B][C]==0 and(B,C)not in A:
				G=F(B,C)
				for(H,I)in G:E[H][I]=abs(len(G)-4)
	return E