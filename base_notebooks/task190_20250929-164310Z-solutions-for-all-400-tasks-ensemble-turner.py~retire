def p(d):
 D=range;E=[r[:]for r in d];K=[(0,1),(1,0),(0,-1),(-1,0)];L=[(-1,-1),(-1,1),(1,1),(1,-1)]
 for r in D(10):
  for B in D(10):
   if d[r][B]and all(0<=r+T<10and 0<=B+D<10and d[r+T][B+D]==0for(T,D)in K):
    for T in L:
     F,G=r+T[0],B+T[1]
     if 0<=F<10and 0<=G<10and d[F][G]:
      M,T=-T[0],-T[1]
      for H in D(1,10):
       I,i=r+M*H,B+T*H
       if 0<=I<10and 0<=i<10:E[I][i]=d[r][B]
 return E