def p(g):
	L,M=len(g),len(g[0]);E,F,D,G=L,M,-1,-1
	for A in range(L):
		for B in range(M):
			if g[A][B]!=0:
				if A<E:E=A
				if A>D:D=A
				if B<F:F=B
				if B>G:G=B
	if D<0:return[[]]
	N=[tuple(g[A][F:G+1])for A in range(E,D+1)];H=[];C=None
	for I in N:
		if I!=C:H.append(I);C=I
	O=list(zip(*H))if H else[];J=[];C=None
	for K in O:
		if K!=C:J.append(K);C=K
	return[list(A)for A in zip(*J)]if J else[]