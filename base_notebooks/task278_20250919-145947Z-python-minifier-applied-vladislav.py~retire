j=lambda A:[[A[C][B]for C in range(len(A))]for B in range(len(A[0]))]
def h(A,c,E):
	if A[c][E]!=2 or A[c][E+1]!=2:return
	for B in range(max(0,c-1),min(len(A),c+2)):
		for C in range(max(0,E-1),min(len(A[0]),E+3)):
			if A[B][C]!=2:A[B][C]=3
def f(A):
	for B in range(len(A)):
		for C in range(len(A[0])-1):h(A,B,C)
def p(A):f(A);A=j(A);f(A);return j(A)