def p(j):
	I,J=len(j),len(j[0]);from collections import Counter as U,deque;L=U(B for A in j for B in A).most_common(1)[0][0];F=[[0]*J for A in range(I)];G=[]
	for D in range(I):
		for E in range(J):
			if F[D][E]or j[D][E]==L:continue
			K=deque([(D,E)]);F[D][E]=1;H=[(D,E)]
			while K:
				V,W=K.popleft()
				for M in(-1,0,1):
					for N in(-1,0,1):
						if M==N==0:continue
						B,C=V+M,W+N
						if 0<=B<I and 0<=C<J and not F[B][C]and j[B][C]!=L:F[B][C]=1;K.append((B,C));H.append((B,C))
			O=[A for(A,B)in H];P=[A for(B,A)in H];G.append((H,(min(O),min(P),max(O),max(P))))
	G.sort(key=lambda t:len(t[0]));X,Y=G[0],G[-1];Z,a,b,c=X[1];d,e,f,g=Y[1];A=[A[a:c+1]for A in j[Z:b+1]];Q=[A[e:g+1]for A in j[d:f+1]];R=len(Q[0])//len(A[0]);h=[[1 if Q[B*R][A*R]==0 else 0 for A in range(len(A[0]))]for B in range(len(A))]
	for S in range(len(A)):
		for T in range(len(A[0])):
			if h[S][T]:A[S][T]=0
	return A