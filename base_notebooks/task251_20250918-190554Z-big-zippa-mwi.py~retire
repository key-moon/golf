def p(j,A=range):
	B,C=len(j),len(j[0]);H=[[0]*C for A in A(B)];I=[]
	for D in A(B):
		for E in A(C):
			if D*E==0 or D==B-1 or E==C-1:
				if j[D][E]==0:H[D][E]=1;I.append((D,E))
	while I:
		J,K=I.pop(0)
		for(L,M)in[(-1,0),(1,0),(0,-1),(0,1)]:
			F,G=J+L,K+M
			if 0<=F<B and 0<=G<C and j[F][G]==0 and not H[F][G]:H[F][G]=1;I.append((F,G))
	N=[[j[B][A]if j[B][A]!=0 or H[B][A]else 1 for A in A(C)]for B in A(B)];return N