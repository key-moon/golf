from collections import Counter
def p(m):
	E=Counter(A for B in m for A in B if A).most_common()
	if not E:return[]
	F=E[-1][0];B=C=-1
	for(A,H)in enumerate(m):
		if F in H:
			if B<0:B=A
			C=A
	D=G=-1
	for A in range(len(m[0])):
		if any(m[B][A]==F for B in range(B,C+1)):
			if D<0:D=A
			G=A
	return[A[D:G+1]for A in m[B:C+1]]