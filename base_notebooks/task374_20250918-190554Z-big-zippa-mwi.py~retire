def p(j):
	I=len(j);J=len(j[0]);K=[]
	for F in range(I):
		for G in range(J):
			if j[F][G]==5:
				H=[(F,G)];j[F][G]=0;C=[]
				while H:
					A,B=H.pop();C+=[(A,B)]
					for(D,E)in((A+1,B),(A-1,B),(A,B+1),(A,B-1)):
						if 0<=D<I and 0<=E<J and j[D][E]==5:j[D][E]=0;H+=[(D,E)]
				K+=C,
	for(C,L)in zip(sorted(K,key=len),(2,4,1)):
		for(A,B)in C:j[A][B]=L
	return j