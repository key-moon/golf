R=len
r=range
def p(i):
 E,F=[],[];T,N=R(i),R(i[0])
 for B in r(T-4):
  for d in r(N-4):
   D=[[i[r+B][D+d]for r in r(5)]for D in r(5)];D=[B for r in D for B in r];D=sum([r for r in D if r==1])
   if D==16:E.append(B+2);F.append(d+2)
 for B in r(T):
  for d in r(N):
   if B in E or d in F:
    if i[B][d]!=1:i[B][d]=6
 return i