def p(D,e=len,i=range):
 n,n=e(D),e(D[0]);R,O=0,[]
 for o in i(e(D)):
  C=[D[o][0]]
  for r in i(e(D[o])-1):
   if D[o][r+1]!=D[o][r]:C+=[D[o][r+1]]
  if e(C)>R:R=e(C);O=C[:]
 D=[O[:]for o in i(e(O))]
 for o in i(e(D)//2):
  for R in i(o,e(D[0])-o-1):D[o][R]=D[o][o];D[-(o+1)][R]=D[-(o+1)][o]
 return D