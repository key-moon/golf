def p(j):
	J=range;K=[A[:]for A in j];F=set();L=[(0,1),(1,0),(0,-1),(-1,0)]
	for C in J(10):
		for D in J(10):
			if j[C][D]==5 and(C,D)not in F:
				E,G=set(),[(C,D)];E.add((C,D));F.add((C,D))
				while G:
					H,I=G.pop(0)
					for(M,N)in L:
						A,B=H+M,I+N
						if 0<=A<10 and 0<=B<10 and j[A][B]==5 and(A,B)not in E:E.add((A,B));F.add((A,B));G.append((A,B))
				O=5-len(E)
				for(H,I)in E:K[H][I]=O
	return K