h=range
N=len
def p(g):
 D=sum(g,[]);C=[R for R in set(D)if R not in[0,2]][0];i,i=N(g),N(g[0])
 for G in h(i-1):
  for l in h(i-1):
   e=[g[G+R][l+n]for(R,n)in[[0,0],[0,1],[1,0],[1,1]]]
   if sum([1for R in e if R>0])>3:
    for e in[R for R in h(N(e))if e[R]==2]:
     for a in h(10):
      if e<2:R=G-a
      else:R=G+a+1
      if e%2:n=l+a+1
      else:n=l-a
      if 0<=R<i and 0<=n<i:
       g[R][n]=C
       if 0<=R-1<i:g[R-1][n]=C
       if 0<=n-1<i:g[R][n-1]=C
       if 0<=R+1<i:g[R+1][n]=C
       if 0<=n+1<i:g[R][n+1]=C
 return g