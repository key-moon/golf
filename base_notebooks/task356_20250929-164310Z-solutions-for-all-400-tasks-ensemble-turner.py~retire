def p(i,H=range):
 E=[H[:]for H in i]
 for F in H(1,10):
  T=[(T,a)for T in H(len(i))for a in H(len(i[0]))if i[T][a]==F]
  for e in H(len(T)):
   for J in H(e+1,len(T)):
    a,D=T[e];r,l=T[J]
    if a==r:
     for r in H(min(D,l),max(D,l)+1):E[a][r]=F
    elif D==l:
     for a in H(min(a,r),max(a,r)+1):E[a][D]=F
 return E