def p(h):
 h=[[0 if r==max(h[0])else r for r in r]for r in h];h=[r for r in h if max(r)>0];h=[list(r)for r in zip(*h)];h=[r[::-1]for r in h];h=[r for r in h if max(r)>0];h=[r[::-1]for r in h];h=[list(r)for r in zip(*h)];r=[r[:]for r in h];h=[[r[0][0],0,r[0][3]],[0,0,0],[r[3][0],0,r[3][3]]]
 if r[0].count(0)==1:h[0][1]=max(r[0])
 if r[3].count(0)==1:h[2][1]=max(r[3])
 if h[2][0]==h[2][2]:h[2][1]=h[2][0]
 if r[0][0]==r[0][2]and r[0][0]>0:h[0][1]=h[0][0]
 if r[0][0]==r[2][0]and r[0][0]>0:h[1][0]=h[0][0]
 if r[0][3]==r[2][3]and r[0][3]>0:h[1][2]=h[0][2]
 if r[3][0]==r[1][0]and r[3][0]>0:h[1][0]=h[2][0]
 if r[3][3]==r[1][3]and r[3][3]>0:h[1][2]=h[2][2]
 if r[0][1]==r[0][2]and r[0][0]==0and r[0][3]==0:h[0][1]=r[0][1]
 if r[1][0]==r[2][0]and r[0][0]==0and r[3][0]==0:h[1][0]=r[1][0]
 if r[3][1]==r[3][2]and r[3][0]==0and r[3][3]==0:h[2][1]=r[3][1]
 if r[1][3]==r[2][3]and r[0][3]==0and r[3][3]==0:h[1][2]=r[2][3]
 return h