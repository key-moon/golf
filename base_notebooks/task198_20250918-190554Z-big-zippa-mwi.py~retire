L=len
R=range
def p(g):
	G=max(sum(g,[]))
	for H in range(4):
		g=list(map(list,zip(*g[::-1])));D,C=L(g),L(g[0])
		for A in R(D):
			if g[A].count(G)>C/2:
				for B in R(C):
					if g[A][B]==0:g[A][B]=4
	for H in range(10):
		for A in R(D):
			for B in R(C):
				if g[A][B]==4:
					for(E,F)in[[0,1],[0,-1],[1,0],[-1,0]]:
						if 0<=A+E<D and 0<=B+F<C and g[A+E][B+F]==0:g[A+E][B+F]=4
	g=[[3 if A==0 else A for A in A]for A in g];return g