def p(g,L=len,R=range):
	C,D=L(g),L(g[0])
	for E in R(25):
		for A in R(C):
			for B in R(D):
				if g[A][B]==0:
					if B+1<D:
						if g[A][B+1]==1:g[A][B]=1
					if A+1<C:
						if g[A+1][B]==1:g[A][B]=1
					if B-1>=0:
						if g[A][B-1]==1:g[A][B]=1
					if A-1>=0:
						if g[A-1][B]==1:g[A][B]=1
	return g