def p(g):
    if len(g) < len(g[0]):                       # fewer rows than columns â†’ fix columns
        for j in range(len(g[0])):
            d = [r[j] for r in g]; v = max(d, key=d.count)
            for r in g: r[j] = v
    else:                                        # otherwise fix rows
        for r in g:
            v = max(r, key=r.count)
            r[:] = [v] * len(r)
    return g
