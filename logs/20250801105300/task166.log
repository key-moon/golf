def p(g):
 h,w=len(g),len(g[0])
 # mark all zeros as temporary fill (3)
 for i in range(h):
  for j in range(w):
   if g[i][j]==0:g[i][j]=3
 # gather all border cells to seed flood-fill
 s=[(i,j)for i in(0,h-1)for j in range(w)]+[(i,j)for i in range(h)for j in(0,w-1)]
 # flood-fill from borders, restoring exterior 3s back to 0
 while s:
  i,j=s.pop()
  if g[i][j]==3:
   g[i][j]=0
   for di,dj in((1,0),(-1,0),(0,1),(0,-1)):
    ni,nj=i+di,j+dj
    if 0<=ni<h and 0<=nj<w:s.append((ni,nj))
 # any 3s left are interior holesâ€”turn them into 2
 for i in range(h):
  for j in range(w):
   if g[i][j]==3:g[i][j]=2
 return g
