def p(g):
    o = [[0]*3 for _ in[0]*3]
    for i, r in enumerate(g):
        for j, v in enumerate(r):
            if v == 5:
                for di in (-1, 0, 1):
                    for dj in (-1, 0, 1):
                        if di|dj:
                            try:
                                t = g[i+di][j+dj]
                            except:
                                pass
                            else:
                                if t and t-5:
                                    o[di+1][dj+1] = t
    o[1][1] = 5
    return o
