def p(g):
    for r in g:
        a=[i for i,x in enumerate(r) if x==2]
        if a:
            for i in range(a[0],a[-1]+1):
                if r[i]!=2: r[i]=8
    for j in range(len(g[0])):
        b=[i for i in range(len(g)) if g[i][j]==2]
        if b:
            for i in range(b[0],b[-1]+1):
                if g[i][j]!=2: g[i][j]=8
    return g
