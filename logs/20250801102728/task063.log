def p(g):
 h=len(g);w=len(g[0]);v=set();B=[];m=0
 for i in range(h):
  for j in range(w):
   if g[i][j]==0 and (i,j) not in v:
    Q=[(i,j)];v.add((i,j));C=[]
    while Q:
     x,y=Q.pop();C.append((x,y))
     for d in (1,-1):
      for a,b in ((x+d,y),(x,y+d)):
       if 0<=a<h and 0<=b<w and g[a][b]==0 and (a,b) not in v:
        v.add((a,b));Q.append((a,b))
    if len(C)>m:m,B=len(C),C
 for x,y in B:g[x][y]=3
 return g
