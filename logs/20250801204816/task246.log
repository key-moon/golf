def p(val_g):
 for val_i,val_row in enumerate(val_g):
  for val_j,val_c in enumerate(val_row):
   if val_c==2:val_p,val_q=val_i,val_j
   elif val_c==3:val_r,val_s=val_i,val_j
 for val_j in range(min(val_q,val_s),max(val_q,val_s)+1):
  if not val_g[val_p][val_j]:val_g[val_p][val_j]=8
 for val_i in range(min(val_p,val_r),max(val_p,val_r)+1):
  if not val_g[val_i][val_s]:val_g[val_i][val_s]=8
 return val_g