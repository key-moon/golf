def p(g):
    for i in range(9):
        for j in range(9):
            if g[i][j]==2:
                for x,y in((-1,-1),(-1,1),(1,-1),(1,1)):
                    ii=i+x; jj=j+y
                    if 0<=ii<9 and 0<=jj<9: g[ii][jj]=4
            if g[i][j]==1:
                for x,y in((-1,0),(1,0),(0,-1),(0,1)):
                    ii=i+x; jj=j+y
                    if 0<=ii<9 and 0<=jj<9: g[ii][jj]=7
    return g
