def p(g):
 r,c=len(g),len(g[0])
 for i in range(r):
  for j in range(c):
   if g[i][j]==1 and (i<1 or g[i-1][j]!=1) and (j<1 or g[i][j-1]!=1):
    d=1
    while j+d<c and g[i][j+d]==1:d+=1
    x=i+d//2;y=j+d//2
    for t in range(r):
     if g[t][y]==8:g[t][y]=6
    for t in range(c):
     if g[x][t]==8:g[x][t]=6
 return g
