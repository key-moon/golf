def p(g):
 n=len(g);m=len(g[0]);b=[];V=0
 for i in range(n):
  for j in range(m):
   if g[i][j]:
    a=g[i][j];s=[(i,j)];g[i][j]=0;c=[]
    while s:
     y,x=s.pop();c.append((y,x))
     for dy,dx in((1,0),(-1,0),(0,1),(0,-1)):
      y1=y+dy;x1=x+dx
      if 0<=y1<n and 0<=x1<m and g[y1][x1]==a:g[y1][x1]=0;s.append((y1,x1))
    if len(c)>len(b):b=c;V=a
 v=[[0]*m for _ in g]
 for y,x in b:v[y][x]=V
 return v
