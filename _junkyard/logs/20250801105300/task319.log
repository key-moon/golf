def p(g):
 C={}
 D={}
 for i,R in enumerate(g):
  for j,v in enumerate(R):
   C[v]=C.get(v,0)+1
   if v in D:
    a,b,c,e=D[v];D[v]=[min(a,i),min(b,j),max(c,i),max(e,j)]
   else:D[v]=[i,j,i,j]
 bg=max(C,key=C.get)
 L=[]
 for v,(a,b,c,e) in D.items():
  if v==bg:continue
  h=c-a+1;w=e-b+1
  if h%2 and w%2:L.append((h*w,-v,a,c,b,e))
 X=max(L)
 return [r[X[4]:X[5]+1] for r in g[X[2]:X[3]+1]]
