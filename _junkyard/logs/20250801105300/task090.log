def p(g):
    h,w=len(g),len(g[0]);b=0;I=J=K=L=0
    for i in range(h):
        for j in range(i+1,h+1):
            for k in range(w):
                for l in range(k+1,w+1):
                    if (j-i)*(l-k)>b and all(not g[x][y] for x in range(i,j) for y in range(k,l)):
                        b=(j-i)*(l-k);I=i;J=j;K=k;L=l
    for x in range(I,J):
        for y in range(K,L):
            g[x][y]=6
    return g
