def p(val_g):
    val_d={}
    for val_y,val_r in enumerate(val_g):
        for val_x,val_c in enumerate(val_r):
            if val_c:
                if val_c in val_d:
                    val_m,val_n,val_o,val_p=val_d[val_c]
                    val_d[val_c]=[min(val_m,val_x),min(val_n,val_y),max(val_o,val_x),max(val_p,val_y)]
                else:
                    val_d[val_c]=[val_x,val_y,val_x,val_y]
    for val_c,(val_m,val_n,val_o,val_p) in val_d.items():
        if val_o-val_m>1 and val_p-val_n>1:
            for val_y in range(val_n+1,val_p):
                for val_x in range(val_m+1,val_o):
                    if val_g[val_y][val_x]==val_c:
                        val_g[val_y][val_x]=8
    return val_g