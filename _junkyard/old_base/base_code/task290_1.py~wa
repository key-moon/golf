def p(g):val_r=[val_i for val_i,val_row in enumerate(g)if any(val_row)];val_c=[val_j for val_j,val_col in enumerate(zip(*g))if any(val_col)];val_a=[val_row[val_c[0]:val_c[-1]+1]for val_row in g[val_r[0]:val_r[-1]+1]];val_h,val_w=len(val_a),len(val_a[0]);val_b=val_a[0][0];val_e=val_a[val_h//2][val_w//2];return[[val_e if val_i*val_j*(val_h-1-val_i)*(val_w-1-val_j)==0 else val_b for val_j in range(val_w)]for val_i in range(val_h)]
