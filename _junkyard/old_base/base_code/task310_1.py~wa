def p(g):val_s=[i for i,r in enumerate(g)if len(set(r))==1];val_k=val_s[0];val_t=val_k+1;val_B=[r[:val_k]for r in g[:val_k]];val_i,val_j=next((i,j)for i in range(len(val_s))for j in range(len(val_s))if any(g[i*val_t+p][j*val_t+q]!=val_B[p][q]for p in range(val_k)for q in range(val_k)));val_u,val_v=val_i*val_t-1,val_j*val_t-1;return [r[val_v:val_v+val_t+1]for r in g[val_u:val_u+val_t+1]]
