def J(A):return max(A for(A,B)in H(A))
def V(A):return min(A for(A,B)in H(A))
def Y(A):return max(A for(B,A)in H(A))
def R(A):return min(A for(B,A)in H(A))
def U(A):
	if isinstance(A,tuple):return frozenset({B for A in A for B in A})
	return frozenset({A for(A,B)in A})
def P(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def H(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def Z(A,B,C):return tuple(A[B[1]:B[1]+C[1]]for A in A[B[0]:B[0]+C[0]])
def M(A):return tuple(map(min,zip(*H(A))))
def L(A):return K(A),S(A)
def X(A,B):return Z(B,M(A),L(A))
def G(A):return frozenset(frozenset((A,(C,E))for(C,D)in enumerate(A)for(E,A)in enumerate(D)if A==B)for B in U(A))
def S(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return Y(A)-R(A)+1
def K(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return J(A)-V(A)+1
def W(A,a,b):return lambda x:A(a(x),b(x))
def Q(A,B):return min(A,key=B)
def E(a,b):
	if isinstance(a,int)and isinstance(b,int):return a*b
	elif isinstance(a,tuple)and isinstance(b,tuple):return a[0]*b[0],a[1]*b[1]
	elif isinstance(a,int)and isinstance(b,tuple):return a*b[0],a*b[1]
	return a[0]*b,a[1]*b
def p(I):I=tuple(map(tuple,I));A=G(I);B=W(E,K,S);C=Q(A,B);D=X(C,I);return[*map(list,D)]