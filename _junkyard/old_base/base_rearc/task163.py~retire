def H(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def M(A):return G(A)|H(A)
def G(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def Q(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def P(A):return max(A for(A,B)in J(A))
def V(A):return max(A for(B,A)in J(A))
def Z(A):return min(A for(B,A)in J(A))
def R(A):return min(A for(A,B)in J(A))
def HH(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return V(A)-Z(A)+1
def GP(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return P(A)-R(A)+1
def HE(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def J(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def W(A):return tuple(map(min,zip(*J(A))))
def GV(A):
	if isinstance(A,tuple):return tuple(zip(*A))
	B,C=W(A)
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(E-C+B,D-B+C))for(A,(D,E))in A)
	return frozenset((D-C+B,A-B+C)for(A,D)in A)
def S(A):C,D=len(A),len(A[0]);B=tuple(A for(A,B)in enumerate(A)if len(set(B))==1);E=tuple(A for(A,B)in enumerate(GV(A))if len(set(B))==1);F=frozenset({frozenset({(A[B][C],(B,C))for C in range(D)})for B in B});G=frozenset({frozenset({(A[C][B],(C,B))for C in range(C)})for B in E});return F|G
def GK(A,B):return tuple(tuple(A for B in range(B[1]))for C in range(B[0]))
def GS(A):return R(A)+GP(A)//2,Z(A)+HH(A)//2
def GH(a,b):return tuple(A+B for(A,B)in zip(a,b))
def GW(A,B):
	F,G=len(A),len(A[0]);C=list(list(A)for A in A)
	for(H,(D,E))in B:
		if 0<=D<F and 0<=E<G:C[D][E]=H
	return tuple(tuple(A)for A in C)
def HX(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in J(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def GY(A):
	if isinstance(A,tuple):return frozenset({B for A in A for B in A})
	return frozenset({A for(A,B)in A})
def GM(A,B,C,D):
	N=K(A)if D else None;O=set();H=set();S,T=len(A),len(A[0]);U=Q(A);V=M if C else G
	for E in U:
		if E in H:continue
		I=A[E[0]][E[1]]
		if I==N:continue
		P={(I,E)};J={E}
		while len(J)>0:
			R=set()
			for F in J:
				L=A[F[0]][F[1]]
				if I==L if B else L!=N:P.add((L,F));H.add(F);R|={(A,B)for(A,B)in V(F)if 0<=A<S and 0<=B<T}
			J=R-H
		O.add(frozenset(P))
	return frozenset(O)
def U(A):
	if len(A)==0:return A
	return HJ(A,(-R(A),-Z(A)))
def HJ(A,C):
	if len(A)==0:return A
	B,D=C
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(C+B,E+D))for(A,(C,E))in A)
	return frozenset((A+B,C+D)for(A,C)in A)
def Y(A,B):
	if isinstance(A,tuple):return sum(A.count(B)for A in A)
	return sum(A==B for(A,C)in A)
def HV(A):return GP(A),HH(A)
def K(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def GL(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda y:A(B,y)
	elif C==3:return lambda y,z:A(B,y,z)
	else:return lambda y,z,a:A(B,y,z,a)
def HY(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def GJ(A,B):return lambda x:A(x)==B
def HM(h,g,f):return lambda x:h(g(f(x)))
def GR(A,B):return lambda x:A(B(x))
def GU(A,B):return type(B)(B for B in B if B!=A)
def HG(A):return next(iter(A))
def GE(A,B):return next(A for A in A if B(A))
def GG(A,B):return type(A)(A for A in A if B(A))
def X(x):return x+1 if isinstance(x,int)else(x[0]+1,x[1]+1)
def GX(A,B):return max(A,key=B)
def GQ(A,B):return B(max(A,key=B,default=0))
def GZ(A):return type(A)(B for A in A for B in A)
def HU(A):return len(A)
def E(A,B):return A in B
def HR(b):return not b
def L(a,b):
	if isinstance(a,int)and isinstance(b,int):return a*b
	elif isinstance(a,tuple)and isinstance(b,tuple):return a[0]*b[0],a[1]*b[1]
	elif isinstance(a,int)and isinstance(b,tuple):return a*b[0],a*b[1]
	return a[0]*b,a[1]*b
def p(I):H=False;I=tuple(map(tuple,I));J=S(I);A=GZ(J);D=K(A);M=HV(I);N=GK(-1,M);O=GH(I,N);P=HX(O,-1,A);B=GM(P,H,H,True);Q=GL(E,4);R=GR(Q,GY);T=GE(B,R);V=GL(GG,B);W=GR(HU,V);Z=HY(GR,GY);a=GL(GL,E);F=HM(W,Z,a);b=GZ(B);c=GY(I);G=GU(D,c);d=GQ(G,F);e=GJ(F,d);f=GG(G,e);g=GL(Y,b);h=GX(f,g);i=HV(I);j=GK(h,i);k=GW(j,A);C=U(T);l=GJ(HG,D);m=GR(HR,l);n=GG(C,m);o=HV(C);p=X(o);q=GJ(HG,4);r=GG(C,q);s=GS(r);t=L(p,s);u=HJ(n,t);v=GW(k,u);return[*map(list,v)]