def GG(A,B):return type(B)(A(B)for B in B)
def S(A):return type(A)(B for A in A for B in A)
def GH(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def E(A):return max(A for(A,B)in Y(A))
def V(A):return min(A for(A,B)in Y(A))
def J(A):return max(A for(B,A)in Y(A))
def R(A):return min(A for(B,A)in Y(A))
def L(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return J(A)-R(A)+1
def Q(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return E(A)-V(A)+1
def Z(A):return Q(A),L(A)
def G(A,B):
	C=set();K=M(B);D,E=len(A),len(A[0]);L,N=Z(B);O,P=D-L+1,E-N+1
	for F in range(O):
		for G in range(P):
			H=True
			for(Q,(I,J))in W(K,(F,G)):
				if not(0<=I<D and 0<=J<E and A[I][J]==Q):H=False;break
			if H:C.add((F,G))
	return frozenset(C)
def GY(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in Y(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def M(A):
	if len(A)==0:return A
	return W(A,(-V(A),-R(A)))
def W(A,C):
	if len(A)==0:return A
	B,D=C
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(C+B,E+D))for(A,(C,E))in A)
	return frozenset((A+B,C+D)for(A,C)in A)
def X(A,B):return frozenset((A,B)for B in Y(B))
def Y(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def U(A,B):return frozenset((A,D)for(A,C)in enumerate(A)for(D,E)in enumerate(C)if E==B)
def H(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return min(set(B),key=B.count)
def K(A,B):return S(GG(A,B))
def P(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda y:A(B,y)
	elif C==3:return lambda y,z:A(B,y,z)
	else:return lambda y,z,a:A(B,y,z,a)
def p(I):I=tuple(map(tuple,I));A=H(I);C=U(I,A);D=X(0,C);B=M(D);E=G(I,B);F=Y(B);J=P(W,F);L=K(J,E);N=GY(I,A,L);return[*map(list,N)]