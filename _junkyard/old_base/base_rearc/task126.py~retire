def GU(A):return type(A)(B for A in A for B in A)
def K(A):return max(A for(A,B)in Y(A))
def J(A):return max(A for(B,A)in Y(A))
def GS(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def H(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def V(A):return G(A)|H(A)
def G(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def U(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def X(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def S(A):return min(A for(B,A)in Y(A))
def R(A):return min(A for(A,B)in Y(A))
def L(A):return tuple(map(max,zip(*Y(A))))
def P(A):return tuple(map(min,zip(*Y(A))))
def Q(A):
	if len(A)==0:return frozenset({})
	B=Y(A);C,D=P(B);E,F=L(A);return frozenset((A,B)for A in range(C,E+1)for B in range(D,F+1))
def GL(B):
	if len(B)==0:return frozenset({})
	return Q(B)-Y(B)
def GE(A):return R(A)+GM(A)//2,S(A)+GK(A)//2
def GZ(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in Y(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def GG(A,B,C,D):
	L=X(A)if D else None;M=set();H=set();P,Q=len(A),len(A[0]);R=U(A);S=V if C else G
	for E in R:
		if E in H:continue
		I=A[E[0]][E[1]]
		if I==L:continue
		N={(I,E)};J={E}
		while len(J)>0:
			O=set()
			for F in J:
				K=A[F[0]][F[1]]
				if I==K if B else K!=L:N.add((K,F));H.add(F);O|={(A,B)for(A,B)in S(F)if 0<=A<P and 0<=B<Q}
			J=O-H
		M.add(frozenset(N))
	return frozenset(M)
def Y(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def W(A):return GM(A)>GK(A)
def GK(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return J(A)-S(A)+1
def GM(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return K(A)-R(A)+1
def GH(A,B):return GU(GP(A,B))
def GP(A,B):return type(B)(A(B)for B in B)
def GR(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda y:A(B,y)
	elif C==3:return lambda y,z:A(B,y,z)
	else:return lambda y,z,a:A(B,y,z,a)
def GQ(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def GV(A,a,b):return a if A else b
def Z(a,b):return a,b
def GW(A):return max(enumerate(A))[1]
def GX(A):return next(iter(A))
def GJ(j):return 0,j
def GY(i):return i,0
def E(x):return x-1 if isinstance(x,int)else(x[0]-1,x[1]-1)
def M(A,B):return A in B
def HG(a,b):
	if isinstance(a,int)and isinstance(b,int):return a+b
	elif isinstance(a,tuple)and isinstance(b,tuple):return a[0]+b[0],a[1]+b[1]
	elif isinstance(a,int)and isinstance(b,tuple):return a+b[0],a+b[1]
	return a[0]+b,a[1]+b
def p(I):I=tuple(map(tuple,I));C=GG(I,True,False,True);D=GH(GL,C);B=GX(C);F=Y(B);G=GQ(M,F);A=W(B);H=GP(GX,D);J=GP(GW,D);K=GV(A,H,J);L=GV(A,(0,1),(1,0));N=GL(B);O=GE(N);P=HG(O,L);Q=G(P);R=GV(A,GK,GM);S=GV(A,GQ,GR);T=R(I);U=E(T);V=S(Z,U);X=GV(A,GY,GJ);a=GV(Q,X,V);b=GP(a,K);c=GZ(I,4,b);return[*map(list,c)]