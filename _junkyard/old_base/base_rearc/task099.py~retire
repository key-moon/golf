def GS(A,B):return type(B)(A(B)for B in B)
def GU(A):return type(A)(B for A in A for B in A)
def Y(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def E(A):return G(A)|Y(A)
def G(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def U(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def X(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def GL(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def Q(A):
	if len(A)==0:return frozenset({})
	B=M(A);C,D=P(B);E,F=L(A);return frozenset((A,B)for A in range(C,E+1)for B in range(D,F+1))
def M(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def GW(B):
	if len(B)==0:return frozenset({})
	return Q(B)-M(B)
def GY(a,b):
	A,B=a;C,D=b;E=min(A,C);F=max(A,C)+1;G=min(B,D);H=max(B,D)+1
	if A==C:return frozenset((A,B)for B in range(G,H))
	elif B==D:return frozenset((A,B)for A in range(E,F))
	elif C-A==D-B:return frozenset((A,B)for(A,B)in zip(range(E,F),range(G,H)))
	elif C-A==B-D:return frozenset((A,B)for(A,B)in zip(range(E,F),range(H-1,G-1,-1)))
	return frozenset()
def GX(A,B):
	F,G=len(A),len(A[0]);C=list(list(A)for A in A)
	for(H,(D,E))in B:
		if 0<=D<F and 0<=E<G:C[D][E]=H
	return tuple(tuple(A)for A in C)
def S(A,B):D,E=len(B),len(B[0]);return frozenset((B[A][C],(A,C))for(A,C)in M(A)if 0<=A<D and 0<=C<E)
def R(a,b):return min(abs(A-C)+abs(B-D)for(A,B)in M(a)for(C,D)in M(b))
def GJ(A,B,C,D):
	M=X(A)if D else None;N=set();I=set();Q,R=len(A),len(A[0]);S=U(A);T=E if C else G
	for F in S:
		if F in I:continue
		J=A[F[0]][F[1]]
		if J==M:continue
		O={(J,F)};K={F}
		while len(K)>0:
			P=set()
			for H in K:
				L=A[H[0]][H[1]]
				if J==L if B else L!=M:O.add((L,H));I.add(H);P|={(A,B)for(A,B)in T(H)if 0<=A<Q and 0<=B<R}
			K=P-I
		N.add(frozenset(O))
	return frozenset(N)
def GQ(A,C):
	if len(A)==0:return A
	B,D=C
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(C+B,E+D))for(A,(C,E))in A)
	return frozenset((A+B,C+D)for(A,C)in A)
def GG(A,B):return frozenset((A,B)for B in M(B))
def L(A):return tuple(map(max,zip(*M(A))))
def W(A):return tuple(map(lambda ix:{0:max,1:min}[ix[0]](ix[1]),enumerate(zip(*M(A)))))
def K(A):return tuple(map(lambda ix:{0:min,1:max}[ix[0]](ix[1]),enumerate(zip(*M(A)))))
def P(A):return tuple(map(min,zip(*M(A))))
def H(A,n):return frozenset(A for A in A if len(A)==n)
def J(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return min(set(B),key=B.count)
def GM(A,B):return GU(GS(A,B))
def GZ(A,a,b):return lambda x:A(a(x),b(x))
def GR(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda y:A(B,y)
	elif C==3:return lambda y,z:A(B,y,z)
	else:return lambda y,z,a:A(B,y,z,a)
def GK(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def GP(h,g,f):return lambda x:h(g(f(x)))
def GV(A,B):return lambda x:A(B(x))
def GH(a,b):return a,b
def GE(A,B):return min(A,key=B)
def V(a,b):return type(a)(A for A in a if A not in b)
def Z(a,b):return type(a)((*a,*b))
def p(I):I=tuple(map(tuple,I));A=GJ(I,True,False,True);B=H(A,1);C=V(A,B);D=GK(S,I);E=GP(J,D,GW);F=GK(GQ,(-1,0));G=GZ(GY,P,K);M=GV(F,G);N=GK(GQ,(1,0));O=GZ(GY,W,L);Q=GV(N,O);T=GZ(GH,M,Q);U=GR(GK,R);X=GV(U,GW);Y=GZ(GE,T,X);a=GZ(Z,GW,Y);b=GZ(GG,E,a);c=GM(b,C);d=GX(I,c);return[*map(list,d)]