def H(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def V(A):return Y(A)|H(A)
def Y(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def E(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def K(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def M(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def Q(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in M(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def X(A,B,C,D):
	L=R(A)if D else None;M=set();H=set();P,Q=len(A),len(A[0]);S=E(A);T=V if C else Y
	for F in S:
		if F in H:continue
		I=A[F[0]][F[1]]
		if I==L:continue
		N={(I,F)};J={F}
		while len(J)>0:
			O=set()
			for G in J:
				K=A[G[0]][G[1]]
				if I==K if B else K!=L:N.add((K,G));H.add(G);O|={(A,B)for(A,B)in T(G)if 0<=A<P and 0<=B<Q}
			J=O-H
		M.add(frozenset(N))
	return frozenset(M)
def J(A,n):return frozenset(A for A in A if len(A)==n)
def G(A,B):return frozenset(A for A in A if next(iter(A))[0]==B)
def R(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def U(A):return type(A)(B for A in A for B in A)
def p(I):B=False;I=tuple(map(tuple,I));C=X(I,True,B,B);D=R(I);A=G(C,D);E=J(A,1);F=U(E);H=J(A,4);K=U(H);L=J(A,9);M=U(L);N=Q(I,6,F);O=Q(N,7,K);P=Q(O,8,M);return[*map(list,P)]