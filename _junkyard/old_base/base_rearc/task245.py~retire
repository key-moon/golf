def X(A):return max(A for(A,B)in Y(A))
def J(A):return max(A for(B,A)in Y(A))
def GX(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def S(A):return min(A for(B,A)in Y(A))
def V(A):return min(A for(A,B)in Y(A))
def GH(A):
	if isinstance(A,tuple):return frozenset({B for A in A for B in A})
	return frozenset({A for(A,B)in A})
def L(A):return tuple(map(max,zip(*Y(A))))
def W(A):return tuple(map(lambda ix:{0:max,1:min}[ix[0]](ix[1]),enumerate(zip(*Y(A)))))
def K(A):return tuple(map(lambda ix:{0:min,1:max}[ix[0]](ix[1]),enumerate(zip(*Y(A)))))
def GG(A):return frozenset({Q(A),K(A),W(A),L(A)})
def GV(A,B):
	F,G=len(A),len(A[0]);C=list(list(A)for A in A)
	for(H,(D,E))in B:
		if 0<=D<F and 0<=E<G:C[D][E]=H
	return tuple(tuple(A)for A in C)
def GK(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in Y(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def G(A):return frozenset(frozenset((A,(C,E))for(C,D)in enumerate(A)for(E,A)in enumerate(D)if A==B)for B in GH(A)-{R(A)})
def E(A):
	if len(A)==0:return A
	return GR(A,(-V(A),-S(A)))
def GR(A,C):
	if len(A)==0:return A
	B,D=C
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(C+B,E+D))for(A,(C,E))in A)
	return frozenset((A+B,C+D)for(A,C)in A)
def Y(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def Q(A):return tuple(map(min,zip(*Y(A))))
def GE(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return J(A)-S(A)+1
def GJ(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return X(A)-V(A)+1
def R(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def GU(A,a,b):return lambda x:A(a(x),b(x))
def Z(A,B):return type(A)(A for A in A if B(A))
def M(x):return x+1 if isinstance(x,int)else(x[0]+1,x[1]+1)
def GY(A,B):return max(A,key=B)
def GM(A):return type(A)(B for A in A for B in A)
def H(a,b):return type(a)(A for A in a if A not in b)
def P(a,b):return a==b
def U(a,b):
	if isinstance(a,int)and isinstance(b,int):return a*b
	elif isinstance(a,tuple)and isinstance(b,tuple):return a[0]*b[0],a[1]*b[1]
	elif isinstance(a,int)and isinstance(b,tuple):return a*b[0],a*b[1]
	return a[0]*b,a[1]*b
def p(I):I=tuple(map(tuple,I));A=G(I);D=GM(A);F=GU(P,Y,GG);J=GU(U,GJ,GE);K=Z(A,F);B=GY(K,J);C=H(D,B);L=R(I);N=GK(I,L,C);O=E(C);S=Q(B);T=M(S);V=GR(O,T);W=GV(N,V);return[*map(list,W)]