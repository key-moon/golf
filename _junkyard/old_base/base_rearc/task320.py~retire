def GX(A,B):return type(B)(A(B)for B in B)
def GV(A):return type(A)(B for A in A for B in A)
def GU(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def H(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def V(A):return G(A)|H(A)
def G(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def R(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def J(A):return max(A for(B,A)in Y(A))
def Q(A):return min(A for(B,A)in Y(A))
def U(A):return max(A for(A,B)in Y(A))
def E(A):return min(A for(A,B)in Y(A))
def GY(A):G,H=E(A)-1,Q(A)-1;I,K=U(A)+1,J(A)+1;B,C=min(G,I),min(H,K);D,F=max(G,I),max(H,K);L={(A,C)for A in range(B,D+1)}|{(A,F)for A in range(B,D+1)};M={(B,A)for A in range(C,F+1)}|{(D,A)for A in range(C,F+1)};return frozenset(L|M)
def GQ(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in Y(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def K(a,b):return M(a,b)==1
def M(a,b):return min(abs(A-C)+abs(B-D)for(A,B)in Y(a)for(C,D)in Y(b))
def W(A,B,C,D):
	L=R(A)if D else None;M=set();H=set();P,Q=len(A),len(A[0]);S=X(A);T=V if C else G
	for E in S:
		if E in H:continue
		I=A[E[0]][E[1]]
		if I==L:continue
		N={(I,E)};J={E}
		while len(J)>0:
			O=set()
			for F in J:
				K=A[F[0]][F[1]]
				if I==K if B else K!=L:N.add((K,F));H.add(F);O|={(A,B)for(A,B)in T(F)if 0<=A<P and 0<=B<Q}
			J=O-H
		M.add(frozenset(N))
	return frozenset(M)
def Y(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def X(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def GH(A,B):return GV(GX(A,B))
def GK(A,a,b):return lambda x:A(a(x),b(x))
def GJ(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda y:A(B,y)
	elif C==3:return lambda y,z:A(B,y,z)
	else:return lambda y,z,a:A(B,y,z,a)
def GE(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def GR(h,g,f):return lambda x:h(g(f(x)))
def GG(A,B):return lambda x:A(B(x))
def Z(A,B):return next(A for A in A if B(A))
def P(A,B):return type(A)(A for A in A if B(A))
def L(A):return frozenset({A})
def GP(A):return len(A)
def S(a,b):return a>b
def GM(n):return n//2 if isinstance(n,int)else(n[0]//2,n[1]//2)
def p(I):I=tuple(map(tuple,I));A=W(I,True,False,True);B=X(I);C=GY(B);D=GJ(K,C);E=GG(D,L);F=GE(Z,E);G=GG(F,Y);H=GE(GG,L);J=GJ(GE,M);N=GR(H,J,L);O=GJ(GJ,S);Q=GR(O,GM,GP);R=GG(N,G);T=GK(GG,Q,R);U=GK(P,Y,T);V=GH(U,A);a=GQ(I,8,V);return[*map(list,a)]