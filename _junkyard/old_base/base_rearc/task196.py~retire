def S(A,B):return type(A)(A for A in A if B(A))
def GY(A):return type(A)(B for A in A for B in A)
def X(A):return max(A for(A,B)in Y(A))
def M(A):return min(A for(A,B)in Y(A))
def J(A):return max(A for(B,A)in Y(A))
def Q(A):return min(A for(B,A)in Y(A))
def GJ(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return J(A)-Q(A)+1
def GH(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return X(A)-M(A)+1
def GR(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def H(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def V(A):return G(A)|H(A)
def G(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def R(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def E(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def P(A):return tuple(map(max,zip(*Y(A))))
def U(A):return tuple(map(min,zip(*Y(A))))
def GQ(A):
	if len(A)==0:return A
	F,G=U(A);H,I=P(A);B,C=min(F,H),min(G,I);D,E=max(F,H),max(G,I);J={(A,C)for A in range(B,D+1)}|{(A,E)for A in range(B,D+1)};K={(B,A)for A in range(C,E+1)}|{(D,A)for A in range(C,E+1)};return frozenset(J|K)
def GK(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in Y(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def GG(A,B,C,D):
	M=E(A)if D else None;N=set();I=set();Q,S=len(A),len(A[0]);T=R(A);U=V if C else G
	for F in T:
		if F in I:continue
		J=A[F[0]][F[1]]
		if J==M:continue
		O={(J,F)};K={F}
		while len(K)>0:
			P=set()
			for H in K:
				L=A[H[0]][H[1]]
				if J==L if B else L!=M:O.add((L,H));I.add(H);P|={(A,B)for(A,B)in U(H)if 0<=A<Q and 0<=B<S}
			K=P-I
		N.add(frozenset(O))
	return frozenset(N)
def Y(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def GM(A):return GH(A),GJ(A)
def GX(A,a,b):return lambda x:A(a(x),b(x))
def GV(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def GE(h,g,f):return lambda x:h(g(f(x)))
def Z(A,B):return GY(S(A,B))
def GU(a,b):return a and b
def L(Aa):return min(Aa,default=0)
def W(a,b):return a>b
def K(a,b):return a==b
def p(I):I=tuple(map(tuple,I));A=GG(I,True,False,True);B=GV(W,2);C=GE(B,L,GM);D=GX(K,Y,GQ);E=GX(GU,C,D);F=Z(A,E);G=GK(I,3,F);return[*map(list,G)]