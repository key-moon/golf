def S(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def K(A):
	if isinstance(A,tuple):return frozenset({B for A in A for B in A})
	return frozenset({A for(A,B)in A})
def Y(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def V(A):return tuple(map(max,zip(*H(A))))
def J(A):return tuple(map(min,zip(*H(A))))
def Z(A,B,C):
	G,I=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in H(C):
		if 0<=E<G and 0<=F<I:D[E][F]=B
	return tuple(tuple(A)for A in D)
def U(A):
	if isinstance(A,tuple):return tuple(A[::-1]for A in A)
	B=J(A)[1]+V(A)[1]
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(C,B-D))for(A,(C,D))in A)
	return frozenset((A,B-C)for(A,C)in A)
def G(A):return frozenset(frozenset((A,(C,E))for(C,D)in enumerate(A)for(E,A)in enumerate(D)if A==B)for B in K(A)-{Y(A)})
def H(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def GG(A,a,b):return lambda x:A(a(x),b(x))
def X(A,B):return lambda x:A(B(x))
def W(A):return max(enumerate(A))[1]
def R(A,B):return type(A)(A for A in A if B(A))
def P(A):return type(A)(B for A in A for B in A)
def E(a,b):return a==b
def L(n):return n//2 if isinstance(n,int)else(n[0]//2,n[1]//2)
def Q(n):return n*2 if isinstance(n,int)else(n[0]*2,n[1]*2)
def M(x):return x
def p(I):I=tuple(map(tuple,I));A=U(I);B=G(A);C=P(B);D=H(C);F=X(Q,L);J=GG(E,M,F);K=X(J,W);N=R(D,K);O=Z(A,3,N);S=U(O);return[*map(list,S)]