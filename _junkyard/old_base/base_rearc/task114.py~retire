def J(A):return max(A for(A,B)in H(A))
def V(A):return min(A for(A,B)in H(A))
def Q(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def H(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def Y(A):return max(A for(B,A)in H(A))
def E(A):return min(A for(B,A)in H(A))
def P(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return Y(A)-E(A)+1
def X(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return J(A)-V(A)+1
def R(A,B):return tuple(tuple(A for B in range(B[1]))for C in range(B[0]))
def U(A,B):
	F,G=len(A),len(A[0]);C=list(list(A)for A in A)
	for(H,(D,E))in B:
		if 0<=D<F and 0<=E<G:C[D][E]=H
	return tuple(tuple(A)for A in C)
def M(A):return frozenset((D,(A,C))for(A,B)in enumerate(A)for(C,D)in enumerate(B))
def K(A,C):
	if len(A)==0:return A
	B,D=C
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(C+B,E+D))for(A,(C,E))in A)
	return frozenset((A+B,C+D)for(A,C)in A)
def S(A):return X(A),P(A)
def G(x):return x+1 if isinstance(x,int)else(x[0]+1,x[1]+1)
def p(I):I=tuple(map(tuple,I));B=S(I);C=G(B);D=G(C);E=R(0,D);F=M(I);A=K(F,(1,1));H=K(A,(0,-1));J=U(E,H);L=K(A,(0,1));N=U(J,L);O=K(A,(-1,0));P=U(N,O);Q=K(A,(1,0));T=U(P,Q);V=U(T,A);return[*map(list,V)]