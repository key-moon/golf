def V(A):return max(A for(A,B)in G(A))
def H(A):return max(A for(B,A)in G(A))
def U(A):
	if isinstance(A,tuple):return frozenset({B for A in A for B in A})
	return frozenset({A for(A,B)in A})
def GV(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def G(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def E(A):return tuple(map(min,zip(*G(A))))
def P(A):
	if isinstance(A,tuple):return tuple(zip(*A))
	B,C=E(A)
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(E-C+B,D-B+C))for(A,(D,E))in A)
	return frozenset((D-C+B,A-B+C)for(A,D)in A)
def GH(A):return next(iter(A))[0]
def R(A):return min(A for(B,A)in G(A))
def J(A):return min(A for(A,B)in G(A))
def Y(A):return frozenset(frozenset((A,(C,E))for(C,D)in enumerate(A)for(E,A)in enumerate(D)if A==B)for B in U(A))
def GY(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return H(A)-R(A)+1
def Z(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return V(A)-J(A)+1
def GJ(A,B):return type(B)(A(B)for B in B)
def GM(A,a,b):return lambda x:A(a(x),b(x))
def Q(A,B):return lambda x:A(x)==B
def W(A,a,b):return a if A else b
def X(A,B):return type(A)(A for A in A if B(A))
def S(a,b):return a or b
def GE(A):return len(A)
def K(a,b):return a>b
def L(A,B):return tuple(A for B in range(B))
def GG(A,B):return tuple(sorted(A,key=B))
def M(x):return x
def p(I):I=tuple(map(tuple,I));C=Y(I);D=Q(Z,1);E=Q(GY,1);F=GM(S,D,E);A=X(C,F);G=Q(Z,1);H=X(A,G);N=GE(H);O=Q(GY,1);T=X(A,O);U=GE(T);B=K(N,U);V=W(B,P,M);a=W(B,J,R);b=GG(A,a);c=GJ(GH,b);d=GE(A);e=L(c,d);f=V(e);return[*map(list,f)]