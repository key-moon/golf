def J(A,B):return type(B)(B for B in B if B!=A)
def M(A):return next(iter(A))
def V(A,B):return tuple(tuple(A for B in range(B[1]))for C in range(B[0]))
def Y(A):
	if isinstance(A,tuple):return frozenset({B for A in A for B in A})
	return frozenset({A for(A,B)in A})
def G(A,B):
	if isinstance(A,tuple):return sum(A.count(B)for A in A)
	return sum(A==B for(A,C)in A)
def H(a,b):return a,b
def E(A,B):return M(J(B,A))
def p(I):I=tuple(map(tuple,I));B=Y(I);A=E(B,0);C=G(I,A);D=H(1,C);F=V(A,D);return[*map(list,F)]