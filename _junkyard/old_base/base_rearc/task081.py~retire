def W(A,B):return type(B)(A(B)for B in B)
def L(A):return type(A)(B for A in A for B in A)
def G(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def J(A):return H(A)|G(A)
def H(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def M(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def E(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def Z(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def V(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def X(A):return tuple(map(max,zip(*V(A))))
def K(A):return tuple(map(lambda ix:{0:max,1:min}[ix[0]](ix[1]),enumerate(zip(*V(A)))))
def U(A):return tuple(map(lambda ix:{0:min,1:max}[ix[0]](ix[1]),enumerate(zip(*V(A)))))
def R(A):return tuple(map(min,zip(*V(A))))
def Q(A):return frozenset({R(A),U(A),K(A),X(A)})
def Y(A,B,C):
	H,I=len(A),len(A[0]);J=E(A);D=list(list(A)for A in A)
	for(F,G)in V(C):
		if 0<=F<H and 0<=G<I:
			if D[F][G]==J:D[F][G]=B
	return tuple(tuple(A)for A in D)
def P(A,B,C,D):
	O=E(A)if D else None;P=set();I=set();S,T=len(A),len(A[0]);U=M(A);V=J if C else H
	for F in U:
		if F in I:continue
		K=A[F[0]][F[1]]
		if K==O:continue
		Q={(K,F)};L={F}
		while len(L)>0:
			R=set()
			for G in L:
				N=A[G[0]][G[1]]
				if K==N if B else N!=O:Q.add((N,G));I.add(G);R|={(A,B)for(A,B)in V(G)if 0<=A<S and 0<=B<T}
			L=R-I
		P.add(frozenset(Q))
	return frozenset(P)
def S(A,B):return L(W(A,B))
def p(I):I=tuple(map(tuple,I));A=P(I,True,False,True);B=S(Q,A);C=Y(I,1,B);return[*map(list,C)]