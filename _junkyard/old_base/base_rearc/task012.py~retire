def GV(A,B):return type(B)(A(B)for B in B)
def GJ(A):return type(A)(B for A in A for B in A)
def GM(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def H(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def V(A):return G(A)|H(A)
def X(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def L(a,b):
	A,B=a;C,D=b;E=min(A,C);F=max(A,C)+1;G=min(B,D);H=max(B,D)+1
	if A==C:return frozenset((A,B)for B in range(G,H))
	elif B==D:return frozenset((A,B)for A in range(E,F))
	elif C-A==D-B:return frozenset((A,B)for(A,B)in zip(range(E,F),range(G,H)))
	elif C-A==B-D:return frozenset((A,B)for(A,B)in zip(range(E,F),range(H-1,G-1,-1)))
	return frozenset()
def GY(A,B):
	F,G=len(A),len(A[0]);C=list(list(A)for A in A)
	for(H,(D,E))in B:
		if 0<=D<F and 0<=E<G:C[D][E]=H
	return tuple(tuple(A)for A in C)
def S(A):
	if isinstance(A,tuple):return A[::-1]
	B=U(A)[0]+K(A)[0]
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(B-C,D))for(A,(C,D))in A)
	return frozenset((B-A,C)for(A,C)in A)
def W(A,B,C,D):
	L=R(A)if D else None;M=set();H=set();P,Q=len(A),len(A[0]);S=X(A);T=V if C else G
	for E in S:
		if E in H:continue
		I=A[E[0]][E[1]]
		if I==L:continue
		N={(I,E)};J={E}
		while len(J)>0:
			O=set()
			for F in J:
				K=A[F[0]][F[1]]
				if I==K if B else K!=L:N.add((K,F));H.add(F);O|={(A,B)for(A,B)in T(F)if 0<=A<P and 0<=B<Q}
			J=O-H
		M.add(frozenset(N))
	return frozenset(M)
def G(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def P(A,B):return frozenset((A,B)for B in J(B))
def J(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def K(A):return tuple(map(max,zip(*J(A))))
def U(A):return tuple(map(min,zip(*J(A))))
def Y(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return min(set(B),key=B.count)
def R(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def GG(A,B):return GJ(GV(A,B))
def GE(A,a,b):return lambda x:A(a(x),b(x))
def GH(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda y:A(B,y)
	elif C==3:return lambda y,z:A(B,y,z)
	else:return lambda y,z,a:A(B,y,z,a)
def Z(A,B):return lambda x:A(B(x))
def M(x):return x-1 if isinstance(x,int)else(x[0]-1,x[1]-1)
def E(x):return x+1 if isinstance(x,int)else(x[0]+1,x[1]+1)
def Q(a,b):return type(a)((*a,*b))
def p(I):I=tuple(map(tuple,I));A=W(I,False,True,True);C=GH(GG,G);D=Z(C,J);F=GE(P,R,D);H=Z(M,U);N=Z(E,K);B=GE(L,H,N);O=Z(S,B);T=GE(Q,B,O);V=GE(P,Y,T);X=GG(F,A);a=GY(I,X);b=GG(V,A);c=GY(a,b);return[*map(list,c)]