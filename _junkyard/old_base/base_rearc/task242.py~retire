def X(A):return tuple(map(max,zip(*H(A))))
def V(A):return max(A for(A,B)in H(A))
def J(A):return min(A for(A,B)in H(A))
def Y(A):return max(A for(B,A)in H(A))
def E(A):return min(A for(B,A)in H(A))
def GH(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return Y(A)-E(A)+1
def Z(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return V(A)-J(A)+1
def GV(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def H(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def GM(A,B,C):return tuple(A[B[1]:B[1]+C[1]]for A in A[B[0]:B[0]+C[0]])
def M(A):return tuple(map(min,zip(*H(A))))
def GJ(A):return Z(A),GH(A)
def S(A,B):return GM(B,M(A),GJ(A))
def P(A):
	if isinstance(A,tuple):return tuple(A[::-1]for A in A)
	B=M(A)[1]+X(A)[1]
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(C,B-D))for(A,(C,D))in A)
	return frozenset((A,B-C)for(A,C)in A)
def U(A):
	if isinstance(A,tuple):return A[::-1]
	B=M(A)[0]+X(A)[0]
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(B-C,D))for(A,(C,D))in A)
	return frozenset((B-A,C)for(A,C)in A)
def R(A,B):D,E=len(B),len(B[0]);return frozenset((B[A][C],(A,C))for(A,C)in H(A)if 0<=A<D and 0<=C<E)
def Q(A,B):return frozenset((A,D)for(A,C)in enumerate(A)for(D,E)in enumerate(C)if E==B)
def G(A,B):
	if isinstance(A,tuple):return sum(A.count(B)for A in A)
	return sum(A==B for(A,C)in A)
def GG(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda y:A(B,y)
	elif C==3:return lambda y,z:A(B,y,z)
	else:return lambda y,z,a:A(B,y,z,a)
def GY(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def L(A,B):return lambda x:A(B(x))
def K(a,b):return a,b
def W(A,B):return min(A,key=B)
def p(I):I=tuple(map(tuple,I));A=Q(I,0);B=GY(G,0);C=GG(R,A);D=L(B,C);E=P(I);F=U(I);H=K(E,F);J=W(H,D);M=S(A,J);return[*map(list,M)]