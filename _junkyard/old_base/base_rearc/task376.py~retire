def Y(A):return max(A for(A,B)in G(A))
def V(A):return min(A for(A,B)in G(A))
def H(A):return max(A for(B,A)in G(A))
def R(A):return min(A for(B,A)in G(A))
def S(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def G(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def E(A):return tuple(map(max,zip(*G(A))))
def M(A):return tuple(map(min,zip(*G(A))))
def K(a,b):return a+b
def X(A):
	if isinstance(A,tuple):return A[::-1]
	B=M(A)[0]+E(A)[0]
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(B-C,D))for(A,(C,D))in A)
	return frozenset((B-A,C)for(A,C)in A)
def W(A,B,C):return tuple(A[B[1]:B[1]+C[1]]for A in A[B[0]:B[0]+C[0]])
def L(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return H(A)-R(A)+1
def P(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return Y(A)-V(A)+1
def U(a,b):return a,b
def J(x):return x-1 if isinstance(x,int)else(x[0]-1,x[1]-1)
def Q(n):return n*2 if isinstance(n,int)else(n[0]*2,n[1]*2)
def p(I):I=tuple(map(tuple,I));D=P(I);A=L(I);B=J(D);E=U(B,A);F=W(I,(0,0),E);G=X(F);C=K(I,G);H=Q(B);M=U(H,A);N=W(C,(1,0),M);O=K(C,N);return[*map(list,O)]