def HG(A,B):return type(B)(A(B)for B in B)
def HH(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def Y(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def E(A):return H(A)|Y(A)
def H(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def K(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def Z(A):return tuple(map(max,zip(*V(A))))
def L(A):return tuple(map(min,zip(*V(A))))
def M(A):return max(A for(B,A)in V(A))
def W(A):return min(A for(B,A)in V(A))
def Q(A):return max(A for(A,B)in V(A))
def R(A):return min(A for(A,B)in V(A))
def GH(a,b):
	A,B=a;C,D=b;E=min(A,C);F=max(A,C)+1;G=min(B,D);H=max(B,D)+1
	if A==C:return frozenset((A,B)for B in range(G,H))
	elif B==D:return frozenset((A,B)for A in range(E,F))
	elif C-A==D-B:return frozenset((A,B)for(A,B)in zip(range(E,F),range(G,H)))
	elif C-A==B-D:return frozenset((A,B)for(A,B)in zip(range(E,F),range(H-1,G-1,-1)))
	return frozenset()
def GU(A,B):return GH(A,(A[0]+42*B[0],A[1]+42*B[1]))
def GR(A):F,G=R(A)-1,W(A)-1;H,I=Q(A)+1,M(A)+1;B,C=min(F,H),min(G,I);D,E=max(F,H),max(G,I);J={(A,C)for A in range(B,D+1)}|{(A,E)for A in range(B,D+1)};K={(B,A)for A in range(C,E+1)}|{(D,A)for A in range(C,E+1)};return frozenset(J|K)
def S(A):
	if len(A)==0:return frozenset({})
	B=V(A);C,D=L(B);E,F=Z(A);return frozenset((A,B)for A in range(C,E+1)for B in range(D,F+1))
def U(A,B,C):
	G,H=len(A),len(A[0]);I=X(A);D=list(list(A)for A in A)
	for(E,F)in V(C):
		if 0<=E<G and 0<=F<H:
			if D[E][F]==I:D[E][F]=B
	return tuple(tuple(A)for A in D)
def HJ(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in V(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def GJ(A,B,C,D):
	N=X(A)if D else None;O=set();I=set();R,S=len(A),len(A[0]);T=K(A);U=E if C else H
	for F in T:
		if F in I:continue
		J=A[F[0]][F[1]]
		if J==N:continue
		P={(J,F)};L={F}
		while len(L)>0:
			Q=set()
			for G in L:
				M=A[G[0]][G[1]]
				if J==M if B else M!=N:P.add((M,G));I.add(G);Q|={(A,B)for(A,B)in U(G)if 0<=A<R and 0<=B<S}
			L=Q-I
		O.add(frozenset(P))
	return frozenset(O)
def V(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def GG(A,B):return frozenset((A,D)for(A,C)in enumerate(A)for(D,E)in enumerate(C)if E==B)
def GS(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return M(A)-W(A)+1
def X(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def GE(A,B):return GK(HG(A,B))
def GM(A,B):return type(A)(A(B)for A in A)
def HY(A,a,b):return lambda x:A(a(x),b(x))
def GP(A,n):
	if n==1:return A
	return GV(A,GP(A,n-1))
def GX(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda y:A(B,y)
	elif C==3:return lambda y,z:A(B,y,z)
	else:return lambda y,z,a:A(B,y,z,a)
def GW(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def GZ(h,g,f):return lambda x:h(g(f(x)))
def GV(A,B):return lambda x:A(B(x))
def GQ(A):return next(iter(A))
def GY(A):return frozenset({A})
def GK(A):return type(A)(B for A in A for B in A)
def J(a,b):return type(a)(A for A in a if A not in b)
def G(a,b):return a&b
def GL(n):return n//2 if isinstance(n,int)else(n[0]//2,n[1]//2)
def P(x):return x
def p(I):A=True;I=tuple(map(tuple,I));B=GJ(I,A,A,A);C=GK(B);D=V(C);E=GW(GU,(1,0));F=GE(E,D);H=U(I,1,F);K=GX(GP,GR);L=GZ(K,GL,GS);M=GY(L);N=GX(GM,M);O=HY(GM,N,P);Q=GV(GQ,O);R=GV(S,Q);T=HY(J,R,V);W=GE(T,B);Y=X(I);Z=GG(I,Y);a=G(W,Z);b=HJ(H,3,a);return[*map(list,b)]