def W(A,B):return type(B)(A(B)for B in B)
def L(A):return type(A)(B for A in A for B in A)
def G(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def V(A):return H(A)|G(A)
def H(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def E(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def U(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def Z(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def M(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def R(A):return max(A for(B,A)in M(A))
def K(A):return min(A for(B,A)in M(A))
def X(A):return max(A for(A,B)in M(A))
def J(A):return min(A for(A,B)in M(A))
def P(A):F,G=J(A)-1,K(A)-1;H,I=X(A)+1,R(A)+1;B,C=min(F,H),min(G,I);D,E=max(F,H),max(G,I);L={(A,C)for A in range(B,D+1)}|{(A,E)for A in range(B,D+1)};M={(B,A)for A in range(C,E+1)}|{(D,A)for A in range(C,E+1)};return frozenset(L|M)
def GG(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in M(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def Q(A,B,C,D):
	M=U(A)if D else None;N=set();I=set();Q,R=len(A),len(A[0]);S=E(A);T=V if C else H
	for F in S:
		if F in I:continue
		J=A[F[0]][F[1]]
		if J==M:continue
		O={(J,F)};K={F}
		while len(K)>0:
			P=set()
			for G in K:
				L=A[G[0]][G[1]]
				if J==L if B else L!=M:O.add((L,G));I.add(G);P|={(A,B)for(A,B)in T(G)if 0<=A<Q and 0<=B<R}
			K=P-I
		N.add(frozenset(O))
	return frozenset(N)
def Y(A,n):return frozenset(A for A in A if len(A)==n)
def S(A,B):return L(W(A,B))
def p(I):I=tuple(map(tuple,I));A=Q(I,True,False,True);B=Y(A,2);C=S(P,B);D=GG(I,3,C);return[*map(list,D)]