def GH(A):return type(A)(B for A in A for B in A)
def H(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def J(A):return G(A)|H(A)
def G(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def M(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def V(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def GM(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def GG(A,B):
	F,G=len(A),len(A[0]);C=list(list(A)for A in A)
	for(H,(D,E))in B:
		if 0<=D<F and 0<=E<G:C[D][E]=H
	return tuple(tuple(A)for A in C)
def S(A,B,C,D):
	N=V(A)if D else None;O=set();H=set();R,S=len(A),len(A[0]);T=M(A);U=J if C else G
	for E in T:
		if E in H:continue
		I=A[E[0]][E[1]]
		if I==N:continue
		P={(I,E)};K={E}
		while len(K)>0:
			Q=set()
			for F in K:
				L=A[F[0]][F[1]]
				if I==L if B else L!=N:P.add((L,F));H.add(F);Q|={(A,B)for(A,B)in U(F)if 0<=A<R and 0<=B<S}
			K=Q-H
		O.add(frozenset(P))
	return frozenset(O)
def GJ(A,C):
	if len(A)==0:return A
	B,D=C
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(C+B,E+D))for(A,(C,E))in A)
	return frozenset((A+B,C+D)for(A,C)in A)
def Q(A,B):return frozenset((A,B)for B in Y(B))
def Y(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def X(A):return tuple(map(max,zip(*Y(A))))
def U(A):return tuple(map(lambda ix:{0:max,1:min}[ix[0]](ix[1]),enumerate(zip(*Y(A)))))
def E(A):return tuple(map(lambda ix:{0:min,1:max}[ix[0]](ix[1]),enumerate(zip(*Y(A)))))
def R(A):return tuple(map(min,zip(*Y(A))))
def W(A,B):return GH(GV(A,B))
def GV(A,B):return type(B)(A(B)for B in B)
def GE(A,a,b):return lambda x:A(a(x),b(x))
def Z(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda y:A(B,y)
	elif C==3:return lambda y,z:A(B,y,z)
	else:return lambda y,z,a:A(B,y,z,a)
def GY(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def L(A,B):return lambda x:A(B(x))
def P(a,b):return a,b
def K(a,b):return type(a)((*a,*b))
def p(I):B=False;I=tuple(map(tuple,I));C=S(I,B,B,True);D=GV(Y,C);A=Z(GM,I);F=L(A,X);G=P(-2,-2);H=GY(GJ,G);J=GE(Q,F,H);M=L(A,R);N=GY(GJ,(2,2));O=GE(Q,M,N);T=L(A,U);V=P(-2,2);a=GY(GJ,V);b=GE(Q,T,a);c=L(A,E);d=P(2,-2);e=GY(GJ,d);f=GE(Q,c,e);g=GE(K,J,O);h=GE(K,b,f);i=GE(K,g,h);j=W(i,D);k=GG(I,j);return[*map(list,k)]