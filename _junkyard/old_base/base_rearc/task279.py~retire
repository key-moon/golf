def W(A,B):return type(A)(A for A in A if B(A))
def GY(A):return type(A)(B for A in A for B in A)
def GM(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def Y(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def R(A):return H(A)|Y(A)
def H(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def Q(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def X(a,b):return min(abs(A-C)+abs(B-D)for(A,B)in V(a)for(C,D)in V(b))
def M(A):return max(A for(B,A)in V(A))
def L(A):return min(A for(B,A)in V(A))
def P(A):return max(A for(A,B)in V(A))
def U(A):return min(A for(A,B)in V(A))
def GR(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in V(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def E(A,B):return U(A)==0 or L(A)==0 or P(A)==len(B)-1 or M(A)==len(B[0])-1
def S(a,b):return X(a,b)==1
def GG(A,B,C,D):
	M=K(A)if D else None;N=set();G=set();S,T=len(A),len(A[0]);U=Q(A);V=R if C else H
	for E in U:
		if E in G:continue
		I=A[E[0]][E[1]]
		if I==M:continue
		O={(I,E)};J={E}
		while len(J)>0:
			P=set()
			for F in J:
				L=A[F[0]][F[1]]
				if I==L if B else L!=M:O.add((L,F));G.add(F);P|={(A,B)for(A,B)in V(F)if 0<=A<S and 0<=B<T}
			J=P-G
		N.add(frozenset(O))
	return frozenset(N)
def V(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def G(A,B):return frozenset(A for A in A if next(iter(A))[0]==B)
def K(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def GV(A,B):return type(B)(A(B)for B in B)
def GJ(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def GH(A,B):return lambda x:A(B(x))
def Z(A,B):return GY(W(A,B))
def J(a,b):return type(a)(A for A in a if A not in b)
def GE(b):return not b
def p(I):C=False;I=tuple(map(tuple,I));A=GG(I,True,C,C);D=K(I);B=G(A,D);F=GJ(E,I);H=GH(GE,F);L=Z(B,H);M=J(A,B);N=GV(V,M);O=GJ(S,L);P=Z(N,O);Q=GR(I,8,P);return[*map(list,Q)]