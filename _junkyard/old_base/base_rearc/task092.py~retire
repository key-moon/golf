def U(A,B):return type(A)(A for A in A if B(A))
def Z(A):return type(A)(B for A in A for B in A)
def Q(A):
	if isinstance(A,tuple):return frozenset({B for A in A for B in A})
	return frozenset({A for(A,B)in A})
def V(A):return max(A for(A,B)in G(A))
def J(A):return min(A for(A,B)in G(A))
def H(A):return max(A for(B,A)in G(A))
def R(A):return min(A for(B,A)in G(A))
def GG(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return H(A)-R(A)+1
def S(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return V(A)-J(A)+1
def GV(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def G(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def X(A):return tuple(map(max,zip(*G(A))))
def E(A):return tuple(map(min,zip(*G(A))))
def M(A):
	if len(A)==0:return frozenset({})
	B=G(A);C,D=E(B);F,H=X(A);return frozenset((A,B)for A in range(C,F+1)for B in range(D,H+1))
def L(A,B):
	F,G=len(A),len(A[0]);C=list(list(A)for A in A)
	for(H,(D,E))in B:
		if 0<=D<F and 0<=E<G:C[D][E]=H
	return tuple(tuple(A)for A in C)
def W(A):return next(iter(A))[0]
def GH(A):return GG(A)==len(A)and S(A)==1
def GJ(A):return S(A)==len(A)and GG(A)==1
def Y(A):return frozenset(frozenset((A,(C,E))for(C,D)in enumerate(A)for(E,A)in enumerate(D)if A==B)for B in Q(A))
def K(A,B):return frozenset((A,B)for B in G(B))
def GY(A,B):return type(B)(A(B)for B in B)
def GM(A,a,b):return lambda x:A(a(x),b(x))
def P(A,B):return Z(U(A,B))
def p(I):I=tuple(map(tuple,I));B=Y(I);C=GM(K,W,M);A=GY(C,B);D=P(A,GH);E=P(A,GJ);F=L(I,D);G=L(F,E);return[*map(list,G)]