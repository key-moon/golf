def P(A,B):return type(B)(A(B)for B in B)
def Q(A):return type(A)(B for A in A for B in A)
def S(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def G(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def J(A):return H(A)|G(A)
def H(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def M(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def X(A):return tuple(map(max,zip(*V(A))))
def R(A):return tuple(map(min,zip(*V(A))))
def Z(A):
	if len(A)==0:return A
	F,G=R(A);H,I=X(A);B,C=min(F,H),min(G,I);D,E=max(F,H),max(G,I);J={(A,C)for A in range(B,D+1)}|{(A,E)for A in range(B,D+1)};K={(B,A)for A in range(C,E+1)}|{(D,A)for A in range(C,E+1)};return frozenset(J|K)
def L(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in V(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def U(A,B,C,D):
	O=E(A)if D else None;P=set();I=set();S,T=len(A),len(A[0]);U=M(A);V=J if C else H
	for F in U:
		if F in I:continue
		K=A[F[0]][F[1]]
		if K==O:continue
		Q={(K,F)};L={F}
		while len(L)>0:
			R=set()
			for G in L:
				N=A[G[0]][G[1]]
				if K==N if B else N!=O:Q.add((N,G));I.add(G);R|={(A,B)for(A,B)in V(G)if 0<=A<S and 0<=B<T}
			L=R-I
		P.add(frozenset(Q))
	return frozenset(P)
def V(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def E(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def K(A,B):return Q(P(A,B))
def W(A,a,b):return lambda x:A(a(x),b(x))
def Y(a,b):return type(a)(A for A in a if A not in b)
def p(I):I=tuple(map(tuple,I));A=U(I,True,False,True);B=W(Y,V,Z);C=K(B,A);D=E(I);F=L(I,D,C);return[*map(list,F)]