def L(A):return next(iter(A))
def V(A):return max(A for(A,B)in G(A))
def H(A):return max(A for(B,A)in G(A))
def E(A):return min(A for(B,A)in G(A))
def Y(A):return min(A for(A,B)in G(A))
def W(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return H(A)-E(A)+1
def Q(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return V(A)-Y(A)+1
def GH(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def GJ(A,B,C):
	H,I=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in G(C):
		if 0<=E<H and 0<=F<I:D[E][F]=B
	return tuple(tuple(A)for A in D)
def G(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def GY(A,B):return GJ(A,J(A),G(B))
def S(A,B):
	F,G=len(A),len(A[0]);C=list(list(A)for A in A)
	for(H,(D,E))in B:
		if 0<=D<F and 0<=E<G:C[D][E]=H
	return tuple(tuple(A)for A in C)
def Z(A,C):
	if len(A)==0:return A
	B,D=C
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(C+B,E+D))for(A,(C,E))in A)
	return frozenset((A+B,C+D)for(A,C)in A)
def GM(A,B,C):return S(GY(A,B),Z(B,C))
def P(A):return Y(A)+Q(A)//2,E(A)+W(A)//2
def R(A):
	if isinstance(A,tuple):return frozenset({B for A in A for B in A})
	return frozenset({A for(A,B)in A})
def X(A,B):return frozenset((A,B)for B in G(B))
def U(A,B):return frozenset((A,D)for(A,C)in enumerate(A)for(D,E)in enumerate(C)if E==B)
def J(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def GG(A,B):return L(K(B,A))
def K(A,B):return type(B)(B for B in B if B!=A)
def GV(x):
	if isinstance(x,int):return 0 if x==0 else 1 if x>0 else-1
	return 0 if x[0]==0 else 1 if x[0]>0 else-1,0 if x[1]==0 else 1 if x[1]>0 else-1
def M(a,b):
	if isinstance(a,int)and isinstance(b,int):return a-b
	elif isinstance(a,tuple)and isinstance(b,tuple):return a[0]-b[0],a[1]-b[1]
	elif isinstance(a,int)and isinstance(b,tuple):return a-b[0],a-b[1]
	return a[0]-b,a[1]-b
def p(I):I=tuple(map(tuple,I));C=J(I);D=R(I);E=K(C,D);A=GG(E,4);B=U(I,A);F=U(I,4);G=P(B);H=P(F);L=M(H,G);N=GV(L);O=X(A,B);Q=GM(I,O,N);return[*map(list,Q)]