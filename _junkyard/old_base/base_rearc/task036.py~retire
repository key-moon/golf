def H(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def M(A):return G(A)|H(A)
def G(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def X(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def R(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def U(A):return max(A for(A,B)in J(A))
def E(A):return min(A for(A,B)in J(A))
def V(A):return max(A for(B,A)in J(A))
def P(A):return min(A for(B,A)in J(A))
def GE(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return V(A)-P(A)+1
def GJ(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return U(A)-E(A)+1
def GK(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def J(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def GP(A,B,C):return tuple(A[B[1]:B[1]+C[1]]for A in A[B[0]:B[0]+C[0]])
def Q(A):return tuple(map(min,zip(*J(A))))
def GR(A):return GJ(A),GE(A)
def W(A,B):return GP(B,Q(A),GR(A))
def GM(A):return next(iter(A))[0]
def GG(A,B,C,D):
	L=R(A)if D else None;N=set();H=set();Q,S=len(A),len(A[0]);T=X(A);U=M if C else G
	for E in T:
		if E in H:continue
		I=A[E[0]][E[1]]
		if I==L:continue
		O={(I,E)};J={E}
		while len(J)>0:
			P=set()
			for F in J:
				K=A[F[0]][F[1]]
				if I==K if B else K!=L:O.add((K,F));H.add(F);P|={(A,B)for(A,B)in U(F)if 0<=A<Q and 0<=B<S}
			J=P-H
		N.add(frozenset(O))
	return frozenset(N)
def Y(A,B):
	if isinstance(A,tuple):return sum(A.count(B)for A in A)
	return sum(A==B for(A,C)in A)
def GU(A,B):return type(B)(A(B)for B in B)
def GV(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda y:A(B,y)
	elif C==3:return lambda y,z:A(B,y,z)
	else:return lambda y,z,a:A(B,y,z,a)
def L(A,B):return lambda x:A(x)==B
def GX(h,g,f):return lambda x:h(g(f(x)))
def Z(A):return tuple(A)
def GH(A,B):return next(A for A in A if B(A))
def S(A,B):return type(A)(A for A in A if B(A))
def GY(A,B):return max(A,key=B)
def GQ(A):return len(A)
def K(x):return x
def p(I):I=tuple(map(tuple,I));A=GG(I,True,False,True);C=Z(A);B=GU(GM,C);D=GV(S,B);E=GV(L,K);F=GX(GQ,D,E);G=L(F,1);H=S(B,G);J=GV(Y,I);M=GY(H,J);N=L(GM,M);O=GH(A,N);P=W(O,I);return[*map(list,P)]