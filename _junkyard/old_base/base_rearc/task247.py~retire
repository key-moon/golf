def Y(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def V(A):return G(A)|Y(A)
def G(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def E(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def M(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def GY(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def J(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def R(A):return tuple(map(min,zip(*J(A))))
def P(A,B):return tuple(tuple(A for B in range(B[1]))for C in range(B[0]))
def K(A):
	if isinstance(A,tuple):return tuple(zip(*A))
	B,C=R(A)
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(E-C+B,D-B+C))for(A,(D,E))in A)
	return frozenset((D-C+B,A-B+C)for(A,D)in A)
def W(A):return next(iter(A))[0]
def X(A):return min(A for(B,A)in J(A))
def Q(A,B,C,D):
	N=M(A)if D else None;O=set();I=set();R,S=len(A),len(A[0]);T=E(A);U=V if C else G
	for F in T:
		if F in I:continue
		J=A[F[0]][F[1]]
		if J==N:continue
		P={(J,F)};K={F}
		while len(K)>0:
			Q=set()
			for H in K:
				L=A[H[0]][H[1]]
				if J==L if B else L!=N:P.add((L,H));I.add(H);Q|={(A,B)for(A,B)in U(H)if 0<=A<R and 0<=B<S}
			K=Q-I
		O.add(frozenset(P))
	return frozenset(O)
def H(A,n):return frozenset(A for A in A if len(A)==n)
def GH(A,B):return type(B)(A(B)for B in B)
def GG(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def U(a,b):return a,b
def S(A,B):return B(max(A,key=B,default=0))
def Z(A):return type(A)(B for A in A for B in A)
def GJ(A):return len(A)
def L(A,B):return tuple(sorted(A,key=B))
def p(I):I=tuple(map(tuple,I));A=Q(I,True,False,True);B=S(A,GJ);C=H(A,B);D=L(C,X);E=GH(W,D);F=U(1,B);G=GG(P,F);J=GH(G,E);M=Z(J);N=K(M);return[*map(list,N)]