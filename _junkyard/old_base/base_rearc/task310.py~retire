def M(A):return max(A for(A,B)in G(A))
def V(A):return min(A for(A,B)in G(A))
def H(A):return max(A for(B,A)in G(A))
def U(A):return min(A for(B,A)in G(A))
def GJ(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def P(A):
	if isinstance(A,tuple):return frozenset({B for A in A for B in A})
	return frozenset({A for(A,B)in A})
def GE(A,B,C):return tuple(A[B[1]:B[1]+C[1]]for A in A[B[0]:B[0]+C[0]])
def K(A):return tuple(map(max,zip(*G(A))))
def R(A):return tuple(map(min,zip(*G(A))))
def GR(A):
	if len(A)==0:return A
	F,G=R(A);H,I=K(A);B,C=min(F,H),min(G,I);D,E=max(F,H),max(G,I);J={(A,C)for A in range(B,D+1)}|{(A,E)for A in range(B,D+1)};L={(B,A)for A in range(C,E+1)}|{(D,A)for A in range(C,E+1)};return frozenset(J|L)
def S(A,B):return GE(B,R(A),GH(A))
def J(A):return frozenset(frozenset((A,(C,E))for(C,D)in enumerate(A)for(E,A)in enumerate(D)if A==B)for B in P(A))
def G(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def GH(A):return W(A),GG(A)
def GG(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return H(A)-U(A)+1
def W(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return M(A)-V(A)+1
def GV(A,a,b):return lambda x:A(a(x),b(x))
def Z(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda y:A(B,y)
	elif C==3:return lambda y,z:A(B,y,z)
	else:return lambda y,z,a:A(B,y,z,a)
def GY(h,g,f):return lambda x:h(g(f(x)))
def Q(A,B):return type(A)(A for A in A if B(A))
def L(A,B):return min(A,key=B)
def Y(A,B):return A in B
def X(a,b):return a==b
def GM(b):return not b
def E(a,b):
	if isinstance(a,int)and isinstance(b,int):return a*b
	elif isinstance(a,tuple)and isinstance(b,tuple):return a[0]*b[0],a[1]*b[1]
	elif isinstance(a,int)and isinstance(b,tuple):return a*b[0],a*b[1]
	return a[0]*b,a[1]*b
def p(I):I=tuple(map(tuple,I));A=J(I);B=Z(Y,1);C=GY(GM,B,GH);D=Q(A,C);F=GV(X,G,GR);H=Q(D,F);K=GV(E,W,GG);M=L(H,K);N=S(M,I);return[*map(list,N)]