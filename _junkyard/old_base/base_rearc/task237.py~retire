def GL(A,B):return type(B)(A(B)for B in B)
def GK(A):return type(A)(B for A in A for B in A)
def H(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def M(A):return G(A)|H(A)
def G(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def X(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def U(A):return max(A for(A,B)in J(A))
def V(A):return max(A for(B,A)in J(A))
def GW(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def J(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def K(A):return min(A for(B,A)in J(A))
def GQ(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return V(A)-K(A)+1
def GJ(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return U(A)-E(A)+1
def GR(A,B):return S(A,(A[0]+42*B[0],A[1]+42*B[1]))
def S(a,b):
	A,B=a;C,D=b;E=min(A,C);F=max(A,C)+1;G=min(B,D);H=max(B,D)+1
	if A==C:return frozenset((A,B)for B in range(G,H))
	elif B==D:return frozenset((A,B)for A in range(E,F))
	elif C-A==D-B:return frozenset((A,B)for(A,B)in zip(range(E,F),range(G,H)))
	elif C-A==B-D:return frozenset((A,B)for(A,B)in zip(range(E,F),range(H-1,G-1,-1)))
	return frozenset()
def GV(A):return E(A)+GJ(A)//2,K(A)+GQ(A)//2
def Y(A,B):
	F,G=len(A),len(A[0]);H=R(A);C=list(list(A)for A in A)
	for(I,(D,E))in B:
		if 0<=D<F and 0<=E<G:
			if C[D][E]==H:C[D][E]=I
	return tuple(tuple(A)for A in C)
def GM(A,B):
	F,G=len(A),len(A[0]);C=list(list(A)for A in A)
	for(H,(D,E))in B:
		if 0<=D<F and 0<=E<G:C[D][E]=H
	return tuple(tuple(A)for A in C)
def GX(A):return next(iter(A))[0]
def E(A):return min(A for(A,B)in J(A))
def W(A,B,C,D):
	L=R(A)if D else None;N=set();H=set();Q,S=len(A),len(A[0]);T=X(A);U=M if C else G
	for E in T:
		if E in H:continue
		I=A[E[0]][E[1]]
		if I==L:continue
		O={(I,E)};J={E}
		while len(J)>0:
			P=set()
			for F in J:
				K=A[F[0]][F[1]]
				if I==K if B else K!=L:O.add((K,F));H.add(F);P|={(A,B)for(A,B)in U(F)if 0<=A<Q and 0<=B<S}
			J=P-H
		N.add(frozenset(O))
	return frozenset(N)
def P(A,B):return frozenset((A,B)for B in J(B))
def Q(A):return tuple(map(max,zip(*J(A))))
def GS(A):return GJ(A),GQ(A)
def R(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def GG(A,B):return GK(GL(A,B))
def GZ(A,a,b):return lambda x:A(a(x),b(x))
def GP(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def Z(A,B):return lambda x:A(B(x))
def HH(a,b):return tuple(zip(a,b))
def GH(A,B):return type(B)(B for B in B if B!=A)
def GY(A,B):return B.union(frozenset({A}))
def HG(A):return max(enumerate(A))[1]
def GU(A):return next(iter(A))
def L(A):return frozenset({A})
def GE(A,B):return tuple(sorted(A,key=B))
def HY(a,b):
	if isinstance(a,int)and isinstance(b,int):return a+b
	elif isinstance(a,tuple)and isinstance(b,tuple):return a[0]+b[0],a[1]+b[1]
	elif isinstance(a,int)and isinstance(b,tuple):return a+b[0],a+b[1]
	return a[0]+b,a[1]+b
def p(I):F=False;A=True;I=tuple(map(tuple,I));G=GS(I);H=W(I,A,F,A);J=GP(GR,(0,1));K=Z(J,GV);M=GZ(P,GX,K);N=GG(M,H);C=GM(I,N);O=HY(G,(1,-1));T=L(O);U=R(I);D=P(U,T);V=W(C,A,F,A);X=GY(D,V);B=GE(X,E);a=GU(B);b=GH(D,B);c=GH(a,B);d=Z(Q,GU);e=Z(Q,HG);f=GZ(S,d,e);g=Z(GX,GU);h=GZ(P,g,f);i=HH(b,c);j=GG(h,i);k=Y(C,j);return[*map(list,k)]