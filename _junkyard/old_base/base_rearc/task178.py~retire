def H(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def J(A):return G(A)|H(A)
def G(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def M(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def V(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def GY(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def Y(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def R(A):return tuple(map(min,zip(*Y(A))))
def K(A):
	if isinstance(A,tuple):return tuple(zip(*A))
	B,C=R(A)
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(E-C+B,D-B+C))for(A,(D,E))in A)
	return frozenset((D-C+B,A-B+C)for(A,D)in A)
def Z(A):return next(iter(A))[0]
def X(A):return min(A for(B,A)in Y(A))
def Q(A,B,C,D):
	N=V(A)if D else None;O=set();H=set();R,S=len(A),len(A[0]);T=M(A);U=J if C else G
	for E in T:
		if E in H:continue
		I=A[E[0]][E[1]]
		if I==N:continue
		P={(I,E)};K={E}
		while len(K)>0:
			Q=set()
			for F in K:
				L=A[F[0]][F[1]]
				if I==L if B else L!=N:P.add((L,F));H.add(F);Q|={(A,B)for(A,B)in U(F)if 0<=A<R and 0<=B<S}
			K=Q-H
		O.add(frozenset(P))
	return frozenset(O)
def GH(A,B):return type(B)(A(B)for B in B)
def S(A,a,b):return a if A else b
def GG(A):return next(iter(A))
def GJ(A):return len(A)
def P(A,B):return tuple(A for B in range(B))
def W(A,B):return tuple(sorted(A,key=B))
def L(A):return tuple(B for(C,B)in enumerate(A)if A.index(B)==C)
def U(a,b):return a==b
def E(x):return x
def p(I):B=False;I=tuple(map(tuple,I));C=GG(I);D=L(C);F=GJ(D);G=U(1,F);A=S(G,K,E);H=A(I);J=Q(H,True,B,B);M=W(J,X);N=GH(Z,M);O=P(N,1);R=A(O);return[*map(list,R)]