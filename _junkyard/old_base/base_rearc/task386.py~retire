def V(A):return max(A for(A,B)in H(A))
def J(A):return min(A for(A,B)in H(A))
def Y(A):return max(A for(B,A)in H(A))
def X(A):return min(A for(B,A)in H(A))
def GY(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return Y(A)-X(A)+1
def GG(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return V(A)-J(A)+1
def GU(A,B,C):return tuple(A[B[1]:B[1]+C[1]]for A in A[B[0]:B[0]+C[0]])
def GM(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def H(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def R(A):return tuple(map(min,zip(*H(A))))
def P(A):
	if isinstance(A,tuple):return tuple(zip(*A))
	B,C=R(A)
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(E-C+B,D-B+C))for(A,(D,E))in A)
	return frozenset((D-C+B,A-B+C)for(A,D)in A)
def M(A):C,D=len(A),len(A[0]);B=tuple(A for(A,B)in enumerate(A)if len(set(B))==1);E=tuple(A for(A,B)in enumerate(P(A))if len(set(B))==1);F=frozenset({frozenset({(A[B][C],(B,C))for C in range(D)})for B in B});G=frozenset({frozenset({(A[C][B],(C,B))for C in range(C)})for B in E});return F|G
def L(A,B):return tuple(tuple(A for B in range(B[1]))for C in range(B[0]))
def S(A,n):B,D=len(A)//n,len(A[0]);E=len(A)%n!=0;return tuple(GU(A,(B*C+C*E,0),(B,D))for C in range(n))
def Z(A,n):D,B=len(A),len(A[0])//n;E=len(A[0])%n!=0;return tuple(GU(A,(0,B*C+C*E),(D,B))for C in range(n))
def GR(A,B,C):
	G,I=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in H(C):
		if 0<=E<G and 0<=F<I:D[E][F]=B
	return tuple(tuple(A)for A in D)
def K(A):
	if isinstance(A,tuple):return frozenset({B for A in A for B in A})
	return frozenset({A for(A,B)in A})
def GJ(A):return GY(A)==len(A)and GG(A)==1
def Q(A,B):return frozenset((A,D)for(A,C)in enumerate(A)for(D,E)in enumerate(C)if E==B)
def GV(A):return GG(A),GY(A)
def W(A,a,b):return a if A else b
def GE(A):return max(enumerate(A))[1]
def GH(A):return next(iter(A))
def U(A,B):return type(A)(A for A in A if B(A))
def E(x):return x>0
def GX(A):return len(A)
def G(a,b):return a&b
def p(I):I=tuple(map(tuple,I));F=M(I);H=U(F,GJ);J=GX(H);N=E(J);O=W(N,S,Z);C=O(I,2);A=GH(C);D=GE(C);P=K(A);R=K(D);T=G(P,R);B=GH(T);V=GV(A);X=L(B,V);Y=Q(A,B);a=Q(D,B);b=G(Y,a);c=GR(X,3,b);return[*map(list,c)]