def G(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def Y(A):return H(A)|G(A)
def H(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def V(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def M(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def Z(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def J(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def R(A):return tuple(map(max,zip(*J(A))))
def E(A):return tuple(map(min,zip(*J(A))))
def K(A,B):return tuple(tuple(A for B in range(B[1]))for C in range(B[0]))
def X(A):
	if isinstance(A,tuple):return A[::-1]
	B=E(A)[0]+R(A)[0]
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(B-C,D))for(A,(C,D))in A)
	return frozenset((B-A,C)for(A,C)in A)
def P(A):return next(iter(A))[0]
def U(A,B,C,D):
	L=M(A)if D else None;N=set();G=set();Q,R=len(A),len(A[0]);S=V(A);T=Y if C else H
	for E in S:
		if E in G:continue
		I=A[E[0]][E[1]]
		if I==L:continue
		O={(I,E)};J={E}
		while len(J)>0:
			P=set()
			for F in J:
				K=A[F[0]][F[1]]
				if I==K if B else K!=L:O.add((K,F));G.add(F);P|={(A,B)for(A,B)in T(F)if 0<=A<Q and 0<=B<R}
			J=P-G
		N.add(frozenset(O))
	return frozenset(N)
def W(A,B):return type(B)(A(B)for B in B)
def L(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def Q(A):return type(A)(B for A in A for B in A)
def GG(A):return len(A)
def S(A,B):return tuple(sorted(A,key=B))
def p(I):A=True;I=tuple(map(tuple,I));B=U(I,A,A,A);C=S(B,GG);D=W(P,C);E=L(K,(1,1));F=W(E,D);G=Q(F);H=X(G);return[*map(list,H)]