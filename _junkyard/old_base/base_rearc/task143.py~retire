def HG(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def GW(A,C):
	if len(A)==0:return A
	B,D=C
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(C+B,E+D))for(A,(C,E))in A)
	return frozenset((A+B,C+D)for(A,C)in A)
def H(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def M(A):return G(A)|H(A)
def G(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def K(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def J(A):return max(A for(B,A)in Y(A))
def GG(A):return min(A for(B,A)in Y(A))
def Q(A):return max(A for(A,B)in Y(A))
def R(A):return min(A for(A,B)in Y(A))
def GJ(A):
	if isinstance(A,tuple):return frozenset({B for A in A for B in A})
	return frozenset({A for(A,B)in A})
def GH(A):return tuple(map(max,zip(*Y(A))))
def L(A):return tuple(map(min,zip(*Y(A))))
def S(A):
	if len(A)==0:return frozenset({})
	B=Y(A);C,D=L(B);E,F=GH(A);return frozenset((A,B)for A in range(C,E+1)for B in range(D,F+1))
def GP(B):
	if len(B)==0:return frozenset({})
	return S(B)-Y(B)
def HV(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in Y(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def W(A,B):D,E=len(B),len(B[0]);return frozenset((B[A][C],(A,C))for(A,C)in Y(A)if 0<=A<D and 0<=C<E)
def GK(A):return next(iter(A))[0]
def P(A):return len(GJ(A))
def V(A,B):return R(A)==0 or GG(A)==0 or Q(A)==len(B)-1 or J(A)==len(B[0])-1
def GM(A,B,C,D):
	N=U(A)if D else None;O=set();H=set();R,S=len(A),len(A[0]);T=K(A);V=M if C else G
	for E in T:
		if E in H:continue
		I=A[E[0]][E[1]]
		if I==N:continue
		P={(I,E)};J={E}
		while len(J)>0:
			Q=set()
			for F in J:
				L=A[F[0]][F[1]]
				if I==L if B else L!=N:P.add((L,F));H.add(F);Q|={(A,B)for(A,B)in V(F)if 0<=A<R and 0<=B<S}
			J=Q-H
		O.add(frozenset(P))
	return frozenset(O)
def X(A):
	if len(A)==0:return A
	return GW(A,(-R(A),-GG(A)))
def Y(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def GS(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return J(A)-GG(A)+1
def GU(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return Q(A)-R(A)+1
def U(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def HH(A,a,b):return lambda x:A(a(x),b(x))
def GL(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def GV(A,B):return lambda x:A(x)==B
def GZ(h,g,f):return lambda x:h(g(f(x)))
def GE(A,B):return lambda x:A(B(x))
def GX(A,B):return type(B)(B for B in B if B!=A)
def GQ(A):return next(iter(A))
def GY(A,B):return type(A)(A for A in A if B(A))
def E(x):return x-1 if isinstance(x,int)else(x[0]-1,x[1]-1)
def HY(a,b):return a and b
def GR(A,B):return max(A,key=B)
def HM(A):return len(A)
def Z(a,b):return a==b
def HJ(b):return not b
def HE(a,b):
	if isinstance(a,int)and isinstance(b,int):return a+b
	elif isinstance(a,tuple)and isinstance(b,tuple):return a[0]+b[0],a[1]+b[1]
	elif isinstance(a,int)and isinstance(b,tuple):return a+b[0],a+b[1]
	return a[0]+b,a[1]+b
def p(I):I=tuple(map(tuple,I));A=GM(I,True,False,True);D=U(I);F=HH(HE,GU,GS);G=GE(E,F);H=HH(Z,G,HM);J=GL(V,I);K=HH(HY,H,J);L=GL(W,I);M=GZ(P,L,GP);N=GV(M,2);O=HH(HY,K,N);Q=GY(A,O);B=GR(Q,HM);R=GP(B);S=W(R,I);T=GV(GQ,D);a=GE(HJ,T);C=GY(S,a);b=X(C);c=Y(b);d=GE(Y,X);e=GV(d,c);f=GX(C,A);g=GR(f,e);h=GK(B);i=HV(I,h,g);return[*map(list,i)]