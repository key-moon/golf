def J(A):return max(A for(A,B)in G(A))
def Y(A):return min(A for(A,B)in G(A))
def H(A):return max(A for(B,A)in G(A))
def E(A):return min(A for(B,A)in G(A))
def Z(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def G(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def M(A):return tuple(map(min,zip(*G(A))))
def U(A):
	if isinstance(A,tuple):return tuple(zip(*A))
	B,C=M(A)
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(E-C+B,D-B+C))for(A,(D,E))in A)
	return frozenset((D-C+B,A-B+C)for(A,D)in A)
def GH(A,B,C):return tuple(A[B[1]:B[1]+C[1]]for A in A[B[0]:B[0]+C[0]])
def L(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return H(A)-E(A)+1
def P(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return J(A)-Y(A)+1
def W(A,B):return type(B)(A(B)for B in B)
def K(A,a,b):return a if A else b
def X(a,b):return a,b
def S(A):return next(iter(A))
def GG(A):return len(A)
def Q(A):return tuple(B for(C,B)in enumerate(A)if A.index(B)==C)
def R(a,b):return a==b
def V(x):return x
def p(I):I=tuple(map(tuple,I));C=S(I);D=Q(C);E=GG(D);A=R(E,1);B=K(A,U,V);F=K(A,P,L);G=F(I);H=X(1,G);J=B(I);M=GH(J,(0,0),H);N=W(Q,M);O=B(N);return[*map(list,O)]