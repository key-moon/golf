def Q(A,B):return type(B)(B for B in B if B!=A)
def S(A):return next(iter(A))
def H(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def M(A):return Y(A)|H(A)
def Y(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def R(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def X(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def W(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def E(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def Z(A,B,C):
	H,I=len(A),len(A[0]);D=list(list(A)for A in A)
	for(F,G)in E(C):
		if 0<=F<H and 0<=G<I:D[F][G]=B
	return tuple(tuple(A)for A in D)
def K(A):
	if isinstance(A,tuple):return frozenset({B for A in A for B in A})
	return frozenset({A for(A,B)in A})
def U(A,B,C,D):
	K=X(A)if D else None;L=set();G=set();P,Q=len(A),len(A[0]);S=R(A);T=M if C else Y
	for E in S:
		if E in G:continue
		H=A[E[0]][E[1]]
		if H==K:continue
		N={(H,E)};I={E}
		while len(I)>0:
			O=set()
			for F in I:
				J=A[F[0]][F[1]]
				if H==J if B else J!=K:N.add((J,F));G.add(F);O|={(A,B)for(A,B)in T(F)if 0<=A<P and 0<=B<Q}
			I=O-G
		L.add(frozenset(N))
	return frozenset(L)
def J(A,n):return frozenset(A for A in A if len(A)==n)
def G(A,B):return frozenset(A for A in A if next(iter(A))[0]==B)
def L(A,B):return S(Q(B,A))
def P(A):return type(A)(B for A in A for B in A)
def V(a,b):return type(a)(A for A in a if A not in b)
def p(I):B=False;I=tuple(map(tuple,I));C=U(I,True,B,B);D=K(I);E=L(D,0);A=G(C,E);F=J(A,1);H=V(A,F);M=P(H);N=Z(I,8,M);return[*map(list,N)]