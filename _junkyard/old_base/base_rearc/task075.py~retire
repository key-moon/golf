def H(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def V(A):return G(A)|H(A)
def G(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def X(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def U(A):return max(A for(A,B)in Y(A))
def J(A):return max(A for(B,A)in Y(A))
def P(A):return min(A for(B,A)in Y(A))
def M(A):return min(A for(A,B)in Y(A))
def GR(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return J(A)-P(A)+1
def GY(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return U(A)-M(A)+1
def GP(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def Y(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def Q(A):return tuple(map(min,zip(*Y(A))))
def S(A):
	if isinstance(A,tuple):return tuple(zip(*A))
	B,C=Q(A)
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(E-C+B,D-B+C))for(A,(D,E))in A)
	return frozenset((D-C+B,A-B+C)for(A,D)in A)
def K(A):C,D=len(A),len(A[0]);B=tuple(A for(A,B)in enumerate(A)if len(set(B))==1);E=tuple(A for(A,B)in enumerate(S(A))if len(set(B))==1);F=frozenset({frozenset({(A[B][C],(B,C))for C in range(D)})for B in B});G=frozenset({frozenset({(A[C][B],(C,B))for C in range(C)})for B in E});return F|G
def GJ(A):return M(A)+GY(A)//2,P(A)+GR(A)//2
def GM(A,B):
	F,G=len(A),len(A[0]);C=list(list(A)for A in A)
	for(H,(D,E))in B:
		if 0<=D<F and 0<=E<G:C[D][E]=H
	return tuple(tuple(A)for A in C)
def GS(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in Y(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def L(A,B,C,D):
	L=R(A)if D else None;M=set();H=set();P,Q=len(A),len(A[0]);S=X(A);T=V if C else G
	for E in S:
		if E in H:continue
		I=A[E[0]][E[1]]
		if I==L:continue
		N={(I,E)};J={E}
		while len(J)>0:
			O=set()
			for F in J:
				K=A[F[0]][F[1]]
				if I==K if B else K!=L:N.add((K,F));H.add(F);O|={(A,B)for(A,B)in T(F)if 0<=A<P and 0<=B<Q}
			J=O-H
		M.add(frozenset(N))
	return frozenset(M)
def E(A):
	if len(A)==0:return A
	return GU(A,(-M(A),-P(A)))
def GU(A,C):
	if len(A)==0:return A
	B,D=C
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(C+B,E+D))for(A,(C,E))in A)
	return frozenset((A+B,C+D)for(A,C)in A)
def GK(A):return GY(A),GR(A)
def R(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def W(A,B):return GE(GQ(A,B))
def GQ(A,B):return type(B)(A(B)for B in B)
def GV(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda y:A(B,y)
	elif C==3:return lambda y,z:A(B,y,z)
	else:return lambda y,z,a:A(B,y,z,a)
def GH(A,B):return type(B)(B for B in B if B!=A)
def GG(A,B):return max(A,key=B)
def GE(A):return type(A)(B for A in A for B in A)
def GL(A):return len(A)
def GX(n):return n//2 if isinstance(n,int)else(n[0]//2,n[1]//2)
def Z(n):return-n if isinstance(n,int)else(-n[0],-n[1])
def p(I):C=False;I=tuple(map(tuple,I));D=K(I);F=GE(D);G=R(I);H=GS(I,G,F);B=L(H,C,C,True);A=GG(B,GL);J=GH(A,B);M=GQ(GJ,J);N=E(A);O=GK(A);P=GX(O);Q=Z(P);S=GU(N,Q);T=GV(GU,S);U=W(T,M);V=GM(I,U);return[*map(list,V)]