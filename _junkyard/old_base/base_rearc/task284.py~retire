def U(A):return max(A for(A,B)in J(A))
def V(A):return max(A for(B,A)in J(A))
def L(A):return min(A for(B,A)in J(A))
def GZ(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return V(A)-L(A)+1
def GK(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return U(A)-E(A)+1
def HY(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def Y(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def M(A):return H(A)|Y(A)
def H(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def X(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def R(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def HG(A,B):return HV(A,R(A),J(B))
def GJ(a,b):
	A,B=a;C,D=b;E=min(A,C);F=max(A,C)+1;G=min(B,D);H=max(B,D)+1
	if A==C:return frozenset((A,B)for B in range(G,H))
	elif B==D:return frozenset((A,B)for A in range(E,F))
	elif C-A==D-B:return frozenset((A,B)for(A,B)in zip(range(E,F),range(G,H)))
	elif C-A==B-D:return frozenset((A,B)for(A,B)in zip(range(E,F),range(H-1,G-1,-1)))
	return frozenset()
def GQ(A,B):
	F,G=len(A),len(A[0]);C=list(list(A)for A in A)
	for(H,(D,E))in B:
		if 0<=D<F and 0<=E<G:C[D][E]=H
	return tuple(tuple(A)for A in C)
def HV(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in J(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def GM(A):
	if isinstance(A,tuple):return tuple(zip(*A))
	B,C=P(A)
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(E-C+B,D-B+C))for(A,(D,E))in A)
	return frozenset((D-C+B,A-B+C)for(A,D)in A)
def S(A,B):D,E=len(B),len(B[0]);return frozenset((B[A][C],(A,C))for(A,C)in J(A)if 0<=A<D and 0<=C<E)
def GS(A):return next(iter(A))[0]
def G(A):return tuple(map(lambda x:sum(x)//len(A),zip(*J(A))))
def E(A):return min(A for(A,B)in J(A))
def GE(A,B,C,D):
	L=R(A)if D else None;N=set();G=set();Q,S=len(A),len(A[0]);T=X(A);U=M if C else H
	for E in T:
		if E in G:continue
		I=A[E[0]][E[1]]
		if I==L:continue
		O={(I,E)};J={E}
		while len(J)>0:
			P=set()
			for F in J:
				K=A[F[0]][F[1]]
				if I==K if B else K!=L:O.add((K,F));G.add(F);P|={(A,B)for(A,B)in U(F)if 0<=A<Q and 0<=B<S}
			J=P-G
		N.add(frozenset(O))
	return frozenset(N)
def HH(A,C):
	if len(A)==0:return A
	B,D=C
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(C+B,E+D))for(A,(C,E))in A)
	return frozenset((A+B,C+D)for(A,C)in A)
def J(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def W(A):return tuple(map(max,zip(*J(A))))
def Z(A):return tuple(map(lambda ix:{0:max,1:min}[ix[0]](ix[1]),enumerate(zip(*J(A)))))
def K(A):return tuple(map(lambda ix:{0:min,1:max}[ix[0]](ix[1]),enumerate(zip(*J(A)))))
def P(A):return tuple(map(min,zip(*J(A))))
def GG(A):return GK(A)>GZ(A)
def GR(A,B):return lambda x:A(B(x))
def GU(A,a,b):return a if A else b
def GY(a,b):return a,b
def GX(A,B):return B.union(frozenset({A}))
def HJ(A):return max(enumerate(A))[1]
def GW(A):return next(iter(A))
def GV(A):return frozenset({A})
def GL(A):return type(A)(B for A in A for B in A)
def GP(A,B):return tuple(sorted(A,key=B))
def GH(a,b):return type(a)((*a,*b))
def HM(a,b):
	if isinstance(a,int)and isinstance(b,int):return a+b
	elif isinstance(a,tuple)and isinstance(b,tuple):return a[0]+b[0],a[1]+b[1]
	elif isinstance(a,int)and isinstance(b,tuple):return a+b[0],a+b[1]
	return a[0]+b,a[1]+b
def Q(x):return x
def p(I):a=False;B=True;I=tuple(map(tuple,I));b=GE(I,B,a,B);c=GL(b);d=GG(c);D=GU(d,Q,GM);F=D(I);e=GE(F,B,a,B);H=GP(e,E);L=GW(H);M=HJ(H);N=GS(L);O=GS(M);R=GR(GW,J);T=R(L);f=R(M);U=GJ(T,f);C=G(U);g=GJ(T,C);h=HV(F,O,U);V=HV(h,N,g);i=HM(C,(1,0));j=GV(C);X=GX(i,j);Y=S(X,V);k=GY(0,-2);l=HH(Y,(0,2));m=HH(Y,k);A=GH(l,m);n=P(A);o=K(A);p=GJ(n,o);q=HH(p,(-1,0));r=Z(A);s=W(A);t=GJ(r,s);u=HH(t,(1,0));v=GQ(V,A);w=HV(v,N,q);x=HV(w,O,u);y=HG(x,X);z=D(y);return[*map(list,z)]