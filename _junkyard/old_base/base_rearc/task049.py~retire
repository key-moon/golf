def V(A):return max(A for(A,B)in G(A))
def J(A):return min(A for(A,B)in G(A))
def H(A):return max(A for(B,A)in G(A))
def R(A):return min(A for(B,A)in G(A))
def K(A):
	if isinstance(A,tuple):return frozenset({B for A in A for B in A})
	return frozenset({A for(A,B)in A})
def Z(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def G(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def GY(A,B,C):return tuple(A[B[1]:B[1]+C[1]]for A in A[B[0]:B[0]+C[0]])
def E(A):return tuple(map(min,zip(*G(A))))
def W(A):return S(A),L(A)
def Q(A,B):return GY(B,E(A),W(A))
def Y(A):return frozenset(frozenset((A,(C,E))for(C,D)in enumerate(A)for(E,A)in enumerate(D)if A==B)for B in K(A))
def L(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return H(A)-R(A)+1
def S(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return V(A)-J(A)+1
def GG(A,a,b):return lambda x:A(a(x),b(x))
def U(A,B):return type(A)(A for A in A if B(A))
def P(A,B):return min(A,key=B)
def GH(A):return len(A)
def X(a,b):return a==b
def M(a,b):
	if isinstance(a,int)and isinstance(b,int):return a*b
	elif isinstance(a,tuple)and isinstance(b,tuple):return a[0]*b[0],a[1]*b[1]
	elif isinstance(a,int)and isinstance(b,tuple):return a*b[0],a*b[1]
	return a[0]*b,a[1]*b
def p(I):I=tuple(map(tuple,I));B=Y(I);A=GG(M,S,L);C=GG(X,GH,A);D=U(B,C);E=P(D,A);F=Q(E,I);return[*map(list,F)]