def GE(A,B):return type(B)(A(B)for B in B)
def GJ(A):return type(A)(B for A in A for B in A)
def H(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def J(A):return G(A)|H(A)
def G(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def E(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def M(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def P(A):
	if isinstance(A,tuple):return frozenset({B for A in A for B in A})
	return frozenset({A for(A,B)in A})
def GR(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def Y(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def K(A):return tuple(map(max,zip(*Y(A))))
def U(A):return tuple(map(min,zip(*Y(A))))
def X(A):
	if len(A)==0:return frozenset({})
	B=Y(A);C,D=U(B);E,F=K(A);return frozenset((A,B)for A in range(C,E+1)for B in range(D,F+1))
def GU(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in Y(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def R(A):return len(P(A))
def L(A,B,C,D):
	O=M(A)if D else None;P=set();I=set();S,T=len(A),len(A[0]);U=E(A);V=J if C else G
	for F in U:
		if F in I:continue
		K=A[F[0]][F[1]]
		if K==O:continue
		Q={(K,F)};L={F}
		while len(L)>0:
			R=set()
			for H in L:
				N=A[H[0]][H[1]]
				if K==N if B else N!=O:Q.add((N,H));I.add(H);R|={(A,B)for(A,B)in V(H)if 0<=A<S and 0<=B<T}
			L=R-I
		P.add(frozenset(Q))
	return frozenset(P)
def Q(A):return tuple(map(lambda ix:{0:max,1:min}[ix[0]](ix[1]),enumerate(zip(*Y(A)))))
def Z(A,B):return GJ(GE(A,B))
def GX(A,a,b):return lambda x:A(a(x),b(x))
def GY(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda y:A(B,y)
	elif C==3:return lambda y,z:A(B,y,z)
	else:return lambda y,z,a:A(B,y,z,a)
def GV(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def GM(h,g,f):return lambda x:h(g(f(x)))
def W(A,B):return lambda x:A(B(x))
def GG(A,B):return B.union(frozenset({A}))
def GH(i):return i,0
def V(x):return x-1 if isinstance(x,int)else(x[0]-1,x[1]-1)
def S(A):return frozenset({A})
def GK(a,b):
	if isinstance(a,int)and isinstance(b,int):return a+b
	elif isinstance(a,tuple)and isinstance(b,tuple):return a[0]+b[0],a[1]+b[1]
	elif isinstance(a,int)and isinstance(b,tuple):return a+b[0],a+b[1]
	return a[0]+b,a[1]+b
def p(I):B=False;I=tuple(map(tuple,I));C=L(I,B,B,True);D=GY(GK,(1,0));A=W(D,Q);E=GV(GK,(0,1));F=W(E,A);G=GM(GH,V,R);H=GX(GK,F,G);J=W(S,H);K=GX(GG,A,J);M=W(X,K);N=Z(M,C);O=GU(I,3,N);return[*map(list,O)]