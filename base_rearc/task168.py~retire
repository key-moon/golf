def GR(A,B):return type(B)(A(B)for B in B)
def GV(A):return type(A)(B for A in A for B in A)
def GX(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def H(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def J(A):return G(A)|H(A)
def G(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def M(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def V(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def L(a,b):
	A,B=a;C,D=b;E=min(A,C);F=max(A,C)+1;G=min(B,D);H=max(B,D)+1
	if A==C:return frozenset((A,B)for B in range(G,H))
	elif B==D:return frozenset((A,B)for A in range(E,F))
	elif C-A==D-B:return frozenset((A,B)for(A,B)in zip(range(E,F),range(G,H)))
	elif C-A==B-D:return frozenset((A,B)for(A,B)in zip(range(E,F),range(H-1,G-1,-1)))
	return frozenset()
def GY(A,B):return L(A,(A[0]+42*B[0],A[1]+42*B[1]))
def GH(A,B):
	F,G=len(A),len(A[0]);C=list(list(A)for A in A)
	for(H,(D,E))in B:
		if 0<=D<F and 0<=E<G:C[D][E]=H
	return tuple(tuple(A)for A in C)
def GJ(A):return next(iter(A))[0]
def W(A,B,C,D):
	N=V(A)if D else None;O=set();H=set();R,S=len(A),len(A[0]);T=M(A);U=J if C else G
	for E in T:
		if E in H:continue
		I=A[E[0]][E[1]]
		if I==N:continue
		P={(I,E)};K={E}
		while len(K)>0:
			Q=set()
			for F in K:
				L=A[F[0]][F[1]]
				if I==L if B else L!=N:P.add((L,F));H.add(F);Q|={(A,B)for(A,B)in U(F)if 0<=A<R and 0<=B<S}
			K=Q-H
		O.add(frozenset(P))
	return frozenset(O)
def S(A,B):return frozenset((A,B)for B in Y(B))
def Y(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def U(A):return tuple(map(max,zip(*Y(A))))
def K(A):return tuple(map(lambda ix:{0:max,1:min}[ix[0]](ix[1]),enumerate(zip(*Y(A)))))
def E(A):return tuple(map(lambda ix:{0:min,1:max}[ix[0]](ix[1]),enumerate(zip(*Y(A)))))
def R(A):return tuple(map(min,zip(*Y(A))))
def Z(A,B):return GV(GR(A,B))
def GU(A,a,b):return lambda x:A(a(x),b(x))
def GM(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def GE(h,g,f):return lambda x:h(g(f(x)))
def GG(A,B):return type(B)(B for B in B if B!=A)
def Q(A,B):return type(A)(A for A in A if B(A))
def P(a,b):return type(a)((*a,*b))
def X(a,b):return a==b
def GK(a,b):
	if isinstance(a,int)and isinstance(b,int):return a+b
	elif isinstance(a,tuple)and isinstance(b,tuple):return a[0]+b[0],a[1]+b[1]
	elif isinstance(a,int)and isinstance(b,tuple):return a+b[0],a+b[1]
	return a[0]+b,a[1]+b
def p(I):I=tuple(map(tuple,I));A=W(I,True,False,True);B=GM(GY,(1,1));C=GM(GK,(1,1));D=GE(B,C,U);F=GU(S,GJ,D);G=GM(GY,(-1,1));H=GM(GK,(-1,1));J=GE(G,H,E);L=GU(S,GJ,J);M=GM(GY,(-1,-1));N=GM(GK,(-1,-1));O=GE(M,N,R);T=GU(S,GJ,O);V=GM(GY,(1,-1));a=GM(GK,(1,-1));b=GE(V,a,K);c=GU(S,GJ,b);d=GU(GG,U,Y);e=GU(X,Y,d);f=Q(A,e);g=GU(GG,E,Y);h=GU(X,Y,g);i=Q(A,h);j=GU(GG,R,Y);k=GU(X,Y,j);l=Q(A,k);m=GU(GG,K,Y);n=GU(X,Y,m);o=Q(A,n);p=Z(F,f);q=Z(L,i);r=P(p,q);s=Z(T,l);t=Z(c,o);u=P(s,t);v=P(r,u);w=GH(I,v);return[*map(list,w)]