def GY(A):return next(iter(A))
def V(A):return max(A for(A,B)in G(A))
def J(A):return min(A for(A,B)in G(A))
def H(A):return max(A for(B,A)in G(A))
def X(A):return min(A for(B,A)in G(A))
def GM(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def G(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def S(a,b):
	A,B=a;C,D=b;E=min(A,C);F=max(A,C)+1;G=min(B,D);H=max(B,D)+1
	if A==C:return frozenset((A,B)for B in range(G,H))
	elif B==D:return frozenset((A,B)for A in range(E,F))
	elif C-A==D-B:return frozenset((A,B)for(A,B)in zip(range(E,F),range(G,H)))
	elif C-A==B-D:return frozenset((A,B)for(A,B)in zip(range(E,F),range(H-1,G-1,-1)))
	return frozenset()
def GG(A,B):return S(A,(A[0]+42*B[0],A[1]+42*B[1]))
def GR(A,B,C):
	H,I=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in G(C):
		if 0<=E<H and 0<=F<I:D[E][F]=B
	return tuple(tuple(A)for A in D)
def GH(A):return next(iter(A))[0]
def P(A):
	if isinstance(A,tuple):return frozenset({B for A in A for B in A})
	return frozenset({A for(A,B)in A})
def Y(A):return frozenset(frozenset((A,(C,E))for(C,D)in enumerate(A)for(E,A)in enumerate(D)if A==B)for B in P(A))
def U(A):return tuple(map(max,zip(*G(A))))
def K(A):return tuple(map(lambda ix:{0:max,1:min}[ix[0]](ix[1]),enumerate(zip(*G(A)))))
def E(A):return tuple(map(lambda ix:{0:min,1:max}[ix[0]](ix[1]),enumerate(zip(*G(A)))))
def R(A):return tuple(map(min,zip(*G(A))))
def GJ(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return H(A)-X(A)+1
def Z(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return V(A)-J(A)+1
def GE(A,a,b):return lambda x:A(a(x),b(x))
def GV(A,B):return GY(W(B,A))
def W(A,B):return type(B)(B for B in B if B!=A)
def L(A,B):return max(A,key=B)
def Q(a,b):return type(a)((*a,*b))
def M(a,b):
	if isinstance(a,int)and isinstance(b,int):return a*b
	elif isinstance(a,tuple)and isinstance(b,tuple):return a[0]*b[0],a[1]*b[1]
	elif isinstance(a,int)and isinstance(b,tuple):return a*b[0],a*b[1]
	return a[0]*b,a[1]*b
def GX(a,b):
	if isinstance(a,int)and isinstance(b,int):return a+b
	elif isinstance(a,tuple)and isinstance(b,tuple):return a[0]+b[0],a[1]+b[1]
	elif isinstance(a,int)and isinstance(b,tuple):return a+b[0],a+b[1]
	return a[0]+b,a[1]+b
def p(I):I=tuple(map(tuple,I));B=GE(M,Z,GJ);C=Y(I);F=L(C,B);D=W(F,C);A=L(D,B);G=GV(D,A);j=P(I);H=U(A);J=GX(H,(1,1));N=K(A);O=GX(N,(1,-1));S=E(A);T=GX(S,(-1,1));V=R(A);X=GX(V,(-1,-1));a=GG(J,(1,1));b=GG(O,(1,-1));c=GG(T,(-1,1));d=GG(X,(-1,-1));e=Q(a,b);f=Q(c,d);g=Q(e,f);h=GH(G);i=GR(I,h,g);return[*map(list,i)]