def E(A):return tuple(map(max,zip(*G(A))))
def J(A):return max(A for(A,B)in G(A))
def Y(A):return min(A for(A,B)in G(A))
def H(A):return max(A for(B,A)in G(A))
def M(A):return min(A for(B,A)in G(A))
def GH(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return H(A)-M(A)+1
def Z(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return J(A)-Y(A)+1
def GV(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def G(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def GE(A,B,C):return tuple(A[B[1]:B[1]+C[1]]for A in A[B[0]:B[0]+C[0]])
def V(A):return tuple(map(min,zip(*G(A))))
def GY(A):return Z(A),GH(A)
def X(A,B,C):return tuple(tuple(C if A==B else A for A in A)for A in A)
def S(A,B):return GE(B,V(A),GY(A))
def K(A):
	if isinstance(A,tuple):return tuple(zip(*(A[::-1]for A in A[::-1])))
	return P(L(P(A)))
def L(A):
	if isinstance(A,tuple):return tuple(zip(*A))
	B,C=V(A)
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(E-C+B,D-B+C))for(A,(D,E))in A)
	return frozenset((D-C+B,A-B+C)for(A,D)in A)
def P(A):
	if isinstance(A,tuple):return tuple(A[::-1]for A in A)
	B=V(A)[1]+E(A)[1]
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(C,B-D))for(A,(C,D))in A)
	return frozenset((A,B-C)for(A,C)in A)
def U(A):
	if isinstance(A,tuple):return A[::-1]
	B=V(A)[0]+E(A)[0]
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(B-C,D))for(A,(C,D))in A)
	return frozenset((B-A,C)for(A,C)in A)
def Q(A,B):return frozenset((A,D)for(A,C)in enumerate(A)for(D,E)in enumerate(C)if E==B)
def W(A,a,b):return tuple(A(B,C)for(B,C)in zip(a,b))
def GJ(A,B):return type(B)(A(B)for B in B)
def GG(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda y:A(B,y)
	elif C==3:return lambda y,z:A(B,y,z)
	else:return lambda y,z,a:A(B,y,z,a)
def GM(a,b):return tuple(zip(a,b))
def R(A):return max(A,default=0)
def p(I):I=tuple(map(tuple,I));B=X(I,3,-1);F=L(B);G=W(GM,B,F);A=GG(GJ,R);C=GJ(A,G);H=K(C);J=W(GM,C,H);D=GJ(A,J);M=U(D);N=W(GM,D,M);E=GJ(A,N);O=P(E);T=W(GM,O,E);V=GJ(A,T);Y=Q(I,3);Z=S(Y,V);return[*map(list,Z)]