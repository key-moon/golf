def X(A):return max(A for(A,B)in J(A))
def E(A):return min(A for(A,B)in J(A))
def V(A):return max(A for(B,A)in J(A))
def U(A):return min(A for(B,A)in J(A))
def W(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return V(A)-U(A)+1
def S(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return X(A)-E(A)+1
def H(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def G(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def GG(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def J(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def P(A,B):return tuple(tuple(A for B in range(B[1]))for C in range(B[0]))
def GH(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in J(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def M(A):return G(A)|H(A)
def K(A,B):return frozenset((A,D)for(A,C)in enumerate(A)for(D,E)in enumerate(C)if E==B)
def Z(A):return S(A),W(A)
def Y(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return min(set(B),key=B.count)
def R(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def L(A):return next(iter(A))
def Q(A):return frozenset({A})
def p(I):I=tuple(map(tuple,I));A=Y(I);C=K(I,A);B=L(C);D=M(B);E=R(I);F=Z(I);G=P(E,F);H=Q(B);J=GH(G,A,H);N=GH(J,2,D);return[*map(list,N)]