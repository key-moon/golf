def GM(A):return type(A)(B for A in A for B in A)
def V(A):return max(A for(A,B)in G(A))
def H(A):return max(A for(B,A)in G(A))
def GK(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def G(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def E(A):return min(A for(B,A)in G(A))
def Y(A):return min(A for(A,B)in G(A))
def GE(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return H(A)-E(A)+1
def Z(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return V(A)-Y(A)+1
def K(a,b):
	A,B=a;C,D=b;E=min(A,C);F=max(A,C)+1;G=min(B,D);H=max(B,D)+1
	if A==C:return frozenset((A,B)for B in range(G,H))
	elif B==D:return frozenset((A,B)for A in range(E,F))
	elif C-A==D-B:return frozenset((A,B)for(A,B)in zip(range(E,F),range(G,H)))
	elif C-A==B-D:return frozenset((A,B)for(A,B)in zip(range(E,F),range(H-1,G-1,-1)))
	return frozenset()
def GY(A,B):return K(A,(A[0]+42*B[0],A[1]+42*B[1]))
def L(A,B):return tuple(tuple(A for B in range(B[1]))for C in range(B[0]))
def GG(A):return Y(A)+Z(A)//2,E(A)+GE(A)//2
def GH(A,B):
	F,G=len(A),len(A[0]);C=list(list(A)for A in A)
	for(H,(D,E))in B:
		if 0<=D<F and 0<=E<G:C[D][E]=H
	return tuple(tuple(A)for A in C)
def M(A):return frozenset((D,(A,C))for(A,B)in enumerate(A)for(C,D)in enumerate(B))
def GJ(A):return next(iter(A))[0]
def X(A,B):return frozenset((A,B)for B in G(B))
def GX(A):return Z(A),GE(A)
def J(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def S(A,B):return GM(GU(A,B))
def GU(A,B):return type(B)(A(B)for B in B)
def GQ(A,a,b):return lambda x:A(a(x),b(x))
def GR(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def U(A,B):return lambda x:A(x)==B
def P(A,B):return lambda x:A(B(x))
def GV(A):return next(iter(A))
def R(A,B):return type(A)(A for A in A if B(A))
def Q(A):return frozenset({A})
def GP(b):return not b
def W(n):return n*2 if isinstance(n,int)else(n[0]*2,n[1]*2)
def p(I):I=tuple(map(tuple,I));A=M(I);B=J(I);C=U(GV,B);D=P(GP,C);E=R(A,D);F=GU(Q,E);G=GR(GY,(1,1));H=P(G,GG);K=GQ(X,GJ,H);N=S(K,F);O=GX(I);T=W(O);V=J(I);Y=L(V,T);Z=GH(Y,N);return[*map(list,Z)]