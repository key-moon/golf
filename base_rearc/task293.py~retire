def H(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def J(A):return Y(A)|H(A)
def Y(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def M(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def E(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def W(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def V(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def X(A):return tuple(map(max,zip(*V(A))))
def R(A):return tuple(map(min,zip(*V(A))))
def U(A):
	if len(A)==0:return frozenset({})
	B=V(A);C,D=R(B);E,F=X(A);return frozenset((A,B)for A in range(C,E+1)for B in range(D,F+1))
def Z(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in V(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def S(A):return next(iter(A))[0]
def P(A,B,C,D):
	N=E(A)if D else None;O=set();H=set();R,S=len(A),len(A[0]);T=M(A);U=J if C else Y
	for F in T:
		if F in H:continue
		I=A[F[0]][F[1]]
		if I==N:continue
		P={(I,F)};K={F}
		while len(K)>0:
			Q=set()
			for G in K:
				L=A[G[0]][G[1]]
				if I==L if B else L!=N:P.add((L,G));H.add(G);Q|={(A,B)for(A,B)in U(G)if 0<=A<R and 0<=B<S}
			K=Q-H
		O.add(frozenset(P))
	return frozenset(O)
def K(A,B):return frozenset((A,D)for(A,C)in enumerate(A)for(D,E)in enumerate(C)if E==B)
def L(A,B):return type(B)(A(B)for B in B)
def Q(A):return tuple(A)
def G(A):return max(set(A),key=A.count)
def p(I):I=tuple(map(tuple,I));B=P(I,True,False,True);C=Q(B);D=L(S,C);A=G(D);E=K(I,A);F=U(E);H=Z(I,A,F);return[*map(list,H)]