def GZ(A,B):return type(B)(A(B)for B in B)
def GL(A):return type(A)(B for A in A for B in A)
def U(A):return max(A for(A,B)in Y(A))
def M(A):return min(A for(A,B)in Y(A))
def J(A):return max(A for(B,A)in Y(A))
def GW(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return J(A)-W(A)+1
def GQ(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return U(A)-M(A)+1
def H(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def V(A):return G(A)|H(A)
def G(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def X(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def E(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def HG(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def Y(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def S(A):return tuple(map(min,zip(*Y(A))))
def R(A):return frozenset((B,A[1])for B in range(30))
def GP(A,B):
	F,G=len(A),len(A[0]);C=list(list(A)for A in A)
	for(H,(D,E))in B:
		if 0<=D<F and 0<=E<G:C[D][E]=H
	return tuple(tuple(A)for A in C)
def GY(A):
	if isinstance(A,tuple):return tuple(zip(*A))
	B,C=S(A)
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(E-C+B,D-B+C))for(A,(D,E))in A)
	return frozenset((D-C+B,A-B+C)for(A,D)in A)
def GS(A):return next(iter(A))[0]
def W(A):return min(A for(B,A)in Y(A))
def GJ(A,B,C,D):
	M=E(A)if D else None;N=set();I=set();Q,R=len(A),len(A[0]);S=X(A);T=V if C else G
	for F in S:
		if F in I:continue
		J=A[F[0]][F[1]]
		if J==M:continue
		O={(J,F)};K={F}
		while len(K)>0:
			P=set()
			for H in K:
				L=A[H[0]][H[1]]
				if J==L if B else L!=M:O.add((L,H));I.add(H);P|={(A,B)for(A,B)in T(H)if 0<=A<Q and 0<=B<R}
			K=P-I
		N.add(frozenset(O))
	return frozenset(N)
def GH(A,B):return frozenset((A,B)for B in Y(B))
def Z(A):return GQ(A)>GW(A)
def GM(A,B):return GL(GZ(A,B))
def GV(A,B):return lambda x:A(B(x))
def GX(A,a,b):return a if A else b
def L(A,B,C):return tuple(range(A,B,C))
def GR(j):return 0,j
def GU(A,B):return min(A,key=B)
def GE(A,B):return max(A,key=B)
def GG(a,b):return type(a)((*a,*b))
def GK(n):return n*2 if isinstance(n,int)else(n[0]*2,n[1]*2)
def K(a,b):
	if isinstance(a,int)and isinstance(b,int):return a*b
	elif isinstance(a,tuple)and isinstance(b,tuple):return a[0]*b[0],a[1]*b[1]
	elif isinstance(a,int)and isinstance(b,tuple):return a*b[0],a*b[1]
	return a[0]*b,a[1]*b
def P(a,b):
	if isinstance(a,int)and isinstance(b,int):return a-b
	elif isinstance(a,tuple)and isinstance(b,tuple):return a[0]-b[0],a[1]-b[1]
	elif isinstance(a,int)and isinstance(b,tuple):return a-b[0],a-b[1]
	return a[0]-b,a[1]-b
def Q(x):return x
def p(I):I=tuple(map(tuple,I));N=Z(I);A=GX(N,GY,Q);B=A(I);C=GJ(B,True,False,True);D=GU(C,W);E=GE(C,W);O=GS(D);S=GS(E);F=W(D);G=W(E);T=P(G,F);H=GK(T);J=K(3,10);U=L(F,J,H);V=L(G,J,H);M=GV(R,GR);X=GM(M,U);Y=GM(M,V);a=GH(O,X);b=GH(S,Y);c=GG(a,b);d=GP(B,c);e=A(d);return[*map(list,e)]