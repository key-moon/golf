def S(A):return type(A)(B for A in A for B in A)
def E(A):return tuple(map(max,zip(*H(A))))
def M(A):return tuple(map(min,zip(*H(A))))
def W(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def H(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def Y(A):return max(A for(B,A)in H(A))
def X(A):return min(A for(B,A)in H(A))
def J(A):return max(A for(A,B)in H(A))
def V(A):return min(A for(A,B)in H(A))
def Q(A):F,G=V(A)-1,X(A)-1;H,I=J(A)+1,Y(A)+1;B,C=min(F,H),min(G,I);D,E=max(F,H),max(G,I);K={(A,C)for A in range(B,D+1)}|{(A,E)for A in range(B,D+1)};L={(B,A)for A in range(C,E+1)}|{(D,A)for A in range(C,E+1)};return frozenset(K|L)
def R(A):
	if len(A)==0:return frozenset({})
	B=H(A);C,D=M(B);F,G=E(A);return frozenset((A,B)for A in range(C,F+1)for B in range(D,G+1))
def Z(A,B,C):
	G,I=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in H(C):
		if 0<=E<G and 0<=F<I:D[E][F]=B
	return tuple(tuple(A)for A in D)
def U(A,B):return frozenset((A,D)for(A,C)in enumerate(A)for(D,E)in enumerate(C)if E==B)
def G(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return min(set(B),key=B.count)
def P(A,B):return S(L(A,B))
def L(A,B):return type(B)(A(B)for B in B)
def K(A):return frozenset({A})
def p(I):I=tuple(map(tuple,I));A=G(I);B=U(I,A);C=L(K,B);D=L(Q,C);E=P(R,D);F=Z(I,1,E);return[*map(list,F)]