def Y(A):return tuple(map(max,zip(*G(A))))
def K(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def G(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def H(A):return tuple(map(min,zip(*G(A))))
def R(a,b):return a+b
def M(A):
	if isinstance(A,tuple):return tuple(zip(*A))
	B,C=H(A)
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(E-C+B,D-B+C))for(A,(D,E))in A)
	return frozenset((D-C+B,A-B+C)for(A,D)in A)
def V(A):
	if isinstance(A,tuple):return A[::-1]
	B=H(A)[0]+Y(A)[0]
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(B-C,D))for(A,(C,D))in A)
	return frozenset((B-A,C)for(A,C)in A)
def P(A,a,b):return lambda x:A(a(x),b(x))
def E(A,B):return lambda x:A(B(x))
def X(A,B):return type(B)(B for B in B if B!=A)
def Q(A):return max(enumerate(A))[1]
def U(A):return tuple(B for(C,B)in enumerate(A)if A.index(B)==C)
def J(x):return x
def p(I):I=tuple(map(tuple,I));A=E(M,U);C=A(I);D=A(C);F=P(X,Q,J);G=E(V,F);B=P(R,J,G);H=B(D);K=M(H);L=B(K);return[*map(list,L)]