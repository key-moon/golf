def U(A):return tuple(map(max,zip(*H(A))))
def R(A):return tuple(map(min,zip(*H(A))))
def Z(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def H(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def Y(A):return max(A for(B,A)in H(A))
def X(A):return min(A for(B,A)in H(A))
def M(A):return max(A for(A,B)in H(A))
def J(A):return min(A for(A,B)in H(A))
def L(A):F,G=J(A)-1,X(A)-1;H,I=M(A)+1,Y(A)+1;B,C=min(F,H),min(G,I);D,E=max(F,H),max(G,I);K={(A,C)for A in range(B,D+1)}|{(A,E)for A in range(B,D+1)};L={(B,A)for A in range(C,E+1)}|{(D,A)for A in range(C,E+1)};return frozenset(K|L)
def E(A):
	if len(A)==0:return frozenset({})
	B=H(A);C,D=R(B);E,F=U(A);return frozenset((A,B)for A in range(C,E+1)for B in range(D,F+1))
def GH(A,B,C):
	G,I=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in H(C):
		if 0<=E<G and 0<=F<I:D[E][F]=B
	return tuple(tuple(A)for A in D)
def K(A):
	if isinstance(A,tuple):return frozenset({B for A in A for B in A})
	return frozenset({A for(A,B)in A})
def Q(A,B):return frozenset((A,D)for(A,C)in enumerate(A)for(D,E)in enumerate(C)if E==B)
def V(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def S(A,B):return type(B)(B for B in B if B!=A)
def GG(A):return max(enumerate(A))[1]
def W(A):return next(iter(A))
def P(A):return tuple(A)
def G(a,b):return a&b
def p(I):I=tuple(map(tuple,I));B=V(I);C=K(I);D=S(B,C);A=P(D);F=W(A);H=GG(A);J=Q(I,F);M=E(J);N=Q(I,H);O=E(N);R=G(M,O);T=L(R);U=GH(I,4,T);return[*map(list,U)]