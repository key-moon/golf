def K(A,B):return type(A)(A for A in A if B(A))
def L(A):return type(A)(B for A in A for B in A)
def H(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def J(A):return Y(A)|H(A)
def Y(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def E(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def R(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def GG(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def M(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def V(a,b):return min(abs(A-C)+abs(B-D)for(A,B)in M(a)for(C,D)in M(b))
def W(A,B):
	F,G=len(A),len(A[0]);C=list(list(A)for A in A)
	for(H,(D,E))in B:
		if 0<=D<F and 0<=E<G:C[D][E]=H
	return tuple(tuple(A)for A in C)
def X(a,b):return V(a,b)==1
def P(A,B,C,D):
	M=R(A)if D else None;N=set();H=set();Q,S=len(A),len(A[0]);T=E(A);U=J if C else Y
	for F in T:
		if F in H:continue
		I=A[F[0]][F[1]]
		if I==M:continue
		O={(I,F)};K={F}
		while len(K)>0:
			P=set()
			for G in K:
				L=A[G[0]][G[1]]
				if I==L if B else L!=M:O.add((L,G));H.add(G);P|={(A,B)for(A,B)in U(G)if 0<=A<Q and 0<=B<S}
			K=P-H
		N.add(frozenset(O))
	return frozenset(N)
def S(A,B):return frozenset((A,B)for B in M(B))
def Q(A,B):return frozenset((A,D)for(A,C)in enumerate(A)for(D,E)in enumerate(C)if E==B)
def G(A,B):return frozenset(A for A in A if next(iter(A))[0]==B)
def Z(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def U(A,B):return L(K(A,B))
def p(I):A=False;I=tuple(map(tuple,I));B=P(I,True,A,A);C=G(B,0);D=Q(I,1);E=Z(X,D);F=U(C,E);H=S(1,F);J=W(I,H);return[*map(list,J)]