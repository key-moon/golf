def X(A):return min(A for(B,A)in Y(A))
def V(A):return min(A for(A,B)in Y(A))
def GG(A,C):
	if len(A)==0:return A
	B,D=C
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(C+B,E+D))for(A,(C,E))in A)
	return frozenset((A+B,C+D)for(A,C)in A)
def H(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def J(A):return G(A)|H(A)
def G(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def R(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def E(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def GY(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def Y(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def GJ(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in Y(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def Q(A,B,C,D):
	N=E(A)if D else None;O=set();I=set();S,T=len(A),len(A[0]);U=R(A);V=J if C else G
	for F in U:
		if F in I:continue
		K=A[F[0]][F[1]]
		if K==N:continue
		P={(K,F)};L={F}
		while len(L)>0:
			Q=set()
			for H in L:
				M=A[H[0]][H[1]]
				if K==M if B else M!=N:P.add((M,H));I.add(H);Q|={(A,B)for(A,B)in V(H)if 0<=A<S and 0<=B<T}
			L=Q-I
		O.add(frozenset(P))
	return frozenset(O)
def M(A):
	if len(A)==0:return A
	return GG(A,(-V(A),-X(A)))
def W(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda y:A(B,y)
	elif C==3:return lambda y,z:A(B,y,z)
	else:return lambda y,z,a:A(B,y,z,a)
def K(A,B):return lambda x:A(x)==B
def GH(h,g,f):return lambda x:h(g(f(x)))
def P(A,B):return lambda x:A(B(x))
def S(A,B):return type(B)(B for B in B if B!=A)
def U(A,B):return type(A)(A for A in A if B(A))
def L(A,B):return min(A,key=B)
def Z(A):return type(A)(B for A in A for B in A)
def GV(A):return len(A)
def p(I):B=True;I=tuple(map(tuple,I));A=Q(I,B,B,B);D=W(U,A);E=W(K,M);F=P(E,M);G=GH(GV,D,F);C=L(A,G);H=S(C,A);J=Z(H);N=GJ(I,2,C);O=GJ(N,1,J);return[*map(list,O)]