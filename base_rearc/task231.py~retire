def M(A):return max(A for(A,B)in G(A))
def H(A):return max(A for(B,A)in G(A))
def GJ(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def G(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def X(A):return min(A for(B,A)in G(A))
def Y(A):return min(A for(A,B)in G(A))
def V(A):
	if len(A)==0:return A
	return GH(A,(-Y(A),-X(A)))
def GH(A,C):
	if len(A)==0:return A
	B,D=C
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(C+B,E+D))for(A,(C,E))in A)
	return frozenset((A+B,C+D)for(A,C)in A)
def U(A):
	B=V(A);C=GG(B)
	for D in range(1,C):
		E=GH(B,(0,-D));F=frozenset({(B,(C,A))for(B,(C,A))in E if A>=0})
		if F.issubset(B):return D
	return C
def P(A):return tuple(tuple(A[::-1])for A in zip(*A[::-1]))[::-1]
def GY(A):return tuple(A for A in zip(*A[::-1]))
def E(A):return frozenset((D,(A,C))for(A,B)in enumerate(A)for(C,D)in enumerate(B))
def GV(A,B,C):return tuple(A[B[1]:B[1]+C[1]]for A in A[B[0]:B[0]+C[0]])
def R(A):return tuple(map(min,zip(*G(A))))
def GG(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return H(A)-X(A)+1
def W(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return M(A)-Y(A)+1
def K(a,b):return a,b
def J(x):return x+1 if isinstance(x,int)else(x[0]+1,x[1]+1)
def Z(A):return type(A)(B for A in A for B in A)
def Q(A,B):return tuple(A for B in range(B))
def L(n):return n*2 if isinstance(n,int)else(n[0]*2,n[1]*2)
def S(a,b):
	if isinstance(a,int)and isinstance(b,int):return a//b
	elif isinstance(a,tuple)and isinstance(b,tuple):return a[0]//b[0],a[1]//b[1]
	elif isinstance(a,int)and isinstance(b,tuple):return a//b[0],a//b[1]
	return a[0]//b,a[1]//b
def p(I):I=tuple(map(tuple,I));F=GG(I);A=E(I);B=U(A);C=W(A);G=K(C,B);H=R(A);M=GV(I,H,G);N=GY(M);D=L(F);O=S(D,B);T=J(O);V=Q(N,T);X=Z(V);Y=P(X);a=K(C,D);b=GV(Y,(0,0),a);return[*map(list,b)]