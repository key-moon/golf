def Y(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def R(A):return H(A)|Y(A)
def H(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def Q(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def U(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def V(A):return max(A for(B,A)in J(A))
def P(A):return max(A for(A,B)in J(A))
def GR(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def GU(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in J(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def M(A,B):return X(A)==0 or S(A)==0 or P(A)==len(B)-1 or V(A)==len(B[0])-1
def K(a,b):return len(set(A for(B,A)in J(a))&set(A for(B,A)in J(b)))>0
def E(a,b):return len(set(A for(A,B)in J(a))&set(A for(A,B)in J(b)))>0
def S(A):return min(A for(B,A)in J(A))
def X(A):return min(A for(A,B)in J(A))
def W(A,B,C,D):
	L=U(A)if D else None;M=set();G=set();P,S=len(A),len(A[0]);T=Q(A);V=R if C else H
	for E in T:
		if E in G:continue
		I=A[E[0]][E[1]]
		if I==L:continue
		N={(I,E)};J={E}
		while len(J)>0:
			O=set()
			for F in J:
				K=A[F[0]][F[1]]
				if I==K if B else K!=L:N.add((K,F));G.add(F);O|={(A,B)for(A,B)in V(F)if 0<=A<P and 0<=B<S}
			J=O-G
		M.add(frozenset(N))
	return frozenset(M)
def J(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def G(A,B):return frozenset(A for A in A if next(iter(A))[0]==B)
def GE(A,B):return type(B)(A(B)for B in B)
def GV(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda y:A(B,y)
	elif C==3:return lambda y,z:A(B,y,z)
	else:return lambda y,z,a:A(B,y,z,a)
def GM(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def GG(A,B):return lambda x:A(B(x))
def GY(A,B):return type(B)(B for B in B if B!=A)
def Z(A,B):return next(A for A in A if B(A))
def L(A,B):return type(A)(A for A in A if B(A))
def GJ(A,B):return min(A,key=B)
def GH(A,B):return max(A,key=B)
def GX(b):return not b
def p(I):H=False;I=tuple(map(tuple,I));N=W(I,True,H,H);O=GR(I,(0,0));P=G(N,O);B=GE(J,P);Q=GM(M,I);R=GG(GX,Q);A=Z(B,R);C=GY(A,B);T=GV(K,A);U=GV(E,A);D=L(C,T);F=L(C,U);V=GJ(D,X);Y=GH(D,X);a=GJ(F,S);b=GH(F,S);c=GU(I,6,A);d=GU(c,2,V);e=GU(d,1,Y);f=GU(e,4,a);g=GU(f,3,b);return[*map(list,g)]