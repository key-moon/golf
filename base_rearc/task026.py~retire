def R(A):return max(A for(A,B)in Y(A))
def E(A):return min(A for(A,B)in Y(A))
def J(A):return max(A for(B,A)in Y(A))
def Q(A):return min(A for(B,A)in Y(A))
def L(A):
	if isinstance(A,tuple):return frozenset({B for A in A for B in A})
	return frozenset({A for(A,B)in A})
def GK(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def Y(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def GV(A):return tuple(tuple(A[::-1])for A in zip(*A[::-1]))[::-1]
def GX(A):return tuple(A for A in zip(*A[::-1]))
def V(A):return GV(H(GX(A)))
def P(A):return GV(S(GX(A)))
def H(A):return A[len(A)//2+len(A)%2:]
def S(A):return A[:len(A)//2]
def GG(a,b):
	A,B=a;C,D=b;E=min(A,C);F=max(A,C)+1;G=min(B,D);H=max(B,D)+1
	if A==C:return frozenset((A,B)for B in range(G,H))
	elif B==D:return frozenset((A,B)for A in range(E,F))
	elif C-A==D-B:return frozenset((A,B)for(A,B)in zip(range(E,F),range(G,H)))
	elif C-A==B-D:return frozenset((A,B)for(A,B)in zip(range(E,F),range(H-1,G-1,-1)))
	return frozenset()
def GH(A,B):return tuple(tuple(A for B in range(B[1]))for C in range(B[0]))
def GQ(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in Y(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def U(A,B):D,E=len(B),len(B[0]);return frozenset((B[A][C],(A,C))for(A,C)in Y(A)if 0<=A<D and 0<=C<E)
def X(A):return len(L(A))
def Z(A,B):return frozenset((A,D)for(A,C)in enumerate(A)for(D,E)in enumerate(C)if E==B)
def GU(A):return GM(A),GE(A)
def GE(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return J(A)-Q(A)+1
def GM(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return R(A)-E(A)+1
def GJ(A,a,b):return a if A else b
def W(a,b):return a,b
def GY(j):return 0,j
def M(x):return x-1 if isinstance(x,int)else(x[0]-1,x[1]-1)
def G(a,b):return a&b
def K(a,b):return a==b
def GR(n):return n//2 if isinstance(n,int)else(n[0]//2,n[1]//2)
def p(I):I=tuple(map(tuple,I));D=GE(I);A=GR(D);E=GY(A);F=GM(I);J=M(F);L=W(J,A);N=GG(E,L);O=U(N,I);Q=X(O);B=K(Q,1);R=GJ(B,P,S);T=GJ(B,V,H);C=R(I);Y=T(I);a=GU(C);b=GH(0,a);c=Z(C,0);d=Z(Y,0);e=G(c,d);f=GQ(b,8,e);return[*map(list,f)]