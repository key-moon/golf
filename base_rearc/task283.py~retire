def GV(A,B):return type(B)(A(B)for B in B)
def GM(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def H(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def J(A):return G(A)|H(A)
def G(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def M(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def V(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def Q(A):return tuple(map(lambda ix:{0:max,1:min}[ix[0]](ix[1]),enumerate(zip(*Y(A)))))
def E(A):return tuple(map(lambda ix:{0:min,1:max}[ix[0]](ix[1]),enumerate(zip(*Y(A)))))
def K(A):return tuple(map(max,zip(*Y(A))))
def X(A):return tuple(map(min,zip(*Y(A))))
def GU(A):
	if len(A)==0:return A
	F,G=X(A);H,I=K(A);B,C=min(F,H),min(G,I);D,E=max(F,H),max(G,I);J={(A,C)for A in range(B,D+1)}|{(A,E)for A in range(B,D+1)};L={(B,A)for A in range(C,E+1)}|{(D,A)for A in range(C,E+1)};return frozenset(J|L)
def R(A):
	if len(A)==0:return frozenset({})
	B=Y(A);C,D=X(B);E,F=K(A);return frozenset((A,B)for A in range(C,E+1)for B in range(D,F+1))
def S(A):return frozenset({X(A),E(A),Q(A),K(A)})
def GX(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in Y(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def GY(A):return next(iter(A))[0]
def W(A,B,C,D):
	N=V(A)if D else None;O=set();H=set();R,S=len(A),len(A[0]);T=M(A);U=J if C else G
	for E in T:
		if E in H:continue
		I=A[E[0]][E[1]]
		if I==N:continue
		P={(I,E)};K={E}
		while len(K)>0:
			Q=set()
			for F in K:
				L=A[F[0]][F[1]]
				if I==L if B else L!=N:P.add((L,F));H.add(F);Q|={(A,B)for(A,B)in U(F)if 0<=A<R and 0<=B<S}
			K=Q-H
		O.add(frozenset(P))
	return frozenset(O)
def Y(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def GH(A,B):return GJ(GV(A,B))
def GE(A,a,b):return lambda x:A(a(x),b(x))
def L(A,B):return lambda x:A(x)==B
def GG(A,B):return lambda x:A(B(x))
def Z(A,B):return next(A for A in A if B(A))
def P(A,B):return type(A)(A for A in A if B(A))
def GJ(A):return type(A)(B for A in A for B in A)
def U(a,b):return a==b
def GR(b):return not b
def p(I):C=False;I=tuple(map(tuple,I));B=W(I,True,C,C);D=GE(U,Y,R);E=GG(GR,D);F=Z(B,E);G=GY(F);H=L(GY,G);J=GG(GR,H);A=P(B,J);K=GJ(A);M=GX(I,2,K);N=GH(GU,A);O=GX(M,4,N);Q=GH(S,A);T=GX(O,1,Q);return[*map(list,T)]