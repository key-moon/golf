def GX(A,B):return type(B)(A(B)for B in B)
def GE(A):return type(A)(B for A in A for B in A)
def H(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def V(A):return G(A)|H(A)
def G(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def R(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def E(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def X(A):return max(A for(A,B)in Y(A))
def J(A):return max(A for(B,A)in Y(A))
def K(A):return min(A for(B,A)in Y(A))
def M(A):return min(A for(A,B)in Y(A))
def GR(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return J(A)-K(A)+1
def GG(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return X(A)-M(A)+1
def GU(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def Y(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def P(a,b):
	A,B=a;C,D=b;E=min(A,C);F=max(A,C)+1;G=min(B,D);H=max(B,D)+1
	if A==C:return frozenset((A,B)for B in range(G,H))
	elif B==D:return frozenset((A,B)for A in range(E,F))
	elif C-A==D-B:return frozenset((A,B)for(A,B)in zip(range(E,F),range(G,H)))
	elif C-A==B-D:return frozenset((A,B)for(A,B)in zip(range(E,F),range(H-1,G-1,-1)))
	return frozenset()
def GV(A,B):return P(A,(A[0]+42*B[0],A[1]+42*B[1]))
def U(a,b):
	A,B=GH(Y(a));C,D=GH(Y(b))
	if A==C:return 0,1 if B<D else-1
	elif B==D:return 1 if A<C else-1,0
	elif A<C:return 1,1 if B<D else-1
	elif A>C:return-1,1 if B<D else-1
def GH(A):return M(A)+GG(A)//2,K(A)+GR(A)//2
def GJ(A,B):
	F,G=len(A),len(A[0]);C=list(list(A)for A in A)
	for(H,(D,E))in B:
		if 0<=D<F and 0<=E<G:C[D][E]=H
	return tuple(tuple(A)for A in C)
def GM(A):return next(iter(A))[0]
def S(A,B,C,D):
	M=E(A)if D else None;N=set();I=set();Q,S=len(A),len(A[0]);T=R(A);U=V if C else G
	for F in T:
		if F in I:continue
		J=A[F[0]][F[1]]
		if J==M:continue
		O={(J,F)};K={F}
		while len(K)>0:
			P=set()
			for H in K:
				L=A[H[0]][H[1]]
				if J==L if B else L!=M:O.add((L,H));I.add(H);P|={(A,B)for(A,B)in U(H)if 0<=A<Q and 0<=B<S}
			K=P-I
		N.add(frozenset(O))
	return frozenset(N)
def Q(A,B):return frozenset((A,B)for B in Y(B))
def L(A,B):return GE(GX(A,B))
def GK(A,a,b):return lambda x:A(a(x),b(x))
def GY(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda y:A(B,y)
	elif C==3:return lambda y,z:A(B,y,z)
	else:return lambda y,z,a:A(B,y,z,a)
def Z(A,B):return type(B)(B for B in B if B!=A)
def W(A,B):return max(A,key=B)
def GQ(A):return len(A)
def p(I):C=False;I=tuple(map(tuple,I));A=S(I,C,C,True);B=W(A,GQ);D=Z(B,A);E=GY(U,B);F=GK(GV,GH,E);G=GK(Q,GM,F);H=L(G,D);J=GJ(I,H);return[*map(list,J)]