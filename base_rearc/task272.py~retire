def U(A):return max(A for(A,B)in J(A))
def E(A):return min(A for(A,B)in J(A))
def V(A):return max(A for(B,A)in J(A))
def K(A):return min(A for(B,A)in J(A))
def GJ(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return V(A)-K(A)+1
def GG(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return U(A)-E(A)+1
def Y(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def M(A):return G(A)|Y(A)
def G(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def X(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def R(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def GM(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def J(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def Z(A,B):return tuple(tuple(A for B in range(B[1]))for C in range(B[0]))
def P(a,b):return tuple(A+B for(A,B)in zip(a,b))
def GR(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in J(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def GH(A):return next(iter(A))[0]
def L(A,B,C,D):
	L=R(A)if D else None;N=set();H=set();Q,S=len(A),len(A[0]);T=X(A);U=M if C else G
	for E in T:
		if E in H:continue
		I=A[E[0]][E[1]]
		if I==L:continue
		O={(I,E)};J={E}
		while len(J)>0:
			P=set()
			for F in J:
				K=A[F[0]][F[1]]
				if I==K if B else K!=L:O.add((K,F));H.add(F);P|={(A,B)for(A,B)in U(F)if 0<=A<Q and 0<=B<S}
			J=P-H
		N.add(frozenset(O))
	return frozenset(N)
def H(A,n):return frozenset(A for A in A if len(A)==n)
def GV(A):return GG(A),GJ(A)
def S(A,B):return lambda x:A(x)==B
def W(A,B):return lambda x:A(B(x))
def Q(A,B):return type(A)(A for A in A if B(A))
def GY(A):return type(A)(B for A in A for B in A)
def GE(b):return not b
def p(I):A=False;I=tuple(map(tuple,I));B=GV(I);C=Z(0,B);D=P(I,C);E=L(D,A,A,True);F=S(GH,0);G=W(GE,F);J=Q(E,G);K=H(J,1);M=GY(K);N=GR(I,1,M);return[*map(list,N)]