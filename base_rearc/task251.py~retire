def GY(A):return type(A)(B for A in A for B in A)
def Y(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def R(A):return H(A)|Y(A)
def H(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def K(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def M(A):return max(A for(B,A)in V(A))
def P(A):return min(A for(B,A)in V(A))
def Q(A):return max(A for(A,B)in V(A))
def X(A):return min(A for(A,B)in V(A))
def GM(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def V(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def GR(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in V(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def GH(A):return next(iter(A))[0]
def E(A,B):return X(A)==0 or P(A)==0 or Q(A)==len(B)-1 or M(A)==len(B[0])-1
def Z(A,B,C,D):
	M=U(A)if D else None;N=set();G=set();Q,S=len(A),len(A[0]);T=K(A);V=R if C else H
	for E in T:
		if E in G:continue
		I=A[E[0]][E[1]]
		if I==M:continue
		O={(I,E)};J={E}
		while len(J)>0:
			P=set()
			for F in J:
				L=A[F[0]][F[1]]
				if I==L if B else L!=M:O.add((L,F));G.add(F);P|={(A,B)for(A,B)in V(F)if 0<=A<Q and 0<=B<S}
			J=P-G
		N.add(frozenset(O))
	return frozenset(N)
def G(A,B):return frozenset(A for A in A if next(iter(A))[0]==B)
def U(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def GV(A,B):return type(B)(A(B)for B in B)
def GJ(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def GG(A,B):return lambda x:A(B(x))
def W(A):return tuple(A)
def L(A,B):return GY(S(A,B))
def S(A,B):return type(A)(A for A in A if B(A))
def J(A):return max(set(A),key=A.count)
def GE(b):return not b
def p(I):B=False;I=tuple(map(tuple,I));A=Z(I,True,B,B);C=GJ(E,I);D=GG(GE,C);F=S(A,D);H=W(F);K=GV(GH,H);T=J(K);M=U(I);N=G(A,M);O=GJ(E,I);P=GG(GE,O);Q=L(N,P);R=GR(I,1,Q);return[*map(list,R)]