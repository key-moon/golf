def W(A,B):return type(A)(A for A in A if B(A))
def GJ(A):return type(A)(B for A in A for B in A)
def Y(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def E(A):return H(A)|Y(A)
def H(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def V(A):return max(A for(B,A)in J(A))
def S(A):return min(A for(B,A)in J(A))
def K(A):return max(A for(A,B)in J(A))
def R(A):return min(A for(A,B)in J(A))
def GM(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def J(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def L(A):return tuple(map(max,zip(*J(A))))
def Q(A):return tuple(map(min,zip(*J(A))))
def GX(A):
	if len(A)==0:return A
	F,G=Q(A);H,I=L(A);B,C=min(F,H),min(G,I);D,E=max(F,H),max(G,I);J={(A,C)for A in range(B,D+1)}|{(A,E)for A in range(B,D+1)};K={(B,A)for A in range(C,E+1)}|{(D,A)for A in range(C,E+1)};return frozenset(J|K)
def Z(A,B,C):return tuple(tuple(C if A==B else A for A in A)for A in A)
def GR(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in J(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def P(A,B):D,E=len(B),len(B[0]);return frozenset((B[A][C],(A,C))for(A,C)in J(A)if 0<=A<D and 0<=C<E)
def M(A,B):return R(A)==0 or S(A)==0 or K(A)==len(B)-1 or V(A)==len(B[0])-1
def GH(A,B,C,D):
	M=X(A)if D else None;N=set();I=set();Q,R=len(A),len(A[0]);S=U(A);T=E if C else H
	for F in S:
		if F in I:continue
		J=A[F[0]][F[1]]
		if J==M:continue
		O={(J,F)};K={F}
		while len(K)>0:
			P=set()
			for G in K:
				L=A[G[0]][G[1]]
				if J==L if B else L!=M:O.add((L,G));I.add(G);P|={(A,B)for(A,B)in T(G)if 0<=A<Q and 0<=B<R}
			K=P-I
		N.add(frozenset(O))
	return frozenset(N)
def U(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def G(A,B):return frozenset(A for A in A if next(iter(A))[0]==B)
def X(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def GV(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def GY(A,B):return lambda x:A(B(x))
def GG(A,B):return GJ(W(A,B))
def GE(b):return not b
def p(I):B=False;I=tuple(map(tuple,I));C=GH(I,True,B,B);D=U(I);E=GX(D);F=P(E,I);A=X(F);H=G(C,A);J=GV(M,I);K=GY(GE,J);L=GG(H,K);N=GR(I,2,L);O=Z(N,A,3);return[*map(list,O)]