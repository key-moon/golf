def V(A):return max(A for(A,B)in G(A))
def J(A):return min(A for(A,B)in G(A))
def H(A):return max(A for(B,A)in G(A))
def E(A):return min(A for(B,A)in G(A))
def X(A):
	if isinstance(A,tuple):return frozenset({B for A in A for B in A})
	return frozenset({A for(A,B)in A})
def GG(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def G(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def U(a,b):
	A,B=a;C,D=b;E=min(A,C);F=max(A,C)+1;G=min(B,D);H=max(B,D)+1
	if A==C:return frozenset((A,B)for B in range(G,H))
	elif B==D:return frozenset((A,B)for A in range(E,F))
	elif C-A==D-B:return frozenset((A,B)for(A,B)in zip(range(E,F),range(G,H)))
	elif C-A==B-D:return frozenset((A,B)for(A,B)in zip(range(E,F),range(H-1,G-1,-1)))
	return frozenset()
def K(A,B):return tuple(tuple(A for B in range(B[1]))for C in range(B[0]))
def GH(A,B,C):
	H,I=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in G(C):
		if 0<=E<H and 0<=F<I:D[E][F]=B
	return tuple(tuple(A)for A in D)
def M(A):return len(X(A))
def Z(A):return L(A),W(A)
def W(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return H(A)-E(A)+1
def L(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return V(A)-J(A)+1
def P(A,a,b):return a if A else b
def S(j):return 0,j
def Q(i):return i,0
def Y(x):return x-1 if isinstance(x,int)else(x[0]-1,x[1]-1)
def R(a,b):return a==b
def p(I):I=tuple(map(tuple,I));A=M(I);B=R(A,3);C=L(I);D=Y(C);E=Q(D);F=P(B,E,(0,0));G=R(A,2);H=Z(I);J=Y(H);N=W(I);O=Y(N);T=S(O);V=P(G,J,T);X=Z(I);a=K(0,X);b=U(F,V);c=GH(a,5,b);return[*map(list,c)]