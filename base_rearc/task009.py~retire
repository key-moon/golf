def HV(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def H(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def M(A):return G(A)|H(A)
def G(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def X(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def U(A):return max(A for(A,B)in J(A))
def E(A):return min(A for(A,B)in J(A))
def V(A):return max(A for(B,A)in J(A))
def L(A):return min(A for(B,A)in J(A))
def GW(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return V(A)-L(A)+1
def GK(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return U(A)-E(A)+1
def S(A):return tuple(map(min,zip(*J(A))))
def GM(A):
	if isinstance(A,tuple):return tuple(zip(*A))
	B,C=S(A)
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(E-C+B,D-B+C))for(A,(D,E))in A)
	return frozenset((D-C+B,A-B+C)for(A,D)in A)
def K(A):C,D=len(A),len(A[0]);B=tuple(A for(A,B)in enumerate(A)if len(set(B))==1);E=tuple(A for(A,B)in enumerate(GM(A))if len(set(B))==1);F=frozenset({frozenset({(A[B][C],(B,C))for C in range(D)})for B in B});G=frozenset({frozenset({(A[C][B],(C,B))for C in range(C)})for B in E});return F|G
def GV(a,b):
	A,B=a;C,D=b;E=min(A,C);F=max(A,C)+1;G=min(B,D);H=max(B,D)+1
	if A==C:return frozenset((A,B)for B in range(G,H))
	elif B==D:return frozenset((A,B)for A in range(E,F))
	elif C-A==D-B:return frozenset((A,B)for(A,B)in zip(range(E,F),range(G,H)))
	elif C-A==B-D:return frozenset((A,B)for(A,B)in zip(range(E,F),range(H-1,G-1,-1)))
	return frozenset()
def GP(A,B):
	F,G=len(A),len(A[0]);C=list(list(A)for A in A)
	for(H,(D,E))in B:
		if 0<=D<F and 0<=E<G:C[D][E]=H
	return tuple(tuple(A)for A in C)
def Q(A):return frozenset((D,(A,C))for(A,B)in enumerate(A)for(C,D)in enumerate(B))
def GS(A):return next(iter(A))[0]
def Z(A):
	if isinstance(A,tuple):return frozenset({B for A in A for B in A})
	return frozenset({A for(A,B)in A})
def GZ(A):return GW(A)==len(A)and GK(A)==1
def HJ(A):return GK(A)==len(A)and GW(A)==1
def GE(A,B,C,D):
	L=R(A)if D else None;N=set();H=set();Q,S=len(A),len(A[0]);T=X(A);U=M if C else G
	for E in T:
		if E in H:continue
		I=A[E[0]][E[1]]
		if I==L:continue
		O={(I,E)};J={E}
		while len(J)>0:
			P=set()
			for F in J:
				K=A[F[0]][F[1]]
				if I==K if B else K!=L:O.add((K,F));H.add(F);P|={(A,B)for(A,B)in U(F)if 0<=A<Q and 0<=B<S}
			J=P-H
		N.add(frozenset(O))
	return frozenset(N)
def GG(A,B):return frozenset((A,B)for B in J(B))
def J(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def R(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def GJ(A,a,b):return frozenset(A(C,B)for B in b for C in a)
def GX(A,B):return GL(HY(A,B))
def HY(A,B):return type(B)(A(B)for B in B)
def HM(A,a,b):return lambda x:A(a(x),b(x))
def GQ(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda y:A(B,y)
	elif C==3:return lambda y,z:A(B,y,z)
	else:return lambda y,z,a:A(B,y,z,a)
def HG(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def GY(A,B):return lambda x:A(x)==B
def HH(h,g,f):return lambda x:h(g(f(x)))
def GR(A,B):return lambda x:A(B(x))
def GH(A,B):return GL(W(A,B))
def W(A,B):return type(A)(A for A in A if B(A))
def GU(a,b):return a or b
def GL(A):return type(A)(B for A in A for B in A)
def Y(a,b):return type(a)(A for A in a if A not in b)
def HE(b):return not b
def P(x):return x
def p(I):C=False;I=tuple(map(tuple,I));D=Q(I);E=K(I);A=GL(E);F=Y(D,A);G=R(F);H=GE(I,True,C,C);L=GS(A);M=GY(GS,L);N=GY(GS,G);O=HM(GU,M,N);S=GR(HE,O);B=W(H,S);T=GL(B);U=Z(T);V=GQ(GH,B);X=GQ(GY,GS);a=GR(V,X);b=HY(a,U);c=HM(GU,HJ,GZ);d=GQ(GJ,GV);e=HM(d,P,P);f=GR(e,J);g=HG(W,c);h=HH(GL,g,f);i=HM(GG,GS,h);j=GX(i,b);k=GP(I,j);l=GP(k,A);return[*map(list,l)]