def X(A):return max(A for(A,B)in Y(A))
def J(A):return max(A for(B,A)in Y(A))
def GK(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return J(A)-P(A)+1
def GM(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return X(A)-M(A)+1
def H(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def V(A):return G(A)|H(A)
def G(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def R(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def E(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def S(A):return tuple(map(max,zip(*Y(A))))
def GW(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def Y(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def Q(A):return tuple(map(min,zip(*Y(A))))
def GH(A):
	if isinstance(A,tuple):return tuple(zip(*A))
	B,C=Q(A)
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(E-C+B,D-B+C))for(A,(D,E))in A)
	return frozenset((D-C+B,A-B+C)for(A,D)in A)
def U(A):C,D=len(A),len(A[0]);B=tuple(A for(A,B)in enumerate(A)if len(set(B))==1);E=tuple(A for(A,B)in enumerate(GH(A))if len(set(B))==1);F=frozenset({frozenset({(A[B][C],(B,C))for C in range(D)})for B in B});G=frozenset({frozenset({(A[C][B],(C,B))for C in range(C)})for B in E});return F|G
def GV(A,B):return tuple(tuple(A for B in range(B[1]))for C in range(B[0]))
def W(a,b):return tuple(A+B for(A,B)in zip(a,b))
def GG(A):
	if isinstance(A,tuple):return tuple(A[::-1]for A in A)
	B=Q(A)[1]+S(A)[1]
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(C,B-D))for(A,(C,D))in A)
	return frozenset((A,B-C)for(A,C)in A)
def GX(A):return next(iter(A))[0]
def P(A):return min(A for(B,A)in Y(A))
def M(A):return min(A for(A,B)in Y(A))
def GY(A,B,C,D):
	M=E(A)if D else None;N=set();I=set();Q,S=len(A),len(A[0]);T=R(A);U=V if C else G
	for F in T:
		if F in I:continue
		J=A[F[0]][F[1]]
		if J==M:continue
		O={(J,F)};K={F}
		while len(K)>0:
			P=set()
			for H in K:
				L=A[H[0]][H[1]]
				if J==L if B else L!=M:O.add((L,H));I.add(H);P|={(A,B)for(A,B)in U(H)if 0<=A<Q and 0<=B<S}
			K=P-I
		N.add(frozenset(O))
	return frozenset(N)
def GP(A):return GM(A),GK(A)
def GL(A,B):return type(B)(A(B)for B in B)
def GE(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda y:A(B,y)
	elif C==3:return lambda y,z:A(B,y,z)
	else:return lambda y,z,a:A(B,y,z,a)
def GQ(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def Z(A,B):return lambda x:A(x)==B
def GS(h,g,f):return lambda x:h(g(f(x)))
def GJ(A,B):return lambda x:A(B(x))
def L(A,B):return type(A)(A for A in A if B(A))
def GU(A):return type(A)(B for A in A for B in A)
def GR(A,B):return tuple(sorted(A,key=B))
def K(x):return x
def p(I):I=tuple(map(tuple,I));B=U(I);C=GU(B);D=GX(C);E=GP(I);F=GV(D,E);G=W(I,F);A=GY(G,True,False,True);H=GL(M,A);J=GR(H,K);N=GE(L,A);O=GE(Z,M);Q=GJ(N,O);R=GE(GL,GX);S=GQ(GR,P);T=GS(R,S,Q);V=GL(T,J);X=GG(V);return[*map(list,X)]