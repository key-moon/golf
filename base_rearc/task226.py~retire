def Y(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def V(A):return H(A)|Y(A)
def H(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def GU(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def J(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def S(A):return tuple(map(max,zip(*J(A))))
def L(A):return tuple(map(lambda ix:{0:max,1:min}[ix[0]](ix[1]),enumerate(zip(*J(A)))))
def X(A):return tuple(map(lambda ix:{0:min,1:max}[ix[0]](ix[1]),enumerate(zip(*J(A)))))
def U(A):return tuple(map(min,zip(*J(A))))
def Z(A):return frozenset({U(A),X(A),L(A),S(A)})
def GP(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in J(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def K(A,B):D,E=len(B),len(B[0]);return frozenset((B[A][C],(A,C))for(A,C)in J(A)if 0<=A<D and 0<=C<E)
def P(A):return min(A for(B,A)in J(A))
def M(A):return min(A for(A,B)in J(A))
def GH(A,B,C,D):
	M=E(A)if D else None;N=set();I=set();Q,S=len(A),len(A[0]);T=R(A);U=V if C else H
	for F in T:
		if F in I:continue
		J=A[F[0]][F[1]]
		if J==M:continue
		O={(J,F)};K={F}
		while len(K)>0:
			P=set()
			for G in K:
				L=A[G[0]][G[1]]
				if J==L if B else L!=M:O.add((L,G));I.add(G);P|={(A,B)for(A,B)in U(G)if 0<=A<Q and 0<=B<S}
			K=P-I
		N.add(frozenset(O))
	return frozenset(N)
def R(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def G(A,B):return frozenset(A for A in A if next(iter(A))[0]==B)
def E(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def GK(A,a,b):return lambda x:A(a(x),b(x))
def GE(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda y:A(B,y)
	elif C==3:return lambda y,z:A(B,y,z)
	else:return lambda y,z,a:A(B,y,z,a)
def GR(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def GX(h,g,f):return lambda x:h(g(f(x)))
def GJ(A,B):return lambda x:A(B(x))
def GY(A,B):return next(A for A in A if B(A))
def W(A,B):return type(A)(A for A in A if B(A))
def GQ(a,b):return a and b
def GM(A,B):return min(A,key=B)
def GV(A,B):return max(A,key=B)
def GS(A):return len(A)
def GG(a,b):return a>b
def Q(a,b):return a==b
def GL(a,b):
	if isinstance(a,int)and isinstance(b,int):return a+b
	elif isinstance(a,tuple)and isinstance(b,tuple):return a[0]+b[0],a[1]+b[1]
	elif isinstance(a,int)and isinstance(b,tuple):return a+b[0],a+b[1]
	return a[0]+b,a[1]+b
def p(I):I=tuple(map(tuple,I));F=R(I);H=Z(F);J=K(H,I);L=E(J);N=GH(I,True,True,False);A=G(N,L);B=GK(GL,P,M);O=GM(A,B);S=GV(A,B);T=GE(W,A);U=GR(GJ,P);C=GX(GS,T,U);V=GE(W,A);X=GR(GJ,M);D=GX(GS,V,X);Y=GE(GE,GG);a=GX(C,Y,P);b=GE(GR,GG);c=GX(C,b,P);d=GE(GE,GG);e=GX(D,d,M);f=GE(GR,GG);g=GX(D,f,M);h=GK(Q,a,c);i=GK(Q,e,g);j=GK(GQ,h,i);k=GY(A,j);l=GP(I,1,O);m=GP(l,3,S);n=GP(m,2,k);return[*map(list,n)]