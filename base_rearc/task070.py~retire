def J(A):return max(A for(A,B)in H(A))
def V(A):return min(A for(A,B)in H(A))
def Y(A):return max(A for(B,A)in H(A))
def U(A):return min(A for(B,A)in H(A))
def K(A):
	if isinstance(A,tuple):return frozenset({B for A in A for B in A})
	return frozenset({A for(A,B)in A})
def L(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def R(A):return tuple(map(max,zip(*H(A))))
def M(A):return tuple(map(min,zip(*H(A))))
def X(A):
	if len(A)==0:return frozenset({})
	B=H(A);C,D=M(B);E,F=R(A);return frozenset((A,B)for A in range(C,E+1)for B in range(D,F+1))
def H(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def S(B):
	if len(B)==0:return frozenset({})
	return X(B)-H(B)
def Z(A,B,C):
	G,I=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in H(C):
		if 0<=E<G and 0<=F<I:D[E][F]=B
	return tuple(tuple(A)for A in D)
def G(A):return frozenset(frozenset((A,(C,E))for(C,D)in enumerate(A)for(E,A)in enumerate(D)if A==B)for B in K(A))
def W(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return Y(A)-U(A)+1
def Q(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return J(A)-V(A)+1
def GG(A,a,b):return lambda x:A(a(x),b(x))
def P(A,B):return min(A,key=B)
def E(a,b):
	if isinstance(a,int)and isinstance(b,int):return a*b
	elif isinstance(a,tuple)and isinstance(b,tuple):return a[0]*b[0],a[1]*b[1]
	elif isinstance(a,int)and isinstance(b,tuple):return a*b[0],a*b[1]
	return a[0]*b,a[1]*b
def p(I):I=tuple(map(tuple,I));A=G(I);B=GG(E,Q,W);C=P(A,B);D=S(C);F=Z(I,3,D);return[*map(list,F)]