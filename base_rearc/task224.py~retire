def K(A):return next(iter(A))
def S(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def H(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def Y(A):return max(A for(B,A)in H(A))
def E(A):return min(A for(B,A)in H(A))
def J(A):return max(A for(A,B)in H(A))
def M(A):return min(A for(A,B)in H(A))
def Q(A):G,H=M(A)+1,E(A)+1;I,K=J(A)-1,Y(A)-1;B,C=min(G,I),min(H,K);D,F=max(G,I),max(H,K);L={(A,C)for A in range(B,D+1)}|{(A,F)for A in range(B,D+1)};N={(B,A)for A in range(C,F+1)}|{(D,A)for A in range(C,F+1)};return frozenset(L|N)
def L(A,B,C):
	G,I=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in H(C):
		if 0<=E<G and 0<=F<I:D[E][F]=B
	return tuple(tuple(A)for A in D)
def X(A):
	if isinstance(A,tuple):return frozenset({B for A in A for B in A})
	return frozenset({A for(A,B)in A})
def R(A,B):return frozenset((A,D)for(A,C)in enumerate(A)for(D,E)in enumerate(C)if E==B)
def G(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return min(set(B),key=B.count)
def V(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def P(A,B):return K(U(B,A))
def U(A,B):return type(B)(B for B in B if B!=A)
def p(I):I=tuple(map(tuple,I));A=G(I);B=R(I,A);C=G(I);D=X(I);E=U(C,D);F=V(I);H=P(E,F);J=Q(B);K=L(I,H,J);return[*map(list,K)]