def GJ(A):return type(A)(B for A in A for B in A)
def Y(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def M(A):return G(A)|Y(A)
def G(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def U(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def X(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def K(A):return max(A for(A,B)in J(A))
def V(A):return max(A for(B,A)in J(A))
def GU(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def J(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def Q(A):return min(A for(B,A)in J(A))
def E(A):return min(A for(A,B)in J(A))
def GV(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return V(A)-Q(A)+1
def Z(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return K(A)-E(A)+1
def GG(A):return E(A)+Z(A)//2,Q(A)+GV(A)//2
def GY(A,B):
	F,G=len(A),len(A[0]);C=list(list(A)for A in A)
	for(H,(D,E))in B:
		if 0<=D<F and 0<=E<G:C[D][E]=H
	return tuple(tuple(A)for A in C)
def P(A,B,C,D):
	L=X(A)if D else None;N=set();H=set();Q,R=len(A),len(A[0]);S=U(A);T=M if C else G
	for E in S:
		if E in H:continue
		I=A[E[0]][E[1]]
		if I==L:continue
		O={(I,E)};J={E}
		while len(J)>0:
			P=set()
			for F in J:
				K=A[F[0]][F[1]]
				if I==K if B else K!=L:O.add((K,F));H.add(F);P|={(A,B)for(A,B)in T(F)if 0<=A<Q and 0<=B<R}
			J=P-H
		N.add(frozenset(O))
	return frozenset(N)
def R(A):
	if len(A)==0:return A
	return GE(A,(-E(A),-Q(A)))
def GE(A,C):
	if len(A)==0:return A
	B,D=C
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(C+B,E+D))for(A,(C,E))in A)
	return frozenset((A+B,C+D)for(A,C)in A)
def H(A,n):return frozenset(A for A in A if len(A)==n)
def GR(A):return Z(A),GV(A)
def S(A,B):return GJ(GX(A,B))
def GX(A,B):return type(B)(A(B)for B in B)
def GH(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda y:A(B,y)
	elif C==3:return lambda y,z:A(B,y,z)
	else:return lambda y,z,a:A(B,y,z,a)
def W(A,B):return max(A,key=B)
def GK(A):return len(A)
def GM(n):return n//2 if isinstance(n,int)else(n[0]//2,n[1]//2)
def L(n):return-n if isinstance(n,int)else(-n[0],-n[1])
def p(I):C=False;I=tuple(map(tuple,I));A=P(I,C,C,True);B=W(A,GK);D=R(B);E=GR(B);F=GM(E);G=L(F);J=GE(D,G);K=H(A,1);M=GX(GG,K);N=GH(GE,J);O=S(N,M);Q=GY(I,O);return[*map(list,Q)]