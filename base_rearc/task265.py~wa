def GH(A,B):return type(B)(A(B)for B in B)
def L(A):return type(A)(B for A in A for B in A)
def E(A):return tuple(map(min,zip(*H(A))))
def M(A):return max(A for(A,B)in H(A))
def Y(A):return max(A for(B,A)in H(A))
def W(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return Y(A)-R(A)+1
def P(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return M(A)-J(A)+1
def GY(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def H(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def R(A):return min(A for(B,A)in H(A))
def J(A):return min(A for(A,B)in H(A))
def V(A):
	if len(A)==0:return A
	return Z(A,(-J(A),-R(A)))
def GG(A):return P(A),W(A)
def G(A,B):
	C=set();K=V(B);D,E=len(A),len(A[0]);L,M=GG(B);N,O=D-L+1,E-M+1
	for F in range(N):
		for G in range(O):
			H=True
			for(P,(I,J))in Z(K,(F,G)):
				if not(0<=I<D and 0<=J<E and A[I][J]==P):H=False;break
			if H:C.add((F,G))
	return frozenset(C)
def X(A,B):
	if isinstance(A,tuple):
		C=tuple()
		for J in A:
			D=tuple()
			for F in J:D=D+tuple(F for A in range(B))
			C=C+tuple(D for A in range(B))
		return C
	else:
		if len(A)==0:return frozenset()
		G,H=E(A);K,L=-G,-H;M=Z(A,(K,L));I=set()
		for(F,(N,O))in M:
			for P in range(B):
				for Q in range(B):I.add((F,(N*B+P,O*B+Q)))
		return Z(frozenset(I),(G,H))
def GJ(A,B,C):
	G,I=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in H(C):
		if 0<=E<G and 0<=F<I:D[E][F]=B
	return tuple(tuple(A)for A in D)
def Z(A,C):
	if len(A)==0:return A
	B,D=C
	if isinstance(next(iter(A))[1],tuple):return frozenset((A,(C+B,E+D))for(A,(C,E))in A)
	return frozenset((A+B,C+D)for(A,C)in A)
def U(A,B):return frozenset((A,B)for B in H(B))
def Q(A,B):return L(GH(A,B))
def S(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda y:A(B,y)
	elif C==3:return lambda y,z:A(B,y,z)
	else:return lambda y,z,a:A(B,y,z,a)
def K(A):return frozenset({A})
def p(I):I=tuple(map(tuple,I));B=K((0,0));C=U(0,B);A=X(C,2);D=G(I,A);E=S(Z,A);F=Q(E,D);H=GJ(I,2,F);return[*map(list,H)]