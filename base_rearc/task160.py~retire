def GL(A,B):return type(B)(A(B)for B in B)
def GW(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def Y(A):return frozenset({(A[0]-1,A[1]-1),(A[0]-1,A[1]+1),(A[0]+1,A[1]-1),(A[0]+1,A[1]+1)})
def M(A):return H(A)|Y(A)
def H(A):return frozenset({(A[0]-1,A[1]),(A[0]+1,A[1]),(A[0],A[1]-1),(A[0],A[1]+1)})
def K(A):return frozenset((B,C)for B in range(len(A))for C in range(len(A[0])))
def Q(A):return max(A for(A,B)in J(A))
def V(A):return max(A for(B,A)in J(A))
def W(A):return min(A for(B,A)in J(A))
def E(A):return min(A for(A,B)in J(A))
def GK(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return V(A)-W(A)+1
def GR(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return Q(A)-E(A)+1
def Z(A):return tuple(map(max,zip(*J(A))))
def S(A):return tuple(map(min,zip(*J(A))))
def HH(A):
	if len(A)==0:return A
	F,G=S(A);H,I=Z(A);B,C=min(F,H),min(G,I);D,E=max(F,H),max(G,I);J={(A,C)for A in range(B,D+1)}|{(A,E)for A in range(B,D+1)};K={(B,A)for A in range(C,E+1)}|{(D,A)for A in range(C,E+1)};return frozenset(J|K)
def P(A):
	if len(A)==0:return frozenset({})
	B=J(A);C,D=S(B);E,F=Z(A);return frozenset((A,B)for A in range(C,E+1)for B in range(D,F+1))
def R(A):return frozenset((A[0],B)for B in range(30))
def U(A):return frozenset((B,A[1])for B in range(30))
def GX(A):return E(A)+GR(A)//2,W(A)+GK(A)//2
def HG(A,B,C):
	G,H=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in J(C):
		if 0<=E<G and 0<=F<H:D[E][F]=B
	return tuple(tuple(A)for A in D)
def GV(A,B,C,D):
	N=X(A)if D else None;O=set();G=set();R,S=len(A),len(A[0]);T=K(A);U=M if C else H
	for E in T:
		if E in G:continue
		I=A[E[0]][E[1]]
		if I==N:continue
		P={(I,E)};J={E}
		while len(J)>0:
			Q=set()
			for F in J:
				L=A[F[0]][F[1]]
				if I==L if B else L!=N:P.add((L,F));G.add(F);Q|={(A,B)for(A,B)in U(F)if 0<=A<R and 0<=B<S}
			J=Q-G
		O.add(frozenset(P))
	return frozenset(O)
def J(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def GP(A):return GR(A),GK(A)
def X(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return max(set(B),key=B.count)
def GE(A,B):return GU(GL(A,B))
def GZ(A,a,b):return lambda x:A(a(x),b(x))
def GQ(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def GS(h,g,f):return lambda x:h(g(f(x)))
def GM(A,B):return lambda x:A(B(x))
def GG(A,B):return type(A)(A for A in A if B(A))
def GH(Aa):return min(Aa,default=0)
def GU(A):return type(A)(B for A in A for B in A)
def GJ(a,b):return a>b
def G(a,b):return a&b
def GY(a,b):return type(a)((*a,*b))
def L(a,b):return a==b
def p(I):I=tuple(map(tuple,I));B=GV(I,True,False,True);C=GQ(GJ,2);D=GS(C,GH,GP);E=GG(B,D);F=GZ(L,J,HH);A=GG(E,F);H=X(I);K=GU(A);M=HG(I,H,K);N=GM(R,GX);O=GM(U,GX);Q=GZ(GY,N,O);S=GZ(G,Q,P);T=GE(S,A);V=HG(M,2,T);return[*map(list,V)]