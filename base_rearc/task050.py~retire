def W(A):return type(A)(B for A in A for B in A)
def V(A):return max(A for(A,B)in H(A))
def J(A):return min(A for(A,B)in H(A))
def Y(A):return max(A for(B,A)in H(A))
def M(A):return min(A for(B,A)in H(A))
def Z(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A[0])
	return Y(A)-M(A)+1
def L(A):
	if len(A)==0:return 0
	if isinstance(A,tuple):return len(A)
	return V(A)-J(A)+1
def GJ(A,B):
	C,D=B;E,F=len(A),len(A[0])
	if not(0<=C<E and 0<=D<F):return
	return A[B[0]][B[1]]
def H(A):
	if len(A)==0:return frozenset()
	if isinstance(next(iter(A))[1],tuple):return frozenset(A for(B,A)in A)
	return A
def Q(a,b):
	A,B=a;C,D=b;E=min(A,C);F=max(A,C)+1;G=min(B,D);H=max(B,D)+1
	if A==C:return frozenset((A,B)for B in range(G,H))
	elif B==D:return frozenset((A,B)for A in range(E,F))
	elif C-A==D-B:return frozenset((A,B)for(A,B)in zip(range(E,F),range(G,H)))
	elif C-A==B-D:return frozenset((A,B)for(A,B)in zip(range(E,F),range(H-1,G-1,-1)))
	return frozenset()
def GM(A,B,C):
	G,I=len(A),len(A[0]);D=list(list(A)for A in A)
	for(E,F)in H(C):
		if 0<=E<G and 0<=F<I:D[E][F]=B
	return tuple(tuple(A)for A in D)
def GG(A):return Z(A)==len(A)and L(A)==1
def GY(A):return L(A)==len(A)and Z(A)==1
def U(A,B):return frozenset((A,D)for(A,C)in enumerate(A)for(D,E)in enumerate(C)if E==B)
def G(A):B=[B for A in A for B in A]if isinstance(A,tuple)else[A for(A,B)in A];return min(set(B),key=B.count)
def K(A,a,b):return frozenset(A(C,B)for B in b for C in a)
def GV(A,a,b):return lambda x:A(a(x),b(x))
def GH(A,B):
	C=A.__code__.co_argcount
	if C==2:return lambda x:A(x,B)
	elif C==3:return lambda x,y:A(x,y,B)
	else:return lambda x,y,z:A(x,y,z,B)
def P(A,B):return lambda x:A(B(x))
def X(A,B):return W(E(A,B))
def E(A,B):return type(A)(A for A in A if B(A))
def S(a,b):return a or b
def GE(A):return len(A)
def R(a,b):return a>b
def p(I):I=tuple(map(tuple,I));B=G(I);A=U(I,B);C=K(Q,A,A);D=GH(R,1);F=P(D,GE);H=E(C,F);J=GV(S,GY,GG);L=X(H,J);M=GM(I,3,L);N=G(I);O=GM(M,N,A);return[*map(list,O)]